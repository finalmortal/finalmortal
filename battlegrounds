//setdepth
function setdepth(){


 if (player1p1name == 'KANG') {      
    p1kang.setDepth(5);
    }
    if (player1p2name == 'KANG') {      
    p1kang.setDepth(4);
    }
     if (player1p3name == 'KANG') {      
    p1kang.setDepth(3);
    }
    if (player1p4name == 'KANG') {      
    p1kang.setDepth(2);
    }
    if (player1p5name == 'KANG') {      
    p1kang.setDepth(1);
    }
    if (player2p1name == 'KANG') {      
    p2kang.setDepth(5);
    }
