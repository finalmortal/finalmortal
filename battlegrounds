     p2combohealthrating = currenttargethealthrating
    }
    if (currentattacktargetname == 'p2sonja'){
     p2sonjahealthrating = currenttargethealthrating
    }
    if (currentattacktargetname == 'p2kunglao'){
     p2kunglaohealthrating = currenttargethealthrating
    }
    if (currentattacktargetname == 'p2glacius'){
     p2glaciushealthrating = currenttargethealthrating
    }
    if (currentattacktargetname == 'p2golbez'){
    p2golbezhealthrating = currenttargethealthrating
    }
    if (currentattacktargetname == 'p2sektor'){
     p2sektorhealthrating = currenttargethealthrating
    }
    if (currentattacktargetname == 'p2cyrax'){
     p2cyraxhealthrating = currenttargethealthrating
