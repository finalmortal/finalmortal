p2cyraxchargerating += p2mogbullstat
p2golbezchargerating += p2mogbullstat
p2jagochargerating += p2mogbullstat
p2cyanchargerating += p2mogbullstat
p2mogbullstatup = true
}
if (p2mogbullstatdown == true){
p2rosachargerating -= p2mogbullstat
p2mogchargerating -= p2mogbullstat
p2sindelchargerating -= p2mogbullstat
p2sonjachargerating -= p2mogbullstat
p2raidenchargerating -= p2mogbullstat    
p2sektorchargerating -= p2mogbullstat
p2glaciuschargerating -= p2mogbullstat
p2kunglaochargerating -= p2mogbullstat
p2combochargerating -= p2mogbullstat
p2kanochargerating -= p2mogbullstat
p2terrachargerating -= p2mogbullstat
p2spinalchargerating -= p2mogbullstat
p2kangchargerating -= p2mogbullstat
p2kainchargerating -= p2mogbullstat
p2cecilchargerating -= p2mogbullstat
p2orchidchargerating -= p2mogbullstat
p2cyraxchargerating -= p2mogbullstat
p2golbezchargerating -= p2mogbullstat
p2jagochargerating -= p2mogbullstat
p2cyanchargerating -= p2mogbullstat
p2mogbullstatdown = false
p2mogbullaura = false
}
}
//
//
}
//endmogspells****************************
//nextround*******************************
