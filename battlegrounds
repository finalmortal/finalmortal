//raidenaction
//p1raidenaction
function p1raidenaction(){
    if (p1raidenidle == true){
p1raiden.anims.play('raidenidle', true);
}
if (p1raidenattackthunderbolt == true){
if (p1raidentargetselected == true){
p1raidenidle = false
if (p1raiden.x < p1raidentargetx) {
p1raidenthunderboltgr.visible = true
p1raiden.anims.play('raidenthunderbolt', true);
p1raidenthunderboltgr.anims.play('raidenthunderboltgr', true);
if (p1raidenthunderboltsoundplayed == false){
        game.sound.add( "raidenthunderboltsound" ).play()
        p1raidenthunderboltsoundplayed = true
    }
p1raidenthunderboltgr.x += 35
if (p1raidenthunderboltgr.y > p1raidentargety){
    p1raidenthunderboltgr.y -= 7;
}
if (p1raidenthunderboltgr.y < p1raidentargety){
    p1raidenthunderboltgr.y += 7;
}
}

if (p1raidenthunderboltgr.x > p1raidentargetx -1 ) {
playerattacking = true
playerbattleattack()
p1raidenidle = true
playerabilityselected = false
p1raidenselected = false
p1raidenthunderboltgr.visible = false
p1raidenthunderboltgr.x = p1raiden.x
p1raidenthunderboltgr.y = p1raiden.y 
player1selectabilitybox.visible = false
p1raiden.x = p1raidenx
p1raiden.y = p1raideny
team2turn = true
team1turn = false
p1raidentargetselected = false
p1raidenattackthunderbolt = false
}
}
}

if (p1raidenattackfly == true){
if (p1raidentargetselected == true){
p1raidenidle = false
p1raidenflytime -= 0.6;
if (p1raiden.x < p1raidentargetx) {
p1raiden.anims.play('raidenfly', true);
if (p1raidenflyvoiceplayed == false){
        game.sound.add( "raidenflyvoice" ).play()
        p1raidenflyvoiceplayed = true
    }
p1raiden.x += 45 
if (p1raiden.y > p1raidentargety){
    p1raiden.y -= 7;
}
if (p1raiden.y < p1raidentargety){
    p1raiden.y += 7;
}
}
if (p1raiden.x > p1raidentargetx ) {
p1raiden.anims.play('raidenfly', true);
p1raiden.x = p1raidentargetx
p1raiden.setDepth(7);
}
if (p1raidenflytime < 1){
playerattacking = true
playerbattleattack()
p1raidenidle = true
playerabilityselected = false
p1raidenselected = false
player1selectabilitybox.visible = false
p1raiden.x = p1raidenx
p1raiden.y = p1raideny
team2turn = true
team1turn = false
setdepth()
p1raidentargetselected = false
p1raidenattackfly = false
}
}
}


if (p1raidenattackstorm == true){
if (p1raidentargetselected == true){
p1raidenidle = false
p1raidenstormtime -= 0.1;
if (p1raiden.x <  600) {
p1raiden.anims.play('raidenwalk', true);
if (p1raidenstormvoiceplayed == false){
        game.sound.add( "raidenstormvoice" ).play()
        p1raidenstormvoiceplayed = true
    }
p1raiden.x += 25 
}
if (p1raiden.x >  601) {
p1raidenstormcloudboltsgr.setDepth(6);
p1raidenstormcloudgr.setDepth(6);
p1raidenstormstrikegr.setDepth(6);
p1raidenstormcloudgr.x = p1raidentargetx - 175
p1raidenstormcloudgr.y = p1raidentargety - 300
p1raidenstormcloudboltsgr.x = p1raidenstormcloudgr.x + 50
p1raidenstormcloudboltsgr.y = p1raidenstormcloudgr.y + 150
p1raidenstormstrikegr.x = p1raidentargetx - 90
p1raidenstormstrikegr.y = p1raidentargety - 100
p1raidenstormcloudgr.visible = true
p1raidenstormstrikegr.visible = true
p1raidenstormcloudboltsgr.visible = true
p1raidenstormcloudgr.anims.play('raidenstormcloudgr', true);
p1raidenstormstrikegr.anims.play('raidenstormstrikegr', true);
p1raidenstormcloudboltsgr.play('raidenstormcloudboltsgr', true);
p1raiden.anims.play('raidenstorm', true);
}

if (p1raidenstormtime < 1 ){
playerattacking = true
playerbattleattack()
p1raidenidle = true
playerabilityselected = false
p1raidenselected = false
player1selectabilitybox.visible = false
p1raidenstormcloudgr.visible = false
p1raidenstormstrikegr.visible = false
p1raidenstormcloudboltsgr.visible = false
p1raiden.x = p1raidenx
p1raiden.y = p1raideny
team2turn = true
team1turn = false
p1raidentargetselected = false
p1raidenattackstorm = false
}
}
}

if (p1raidenattackpowerstrike == true){
p1raidenidle = false
p1raidenpowerstriketime -= 0.1;
if (p1raiden.x <  600) {
p1raiden.anims.play('raidenwalk', true);
if (p1raidenpowerstrikevoiceplayed == false){
        game.sound.add( "raidenpowerstrikevoice" ).play()
        p1raidenpowerstrikevoiceplayed = true
    }
p1raiden.x += 25 
}
if (p1raiden.x >  601) {
p1raidenpowerstrikegr.x = p1raiden.x - 30
p1raidenpowerstrikegr.y = p1raiden.y - 50
p1raidenpowerstrikegr.visible = true
p1raidenpowerstrikegr.anims.play('raidenpowerstrikegr', true);
p1raiden.anims.play('raidenpowerstrike', true);

}
if (p1raidenpowerstriketime < 1 ){
playerattacking = true
playerbattleattack()
p1raidenidle = true
playerabilityselected = false
p1raidenselected = false
player1selectabilitybox.visible = false
p1raidenpowerstrikegr.visible = false
p1raiden.x = p1raidenx
p1raiden.y = p1raideny
team2turn = true
team1turn = false
p1raidentargetselected = false
p1raidenattackpowerstrike = false
}
}

if (p1raidenattackkick == true){
if (p1raidentargetselected == true){
p1raidenidle = false
p1raidenkicktime -= 0.1;
if (p1raiden.x < p1raidentargetx - 80) {
    if (p1raidenkickvoiceplayed == false){
        game.sound.add( "raidenkickvoice" ).play()
        p1raidenkickvoiceplayed = true
    }
p1raiden.anims.play('raidenwalk', true);
p1raiden.x += 25 
if (p1raiden.y > p1raidentargety){
    p1raiden.y -= 7;
}
if (p1raiden.y < p1raidentargety){
    p1raiden.y += 7;
}
}
if (p1raiden.x > p1raidentargetx - 81) {
p1raiden.anims.play('raidenkick', true);
}
if (p1raidenkicktime < 1){
playerattacking = true
playerbattleattack()
p1raidenidle = true
playerabilityselected = false
p1raidenselected = false
player1selectabilitybox.visible = false
p1raiden.x = p1raidenx
p1raiden.y = p1raideny
team2turn = true
team1turn = false
p1raidentargetselected = false
p1raidenattackkick = false
}
}
}
}


