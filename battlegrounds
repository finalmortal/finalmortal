//raidenbuttonclick
function p1raidenbuttonclick(){
    if (team1turn == true){
    if (p1rosahealingtarget == true){
    raidenistarget = true
    setsupporttarget()
    playertargetselected()
    setenemyposition()
    removetint()
    }

    else {
    deselect()
    team1noplayerselected = false
    p1raidenselected = true
    playerbuttons()
    }
    }
    
    if (team2turn == true){
    raidenistarget = true
    settarget()
    playertargetselected()
    setenemyposition()
    removetint()
    }
}

function p2raidenbuttonclick(){
 if (team2turn == true){
    if (p2rosahealingtarget == true){
    raidenistarget = true
    setsupporttarget()
    playertargetselected()
    setenemyposition()
    removetint()
    }

    else {
    deselect()
    team2noplayerselected = false
    p2raidenselected = true
    playerbuttons()
    }
    }
    
    if (team1turn == true){
    raidenistarget = true
    settarget()
    playertargetselected()
    setenemyposition()
    removetint()
    }
}

