//rosaspells
//p1rosaspells
function p1rosaspells(){
//p1rosashell
if (p1rosashellaura == true){
if (p1rosashellauratime < 1){
p1rosashellstatdown = true
}
}
if (p1rosashellaura == true){
if (p1rosashellstatup == false){
p1rosadefenserating += p1rosashellstat
p1mogdefenserating += p1rosashellstat
p1sindeldefenserating += p1rosashellstat
p1sonjadefenserating += p1rosashellstat
p1raidendefenserating += p1rosashellstat    
p1sektordefenserating += p1rosashellstat
p1glaciusdefenserating += p1rosashellstat
p1kunglaodefenserating += p1rosashellstat
p1combodefenserating += p1rosashellstat
p1kanodefenserating += p1rosashellstat
p1terradefenserating += p1rosashellstat
p1spinaldefenserating += p1rosashellstat
p1kangdefenserating += p1rosashellstat
p1kaindefenserating += p1rosashellstat
p1cecildefenserating += p1rosashellstat
p1orchiddefenserating += p1rosashellstat
p1cyraxdefenserating += p1rosashellstat
p1golbezdefenserating += p1rosashellstat
p1jagodefenserating += p1rosashellstat
p1cyandefenserating += p1rosashellstat
p1rosashellstatup = true
}
if (p1rosashellstatdown == true){
p1rosadefenserating -= p1rosashellstat
p1mogdefenserating -= p1rosashellstat
p1sindeldefenserating -= p1rosashellstat
p1sonjadefenserating -= p1rosashellstat
p1raidendefenserating -= p1rosashellstat    
p1sektordefenserating -= p1rosashellstat
p1glaciusdefenserating -= p1rosashellstat
p1kunglaodefenserating -= p1rosashellstat
p1combodefenserating -= p1rosashellstat
p1kanodefenserating -= p1rosashellstat
p1terradefenserating -= p1rosashellstat
p1spinaldefenserating -= p1rosashellstat
p1kangdefenserating -= p1rosashellstat
p1kaindefenserating -= p1rosashellstat
p1cecildefenserating -= p1rosashellstat
p1orchiddefenserating -= p1rosashellstat
p1cyraxdefenserating -= p1rosashellstat
p1golbezdefenserating -= p1rosashellstat
p1jagodefenserating -= p1rosashellstat
p1cyandefenserating -= p1rosashellstat
p1rosashellstatdown = false
p1rosashellaura = false
}
}

//p1rosafast
if (p1rosafastaura == true){
