    p2raiden.y += 7;
}
}
if (p2raiden.x < p2raidentargetx + 81) {
p2raiden.anims.play('raidenkick', true);
}
if (p2raidenkicktime < 1){
playerattacking = true
playerbattleattack()
p2raidenidle = true
playerabilityselected = false
p2raidenselected = false
player2selectabilitybox.visible = false
p2raiden.x = p2raidenx
p2raiden.y = p2raideny
team1turn = true
team2turn = false
p2raidentargetselected = false
p2raidenattackkick = false
}
}
}
}
//cyanaction
//p1cyanaction
function p1cyanaction(){
    if (p1cyanidle == true){
p1cyan.anims.play('cyanidle', true);
}

//cyanswordslash
if (p1cyanattackswordslash == true){
if (p1cyantargetselected == true){
p1cyanswordslashtime -= 0.1;
if (p1cyan.x < p1cyantargetx - 75){
    p1cyan.anims.play('cyanwalk', true);
    p1cyan.x += 25
    if (p1cyan.y > p1cyantargety + 25){
        p1cyan.y -= 5
    }
    if (p1cyan.y < p1cyantargety + 25){
        p1cyan.y += 5
    }
}
if (p1cyan.x > p1cyantargetx - 76){
    p1cyanswordslash.x = p1cyan.x - 25
    p1cyanswordslash.y = p1cyan.y - 75
    p1cyanswordslasheffect.x = p1cyantargetx - 30
    p1cyanswordslasheffect.y = p1cyantargety 
    p1cyan.anims.play('cyanattacksword', true);
    p1cyanswordslash.anims.play('cyanswordslash', true);
    p1cyanswordslash.visible = true
    p1cyanswordslasheffect.anims.play('cyanswordslasheffect', true);
    p1cyanswordslasheffect.visible = true
    }

if (p1cyanswordslashtime < 1){
    p1cyanswordslash.visible = false
    p1cyanswordslasheffect.visible = false
    playerattacking = true
    playerbattleattack()
    p1cyan.x = p1cyanx
    p1cyan.y = p1cyany
    p1cyan.anims.play('cyanidle', true);
    playerabilityselected = false
    p1cyanselected = false
    player1selectabilitybox.visible = false
    team2turn = true
    team1turn = false
    p1cyantargetselected = false    
    p1cyanattackswordslash = false
    }
}
}


if (p1cyanattackswordswing == true){
if (p1cyantargetselected == true){
p1cyanswordswingtime -= 0.1;
if (p1cyan.x < p1cyantargetx - 75){
    p1cyan.anims.play('cyanwalk', true);
    p1cyan.x += 25
    if (p1cyan.y > p1cyantargety + 25){
        p1cyan.y -= 5
    }
    if (p1cyan.y < p1cyantargety + 25){
        p1cyan.y += 5
    }
}
if (p1cyan.x > p1cyantargetx - 76){
    p1cyanswordswing.x = p1cyan.x - 50
    p1cyanswordswing.y = p1cyan.y - 100
    p1cyan.anims.play('cyanattacksword', true);
    p1cyanswordswing.anims.play('cyanswordswing', true);
    p1cyanswordswing.visible = true
   }

if (p1cyanswordswingtime < 1){
    p1cyanswordswing.visible = false
    playerattacking = true
    playerbattleattack()
    p1cyan.x = p1cyanx
    p1cyan.y = p1cyany
    p1cyan.anims.play('cyanidle', true);
    playerabilityselected = false
    p1cyanselected = false
    player1selectabilitybox.visible = false
    team2turn = true
    team1turn = false
    p1cyantargetselected = false    
    p1cyanattackswordswing = false
    }
}
}

if (p1cyanattackmagicsword == true){
if (p1cyantargetselected == true){
p1cyanmagicswordtime -= 0.1;
if (p1cyan.x < p1cyantargetx - 75){
    p1cyan.anims.play('cyanwalk', true);
    if (p1cyanmagicswordsoundplayed == false){
        game.sound.add( "cyanmagicswordsound" ).play()
        p1cyanmagicswordsoundplayed = true
}
    p1cyan.x += 25
    if (p1cyan.y > p1cyantargety + 25){
        p1cyan.y -= 5
    }
    if (p1cyan.y < p1cyantargety + 25){
        p1cyan.y += 5
    }
}
if (p1cyan.x > p1cyantargetx - 76){
    p1cyanmagicsword.x = p1cyan.x - 25
    p1cyanmagicsword.y = p1cyan.y - 75
    p1cyanmagicswordeffect.x = p1cyantargetx - 30
    p1cyanmagicswordeffect.y = p1cyantargety 
    p1cyan.anims.play('cyanattacksword', true);
    p1cyanmagicsword.anims.play('cyanmagicsword', true);
    p1cyanmagicsword.visible = true
    p1cyanmagicswordeffect.anims.play('cyanmagicswordeffect', true);
    p1cyanmagicswordeffect.visible = true
    }

if (p1cyanmagicswordtime < 1){
    p1cyanmagicsword.visible = false
    p1cyanmagicswordeffect.visible = false
    playerattacking = true
    playerbattleattack()
    p1cyan.x = p1cyanx
    p1cyan.y = p1cyany
    p1cyan.anims.play('cyanidle', true);
    playerabilityselected = false
    p1cyanselected = false
    player1selectabilitybox.visible = false
    team2turn = true
    team1turn = false
    p1cyantargetselected = false    
    p1cyanattackmagicsword = false
    }
}
}

if (p1cyanattackcommand == true){
p1cyanidle = false
p1cyancommandtime -= 0.1;
if (p1cyan.x <  600) {
p1cyan.anims.play('cyanwalk', true);
p1cyan.x += 25 
}
if (p1cyan.x >  601) {
p1cyancommand.x = p1cyan.x - 80
p1cyancommand.y = p1cyan.y - 140
p1cyancommand.visible = true
p1cyancommand.anims.play('cyancommand', true);
if (p1cyancommandsoundplayed == false){
        game.sound.add( "cyancommandsound" ).play()
        p1cyancommandsoundplayed = true
}
p1cyan.anims.play('cyancast', true);

}
if (p1cyancommandtime < 1 ){
playerattacking = true
playerbattleattack()
p1cyanidle = true
playerabilityselected = false
p1cyanselected = false
player1selectabilitybox.visible = false
p1cyancommand.visible = false
p1cyan.x = p1cyanx
p1cyan.y = p1cyany
team2turn = true
team1turn = false
p1cyantargetselected = false
p1cyanattackcommand = false
}
}

if (p1cyanattackbattleplan == true){
p1cyanidle = false
p1cyanbattleplantime -= 0.1;
if (p1cyan.x <  600) {
p1cyan.anims.play('cyanwalk', true);
p1cyan.x += 25 
}
if (p1cyan.x >  601) {
p1cyanbattleplan.x = p1cyan.x - 80
p1cyanbattleplan.y = p1cyan.y - 140
p1cyanbattleplan.visible = true
p1cyanbattleplan.anims.play('cyanbattleplan', true);
if (p1cyanbattleplansoundplayed == false){
        game.sound.add( "cyanbattleplansound" ).play()
        p1cyanbattleplansoundplayed = true
}
p1cyan.anims.play('cyancast', true);

}
if (p1cyanbattleplantime < 1 ){
playerattacking = true
playerbattleattack()
p1cyanidle = true
playerabilityselected = false
p1cyanselected = false
player1selectabilitybox.visible = false
p1cyanbattleplan.visible = false
p1cyan.x = p1cyanx
p1cyan.y = p1cyany
team2turn = true
team1turn = false
p1cyantargetselected = false
p1cyanattackbattleplan = false
}
}
}
//endp1cyanaction


