}
}
function p2mogpowerkey(){
if (p2mogactionpoints > 49){    
if (playerabilityselected == false){
p2mogpowerbutton.setTint(0xff0000);
p2mogsupportpower = true
p2mogpowertime = 10
p2mogpowerauratime = 100
p2mogpowerstatdown = false
p2mogpowerstatup = false
p2mogpowersoundplayed = false
playerabilityselected = true
}
}
}
function p2mogdancekey(){
if (p2mogactionpoints > 49){    
if (playerabilityselected == false){
p2mogdancebutton.setTint(0xff0000);
p2mogsupportdance = true
p2mogdancetime = 10
p2mogdanceauratime = 100
p2mogdancestatdown = false
p2mogdancestatup = false
p2mogadancesoundplayed = false
playerabilityselected = true
}
}
}


