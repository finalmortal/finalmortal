        game.sound.add( "rosahealsound" ).play()
        p2rosahealsoundplayed = true
    }

        p2rosahealspell.x = p2rosatargetx - 45
        p2rosahealspell.y = p2rosatargety - 25

    if (p2rosahealtime < 1){
       playerattacking = true
