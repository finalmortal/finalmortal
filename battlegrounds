p1orchidspeedrating += p1rosafaststat
p1cyraxspeedrating += p1rosafaststat
p1golbezspeedrating += p1rosafaststat
p1jagospeedrating += p1rosafaststat
p1cyanspeedrating += p1rosafaststat
p1rosafaststatup = true
}
if (p1rosafaststatdown == true){
p1rosaspeedrating -= p1rosafaststat
p1mogspeedrating -= p1rosafaststat
p1sindelspeedrating -= p1rosafaststat
p1sonjaspeedrating -= p1rosafaststat
p1raidenspeedrating -= p1rosafaststat    
p1sektorspeedrating -= p1rosafaststat
p1glaciusspeedrating -= p1rosafaststat
p1kunglaospeedrating -= p1rosafaststat
p1combospeedrating -= p1rosafaststat
p1kanospeedrating -= p1rosafaststat
p1terraspeedrating -= p1rosafaststat
p1spinalspeedrating -= p1rosafaststat
