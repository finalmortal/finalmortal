p1sindelselected = false
player1selectabilitybox.visible = false
p1sindel.x = p1sindelx
p1sindel.y = p1sindely
p1sindelscreamgr.visible = false
p1sindelscreamgr.x = p1sindelx
p1sindelscreamgr.y = p1sindely
team2turn = true
team1turn = false
p1sindeltargetselected = false
p1sindelattackscream = false
}
}
}

if (p1sindelattackflybreath == true){
if (p1sindeltargetselected == true){
p1sindelidle = false

if (sindelflyingup == true){
if (p1sindel.y >  p1sindely - 300) {
p1sindel.anims.play('sindelflyup', true);
p1sindel.y -= 5
if (p1sindel.y < p1sindely - 299){
p1sindelflyingtotarget = true
p1sindelflyingup = false
}
}
}

if (p1sindelflyingtotarget == true){
p1sindel.x += 25
if (p1sindel.x <  p1sindeltargetx - 200) {
p1sindel.anims.play('sindelflymove', true);
}
if (p1sindel.x >  p1sindeltargetx - 199) {
p1sindelflybreathattack = true
p1sindelflybreathgr.x = p1sindel.x
p1sindelflybreathgr.y = p1sindel.y
p1sindelflyingtotarget = false
}
}

if (p1sindelflybreathattack == true){
p1sindelflybreathtime -= 0.3
p1sindelflybreathgr.visible = true
p1sindel.anims.play('sindelflybreath', true);
p1sindelflybreathgr.anims.play('sindelflybreathgr', true);
if (p1sindelflybreathgr.x < p1sindeltargetx + 25){
    p1sindelflybreathgr.x += 10;
}
if (p1sindelflybreathgr.y < p1sindeltargety + 25){
    p1sindelflybreathgr.y += 25;
}
if (p1sindelflybreathgr.y > p1sindeltargety + 24){
    p1sindelflybreathgr.visible = false
}
}

if (p1sindelflybreathtime < 1 ){
playerattacking = true
playerbattleattack()
p1sindelidle = true
playerabilityselected = false
p1sindelselected = false
p1sindelflybreathgr.visible = false
player1selectabilitybox.visible = false
p1sindelflybreathattack = false
p1sindelflybreathgr.x = p1sindelx
p1sindelflybreathgr.y = p1sindely
p1sindel.x = p1sindelx
p1sindel.y = p1sindely
team2turn = true
team1turn = false
p1sindeltargetselected = false
p1sindelattackflybreath = false
}
}
}

if (p1sindelattackhairwhip == true){
if (p1sindeltargetselected == true){
p1sindelidle = false
p1sindelhairwhiptime -= 0.1;
if (p1sindel.x < p1sindeltargetx - 200) {
p1sindel.anims.play('sindelrun', true);
p1sindel.x += 25 
if (p1sindel.y > p1sindeltargety - 50){
    p1sindel.y -= 6;
}
if (p1sindel.y < p1sindeltargety - 50){
    p1sindel.y += 6;
}
}
if (p1sindel.x > p1sindeltargetx - 201) {
p1sindel.anims.play('sindelhairwhip', true);
}
if (p1sindelhairwhiptime < 1){
playerattacking = true
playerbattleattack()
p1sindelidle = true
playerabilityselected = false
p1sindelselected = false
player1selectabilitybox.visible = false
p1sindel.x = p1sindelx
p1sindel.y = p1sindely
team2turn = true
team1turn = false
p1sindeltargetselected = false
p1sindelattackhairwhip = false
}
}
}
}

//p2sindelaction
function p2sindelaction(){
    if (p2sindelidle == true){
        p2sindel.anims.play('sindelidle',true);
    }
    
if (p2sindelattackflamebreath == true){
if (p2sindeltargetselected == true){
p2sindelidle = false
if (p2sindel.x > p2sindeltargetx) {
p2sindelflamebreathgr.visible = true
p2sindel.anims.play('sindelflamebreath', true);
p2sindelflamebreathgr.anims.play('sindelflamebreathgr', true);
p2sindelflamebreathgr.x -= 35
if (p2sindelflamebreathgr.y > p2sindeltargety){
    p2sindelflamebreathgr.y -= 7;
}
if (p2sindelflamebreathgr.y < p2sindeltargety){
    p2sindelflamebreathgr.y += 7;
}
}

if (p2sindelflamebreathgr.x < p2sindeltargetx + 45 ) {
playerattacking = true
playerbattleattack()
p2sindelidle = true
playerabilityselected = false
p2sindelselected = false
p2sindelflamebreathgr.visible = false
p2sindelflamebreathgr.x = p2sindel.x
p2sindelflamebreathgr.y = p2sindel.y 
player2selectabilitybox.visible = false
p2sindel.x = p2sindelx
p2sindel.y = p2sindely
team1turn = true
team2turn = false
p2sindeltargetselected = false
p2sindelattackflamebreath = false
}
}
}

if (p2sindelattackscream == true){
if (p2sindeltargetselected == true){
p2sindelidle = false
p2sindelscreamtime -= 0.1;
if (p2sindel.x > p2sindeltargetx + 71) {
p2sindel.anims.play('sindelrun', true);
p2sindel.x -= 25 
if (p2sindel.y > p2sindeltargety){
    p2sindel.y -= 7;
}
if (p2sindel.y < p2sindeltargety){
    p2sindel.y += 7;
}
}
if (p2sindel.x < p2sindeltargetx + 71) {
p2sindel.anims.play('sindelscream', true);
p2sindelscreamgr.x = p2sindel.x - 25
p2sindelscreamgr.y = p2sindel.y
p2sindelscreamgr.visible = true
p2sindelscreamgr.anims.play('sindelscreamgr', true);
}

if (p2sindelscreamtime < 1){
playerattacking = true
playerbattleattack()
p2sindelidle = true
playerabilityselected = false
p2sindelselected = false
player2selectabilitybox.visible = false
p2sindel.x = p2sindelx
p2sindel.y = p2sindely
p2sindelscreamgr.visible = false
p2sindelscreamgr.x = p2sindelx
p2sindelscreamgr.y = p2sindely
team1turn = true
team2turn = false
p2sindeltargetselected = false
p2sindelattackscream = false
}
}
}

if (p2sindelattackflybreath == true){
if (p2sindeltargetselected == true){
p2sindelidle = false

if (p2sindelflyingup == true){
if (p2sindel.y >  p2sindely - 300) {
