//********************************************

function resettarget(){
    if (cecilistarget == true){
        cecilistarget = false
    }
    if (kainistarget == true){
        kainistarget = false
    }
    if (kangistarget == true){
        kangistarget = false
    }
    if (orchidistarget == true){
        orchidistarget = false
    }
    if (spinalistarget == true){
        spinalistarget = false
    }
    if (jagoistarget == true){
        jagoistarget = false
    }
    if (terraistarget == true){
        terraistarget = false
    }
    if (kanoistarget == true){
        kanoistarget = false
    }
    if (comboistarget == true){
        comboistarget = false
    }
    if (sonjaistarget == true){
        sonjaistarget = false
    }

    if (kunglaoistarget == true){
        kunglaoistarget = false
    }
    if (glaciusistarget == true){
        glaciusistarget = false
    }
    if (golbezistarget == true){
        golbezistarget = false
    }
    if (sektoristarget == true){
        sektoristarget = false
    }
     if (cyraxistarget == true){
        cyraxistarget = false
    }
    if (sindelistarget == true){
        sindelistarget = false
    }
    if (raidenistarget == true){
        raidenistarget = false
    }
    if (cyanistarget == true){
        cyanistarget = false
    }

     if (rosaistarget == true){
        rosaistarget = false
    }
    if (mogistarget == true){
        mogistarget = false
    }

}


