    if (p2glaciusidle == true){
        p2glacius.anims.play('glaciusidle', true);
    }

if (p2glaciusattackicespike == true){
if (p2glaciustargetselected == true){
p2glaciusidle = false
p2glaciusicespiketime -= 0.2;
if (p2glacius.x > p2glaciustargetx + 140) {
if (p2glaciusicespikevoiceplayed == false){
        game.sound.add( "glaciusicespikevoice" ).play()
        p2glaciusicespikevoiceplayed = true
}
p2glacius.anims.play('glaciuswalk', true);
p2glacius.x -= 25 
if (p2glacius.y > p2glaciustargety){
    p2glacius.y -= 7;
}
if (p2glacius.y < p2glaciustargety){
    p2glacius.y += 7;
}
}
if (p2glacius.x < p2glaciustargetx + 141) {
p2glacius.anims.play('glaciusicespike', true);
}
if (p2glaciusicespiketime < 1){
playerattacking = true
playerbattleattack()
p2glaciusidle = true
playerabilityselected = false
p2glaciusselected = false
player2selectabilitybox.visible = false
p2glacius.x = p2glaciusx
p2glacius.y = p2glaciusy
team1turn = true
team2turn = false
p2glaciustargetselected = false
p2glaciusattackicespike = false
}
}
}

if (p2glaciusattackgrab == true){
if (p2glaciustargetselected == true){
p2glaciusidle = false
p2glaciusgrabtime -= 0.2;
if (p2glacius.x > p2glaciustargetx + 141) {
p2glacius.anims.play('glaciuswalk', true);
if (p2glaciusgrabvoiceplayed == false){
        game.sound.add( "glaciusgrabvoice" ).play()
        p2glaciusgrabvoiceplayed = true
}

p2glacius.x -= 25 
if (p2glacius.y > p2glaciustargety){
    p2glacius.y -= 7;
}
