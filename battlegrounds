//playerbuttons
function playerbuttons(){
if (team1turn == true){
    if (p1cecilselected == true){
    player1selectabilitybox.visible = true
    p1cecilswordbutton.visible = true
    p1cecilholybutton.visible = true 
    p1cecilholyswordbutton.visible = true
    p1cecilshieldbutton.visible = true
    p1ceciltargetbutton.visible = true
}
if (p1kainselected == true){
    player1selectabilitybox.visible = true
    p1kainslashbutton.visible = true
    p1kainjumpbutton.visible = true
    p1kainstrikebutton.visible = true
    p1kainpowerbutton.visible = true

}
if (p1kangselected == true){
    player1selectabilitybox.visible = true
    p1kangflykickbutton.visible = true
    p1kangfireballbutton.visible = true 
    p1kangbicyclebutton.visible = true
    p1kangdragonbutton.visible = true
    p1kangprayerbutton.visible = true
}
if (p1orchidselected == true){
    player1selectabilitybox.visible = true
    p1orchidwheelbutton.visible = true
    p1orchidfirebabutton.visible = true 
    p1orchidcatbutton.visible = true
    p1orchidbatonbutton.visible = true
    p1orchidfitbutton.visible = true
}
if (p1spinalselected == true){
    player1selectabilitybox.visible = true
    p1spinalslicebutton.visible = true
    p1spinalslashbutton.visible = true 
    p1spinalbashbutton.visible = true
    p1spinalspikebutton.visible = true
