//sonjaaction
//p1sonjaaction
function p1sonjaaction(){
if (p1sonjaidle == true){
p1sonja.anims.play('sonjaidle', true);
}

if (p1sonjaattackblaster == true){
if (p1sonjatargetselected == true){
p1sonjaidle = false
if (p1sonja.x < p1sonjatargetx) {
p1sonjablaster.visible = true
p1sonja.anims.play('sonjablaster', true);
p1sonjablaster.anims.play('sonjablastergr', true);
if (p1sonjablastersoundplayed == false){
        game.sound.add( "sonjablaster" ).play()
        p1sonjablastersoundplayed = true
    }

if (p1sonjablastervoiceplayed == false){
        game.sound.add( "sonjablastervoice" ).play()
        p1sonjablastervoiceplayed = true
    }
p1sonjablaster.x += 35
if (p1sonjablaster.y > p1sonjatargety){
    p1sonjablaster.y -= 7;
}
if (p1sonjablaster.y < p1sonjatargety){
    p1sonjablaster.y += 7;
}
}

if (p1sonjablaster.x > p1sonjatargetx -1 ) {
playerattacking = true
playerbattleattack()
p1sonjaidle = true
playerabilityselected = false
p1sonjaselected = false
p1sonjablaster.visible = false
p1sonjablaster.x = p1sonja.x
p1sonjablaster.y = p1sonja.y 
player1selectabilitybox.visible = false
p1sonja.x = p1sonjax
p1sonja.y = p1sonjay
team2turn = true
team1turn = false
p1sonjatargetselected = false
p1sonjaattackblaster = false
}
}
}

if (p1sonjaattackhandst == true){
if (p1sonjatargetselected == true){
p1sonjaidle = false
p1sonjahandsttime -= 0.2;
if (p1sonja.x < p1sonjatargetx) {
p1sonja.anims.play('sonjarun', true);
if (p1sonjahandstsoundplayed == false){
        game.sound.add( "sonjahandstsound" ).play()
        p1sonjahandstsoundplayed = true
    }
p1sonja.x += 25 
if (p1sonja.y > p1sonjatargety){
    p1sonja.y -= 7;
}
if (p1sonja.y < p1sonjatargety){
    p1sonja.y += 7;
}
}
if (p1sonja.x > p1sonjatargetx - 5) {
p1sonja.anims.play('sonjahandst', true);
}
if (p1sonjahandsttime < 1){
playerattacking = true
playerbattleattack()
p1sonjaidle = true
playerabilityselected = false
p1sonjaselected = false
player1selectabilitybox.visible = false
p1sonja.x = p1sonjax
p1sonja.y = p1sonjay
team2turn = true
team1turn = false
p1sonjatargetselected = false
p1sonjaattackhandst = false
}
}
}

if (p1sonjaattackkiss == true){
if (p1sonjatargetselected == true){
p1sonjaidle = false
p1sonjakisstime -= 0.3;
if (p1sonja.x < p1sonjatargetx) {
p1sonjakiss.visible = true
p1sonja.anims.play('sonjakiss', true);
p1sonjakiss.anims.play('sonjakissgr', true);
p1sonjakiss.setDepth(7);
p1sonjakiss.x = p1sonjatargetx
p1sonjakiss.y = p1sonjatargety + 35
}

if (p1sonjakisstime < 1) {
playerattacking = true
playerbattleattack()
p1sonjaidle = true
playerabilityselected = false
p1sonjaselected = false
p1sonjakiss.visible = false
p1sonjakiss.x = p1sonja.x
p1sonjakiss.y = p1sonja.y 
player1selectabilitybox.visible = false
p1sonja.x = p1sonjax
p1sonja.y = p1sonjay
team2turn = true
team1turn = false
p1sonjatargetselected = false
p1sonjaattackkiss = false
}
}
}

if (p1sonjaattackdancer == true){
p1sonjaidle = false
p1sonjadancertime -= 0.2;
if (p1sonja.x <  600) {
p1sonja.anims.play('sonjarun', true);
p1sonja.x += 25 
}
if (p1sonja.x >  601) {
p1sonja.anims.play('sonjadancer', true);
}
if (p1sonjadancertime < 1 ){
playerattacking = true
playerbattleattack()
p1sonjaidle = true
playerabilityselected = false
p1sonjaselected = false
player1selectabilitybox.visible = false
p1sonja.x = p1sonjax
p1sonja.y = p1sonjay
team2turn = true
team1turn = false
p1sonjatargetselected = false
p1sonjaattackdancer = false
}
}

if (p1sonjaattackkicks == true){
if (p1sonjatargetselected == true){
p1sonjaidle = false
p1sonjakickstime -= 0.2;
if (p1sonja.x < p1sonjatargetx) {
p1sonja.anims.play('sonjarun', true);
p1sonja.x += 25 
if (p1sonja.y > p1sonjatargety){
    p1sonja.y -= 7;
}
if (p1sonja.y < p1sonjatargety){
    p1sonja.y += 7;
}
}
if (p1sonja.x > p1sonjatargetx - 5) {
p1sonja.anims.play('sonjakicks', true);
}
if (p1sonjakickstime < 1){
playerattacking = true
playerbattleattack()
p1sonjaidle = true
playerabilityselected = false
p1sonjaselected = false
player1selectabilitybox.visible = false
p1sonja.x = p1sonjax
p1sonja.y = p1sonjay
team2turn = true
team1turn = false
p1sonjatargetselected = false
p1sonjaattackkicks = false
}
}
}
}

//sonjaaction
//p2sonjaaction
function p2sonjaaction(){
if (p2sonjaidle == true){
p2sonja.anims.play('sonjaidle', true);
}

if (p2sonjaattackblaster == true){
if (p2sonjatargetselected == true){
p2sonjaidle = false
if (p2sonja.x > p2sonjatargetx) {
p2sonjablaster.visible = true
p2sonja.anims.play('sonjablaster', true);
p2sonjablaster.anims.play('sonjablastergr', true);
if (p2sonjablastersoundplayed == false){
        game.sound.add( "sonjablaster" ).play()
        p2sonjablastersoundplayed = true
    }

if (p2sonjablastervoiceplayed == false){
        game.sound.add( "sonjablastervoice" ).play()
        p2sonjablastervoiceplayed = true
    }
p2sonjablaster.x -= 35
if (p2sonjablaster.y > p2sonjatargety){
    p2sonjablaster.y -= 7;
}
if (p2sonjablaster.y < p2sonjatargety){
    p2sonjablaster.y += 7;
}
}

if (p2sonjablaster.x < p2sonjatargetx + 20 ) {
playerattacking = true
playerbattleattack()
p2sonjaidle = true
playerabilityselected = false
p2sonjaselected = false
p2sonjablaster.visible = false
p2sonjablaster.x = p2sonja.x
p2sonjablaster.y = p2sonja.y 
player2selectabilitybox.visible = false
p2sonja.x = p2sonjax
p2sonja.y = p2sonjay
team1turn = true
team2turn = false
p2sonjatargetselected = false
p2sonjaattackblaster = false
}
}
}

if (p2sonjaattackhandst == true){
if (p2sonjatargetselected == true){
p2sonjaidle = false
p2sonjahandsttime -= 0.2;
if (p2sonja.x > p2sonjatargetx) {
p2sonja.anims.play('sonjarun', true);
if (p2sonjahandstsoundplayed == false){
        game.sound.add( "sonjahandstsound" ).play()
        p2sonjahandstsoundplayed = true
    }
p2sonja.x -= 25 
if (p2sonja.y > p2sonjatargety){
    p2sonja.y -= 7;
}
if (p2sonja.y < p2sonjatargety){
    p2sonja.y += 7;
}
}
if (p2sonja.x < p2sonjatargetx + 25) {
p2sonja.anims.play('sonjahandst', true);
}
if (p2sonjahandsttime < 1){
playerattacking = true
playerbattleattack()
p2sonjaidle = true
playerabilityselected = false
p2sonjaselected = false
player2selectabilitybox.visible = false
p2sonja.x = p2sonjax
p2sonja.y = p2sonjay
team1turn = true
team2turn = false
p2sonjatargetselected = false
p2sonjaattackhandst = false
}
}
}

if (p2sonjaattackkiss == true){
if (p2sonjatargetselected == true){
p2sonjaidle = false
p2sonjakisstime -= 0.3;
if (p2sonja.x > p2sonjatargetx) {
p2sonjakiss.visible = true
p2sonjakiss.setDepth(7);
p2sonja.anims.play('sonjakiss', true);
p2sonjakiss.anims.play('sonjakissgr', true);
p2sonjakiss.x = p2sonjatargetx
p2sonjakiss.y = p2sonjatargety + 30
}

