    this.input.setPollAlways();
        this.input.on('gameobjectover', function (pointer, gameObject) {
        gameObject.setTint(0xff0000);
    });
    
        this.input.on('gameobjectout', function (pointer, gameObject) {
        gameObject.clearTint();
    });

//orchidfireballaurabegin

//orchidfireballauraend

if (player1cecil == true){
//cecilholyaurabegin
var p1cecilholytextures = this.textures;
var holyorigin = p1cecilholy.getTopLeft();

    var p1holyauraSource = {
        getRandomPoint: function (vec)
        {
            do
            {
                var x = Phaser.Math.Between(0, p1cecilholy.width - 1);
                var y = Phaser.Math.Between(0, p1cecilholy.height - 1);
                var pixel = p1cecilholytextures.getPixel(x, y, 'cecilholy');
            } while (pixel.alpha < 255);

            return vec.setTo(x + p1cecil.x - 75, y + p1cecil.y - 75);
        }
    };
    var p1cecilholyparticles = this.add.particles('flares');

}

if (player2cecil == true){

var p2cecilholytextures = this.textures;
var p2holyorigin = p2cecilholy.getTopLeft();

    var p2holyauraSource = {
        getRandomPoint: function (vec)
        {
            do
            {
                var x = Phaser.Math.Between(0, p2cecilholy.width - 1);
                var y = Phaser.Math.Between(0, p2cecilholy.height - 1);
                var pixel = p2cecilholytextures.getPixel(x, y, 'cecilholy');
            } while (pixel.alpha < 255);

            return vec.setTo(x + p2cecil.x - 75, y + p2cecil.y - 75);
        }
    };
    var p2cecilholyparticles = this.add.particles('flares');
//cecilholyauraend
}

//BEGINACTIONS************************************
//cecil1action
function p1cecilaction(){
if (p1cecilselected == true){
if (p1cecilcastholy == true){
        p1cecilholy.x = p1cecil.x - 85
        p1cecilholy.y = p1cecil.y - 80
        p1cecil.anims.play('cecilcasting', true);
        p1cecilholy.anims.play('cecilholy', true);
        p1cecilholy.visible = true
        p1cecilholytime -= 0.5;
        //holyaura
        p1cecilholyparticles.createEmitter({
        x: 0,
        y: 0,
        lifespan: 1000,
        gravityY: 10,
        scale: { start: 0, end: 0.25, ease: 'Quad.easeOut' },
        alpha: { start: 1, end: 0, ease: 'Quad.easeIn' },
        blendMode: 'ADD',
        emitZone: { type: 'random', source: p1holyauraSource }

    });

    if (p1cecilholytime < 1){
        p1cecilcastholy = false
        p1cecil.anims.play('cecilidle', true);
        p1cecilholy.visible = false
        p1cecilholyaura = true
        playerabilityselected = false
        p1cecilselected = false
        player1selectabilitybox.visible = false
        team2turn = true
        team1turn = false
      
    }

}

if (p1cecilattackshield == true){
p1cecilidle = false
p1cecilshieldtime -= 0.1;
if (p1cecil.x <  600) {
p1cecil.anims.play('cecilwalk', true);
p1cecil.x += 20 
}
if (p1cecil.x >  601) {
p1cecilshield.y = p1cecil.y - 200
p1cecilshield.x = p1cecil.x - 100
p1cecil.anims.play('cecilcasting', true);
p1cecilshield.anims.play('cecilshield', true);
p1cecilshield.visible = true
}
if (p1cecilshieldtime < 1 ){
playerattacking = true
playerbattleattack()
p1cecilidle = true
p1cecil.anims.play('cecilidle', true);
playerabilityselected = false
p1cecilselected = false
p1cecilshield.visible = false
player1selectabilitybox.visible = false
p1cecil.x = p1cecilx
p1cecil.y = p1cecily
team2turn = true
team1turn = false
p1ceciltargetselected = false
p1cecilattackshield = false
}
}

//cecil1attacktarget******************
  if (p1ceciltargetselected == true){
    if (p1cecilattacktarget == true){
    if (p1cecil.x < p1ceciltargetx - 75){
            p1cecil.anims.play('cecilwalk', true);
            p1cecil.x += 25
            
    }
    if (p1cecil.y > p1ceciltargety + 25){
            p1cecil.y -= 5
    }
    if (p1cecil.y < p1ceciltargety + 25){
            p1cecil.y += 5
    }
    if (p1cecil.x > p1ceciltargetx - 76){
        p1ceciltargettime -= 1;
        p1ceciltarget.x = p1cecil.x - 100
        p1ceciltarget.y = p1cecil.y - 25
        playerbattleattack()
        p1cecil.anims.play('cecilsword', true);
        p1ceciltarget.anims.play('ceciltarget', true);
        
    if (p1ceciltargetsoundplayed == false){
            playerattacking = true 
            game.sound.add( "ceciltargetsound" ).play() 
            p1ceciltargetsoundplayed = true
            
    }
        p1ceciltarget.visible = true
    }

    if (p1ceciltargettime < 1){
        p1cecilattacktarget = false
        p1ceciltarget.visible = false
        p1cecil.x = p1cecilx
        p1cecil.y = p1cecily
       
        p1cecil.anims.play('cecilidle', true);
        p1ceciltargetselected = false
        playerabilityselected = false
        p1cecilselected = false
        player1selectabilitybox.visible = false
        team2turn = true
        team1turn = false
       

        
}
}
//endcecilattacktarget***********************
//cecilholysword
    if (p1cecilattackholysword == true){
        if (p1cecil.x < p1ceciltargetx - 75){
            p1cecil.anims.play('cecilwalk', true);
            p1cecil.x += 25
            
        }
         if (p1cecil.y > p1ceciltargety + 25){
            p1cecil.y -= 5
        }
        if (p1cecil.y < p1ceciltargety + 25){
            p1cecil.y += 5
        }
        
        
        if (p1cecil.x > p1ceciltargetx - 76){
        p1cecilholyswordtime -= 1;
        p1cecilholysword.x = p1cecil.x - 50
        p1cecilholysword.y = p1cecil.y - 25
        playerbattleattack()
        p1cecil.anims.play('cecilattacksword', true);
        p1cecilholysword.anims.play('cecilholysword', true);
         if (p1cecilholyswordsoundplayed == false){
            playerattacking = true 
            game.sound.add( "cecilholyswordsound" ).play() 
            p1cecilholyswordsoundplayed = true
        }
        p1cecilholysword.visible = true
    }

    if (p1cecilholyswordtime < 1){
        p1cecilattackholysword = false
        p1cecilholysword.visible = false
        p1cecil.x = p1cecilx
        p1cecil.y = p1cecily
        p1cecil.anims.play('cecilidle', true);
        p1ceciltargetselected = false
        playerabilityselected = false
        p1cecilselected = false
        player1selectabilitybox.visible = false
        team2turn = true
        team1turn = false
        
    }
}
//endcecilholysword
//cecilsword
    if (p1cecilattacksword == true){
        if (p1cecil.x < p1ceciltargetx - 75){
            p1cecil.anims.play('cecilwalk', true);
            p1cecil.x += 25
            
        }
         if (p1cecil.y > p1ceciltargety + 25){
            p1cecil.y -= 5
        }
        if (p1cecil.y < p1ceciltargety + 25){
            p1cecil.y += 5
        }
        
        if (p1cecil.x > p1ceciltargetx - 76){
            p1cecilswordtime -= 1;
        p1cecilsword.x = p1cecil.x - 7
        p1cecilsword.y = p1cecil.y - 25
        playerbattleattack()
        p1cecil.anims.play('cecilattacksword', true);
        if (p1cecilswordsoundplayed == false){
            playerattacking = true 
            game.sound.add( "cecilswordsound" ).play() 
            p1cecilswordsoundplayed = true
        }
        p1cecilsword.anims.play('cecilsword', true);
        p1cecilsword.visible = true
    }

if (p1cecilswordtime < 1){
        p1cecilattacksword = false
        p1cecilsword.visible = false
        p1cecil.x = p1cecilx
        p1cecil.y = p1cecily
        p1cecil.anims.play('cecilidle', true);
        p1ceciltargetselected = false
        playerabilityselected = false
        p1cecilselected = false
        player1selectabilitybox.visible = false
        team2turn = true
        team1turn = false
        
    }

    }
}
}
}
//cecil2action
function p2cecilaction(){
if (p2cecilselected == true){
if (p2cecilcastholy == true){
        p2cecilholy.x = p2cecil.x - 85
        p2cecilholy.y = p2cecil.y - 80
        p2cecil.anims.play('cecilcasting', true);
        p2cecilholy.anims.play('cecilholy', true);
        p2cecilholy.visible = true
        p2cecilholytime -= 0.5;
        //holyaura
        p2cecilholyparticles.createEmitter({
        x: 0,
        y: 0,
        lifespan: 1000,
        gravityY: 10,
        scale: { start: 0, end: 0.25, ease: 'Quad.easeOut' },
        alpha: { start: 1, end: 0, ease: 'Quad.easeIn' },
        blendMode: 'ADD',
        emitZone: { type: 'random', source: p2holyauraSource }

    });

    if (p2cecilholytime < 1){
        p2cecilcastholy = false
        p2cecil.anims.play('cecilidle', true);
        p2cecilholy.visible = false
        p2cecilholyaura = true
        playerabilityselected = false
        p2cecilselected = false
        player2selectabilitybox.visible = false
        team1turn = true
        team2turn = false
        resetai()
      
    }
}
    //cecil2attacktarget******************
  if (p2ceciltargetselected == true){
    if (p2cecilattacktarget == true){

        if (p2cecil.x > p2ceciltargetx + 75){
            p2cecil.anims.play('cecilwalk', true);
            p2cecil.x -= 25
            
        }
         if (p2cecil.y < p2ceciltargety + 25){
            p2cecil.y += 5
        }
        if (p2cecil.y > p2ceciltargety + 25){
            p2cecil.y -= 5
        }
        

        if (p2cecil.x < p2ceciltargetx + 76){
         
        p2ceciltargettime -= 1;
        p2ceciltarget.x = p2cecil.x - 100
        p2ceciltarget.y = p2cecil.y - 25
        playerbattleattack()
        p2cecil.anims.play('cecilsword', true);
        p2ceciltarget.anims.play('ceciltarget', true);
        
        if (p2ceciltargetsoundplayed == false){
            playerattacking = true 
            game.sound.add( "ceciltargetsound" ).play() 
            p2ceciltargetsoundplayed = true
            
        }
        p2ceciltarget.visible = true
    }

    if (p2ceciltargettime < 1){
        p2cecilattacktarget = false
        p2ceciltarget.visible = false
        p2cecil.x = p2cecilx
        p2cecil.y = p2cecily
        
        p2cecil.anims.play('cecilidle', true);
        p2ceciltargetselected = false
        playerabilityselected = false
        p2cecilselected = false
        player2selectabilitybox.visible = false
        team1turn = true
        team2turn = false
        resetai()
        
       

        
}
}
//endcecilattacktarget***********************8
//cecil2holysword
    if (p2cecilattackholysword == true){
        if (p2cecil.x > p2ceciltargetx + 75){
            p2cecil.anims.play('cecilwalk', true);
            p2cecil.x -= 25
            
        }
         if (p2cecil.y < p2ceciltargety + 25){
            p2cecil.y += 5
        }
        if (p2cecil.y > p2ceciltargety + 25){
            p2cecil.y -= 5
        }
        
        
        if (p2cecil.x < p2ceciltargetx + 76){
        p2cecilholyswordtime -= 1;
        p2cecilholysword.x = p2cecil.x - 50
        p2cecilholysword.y = p2cecil.y - 25
        playerbattleattack()
        p2cecil.anims.play('cecilattacksword', true);
        p2cecilholysword.anims.play('cecilholysword', true);
         if (p2cecilholyswordsoundplayed == false){
            playerattacking = true 
            game.sound.add( "cecilholyswordsound" ).play() 
            p2cecilholyswordsoundplayed = true
        }
        p2cecilholysword.visible = true
    }

    if (p2cecilholyswordtime < 1){
        p2cecilattackholysword = false
        p2cecilholysword.visible = false
        p2cecil.x = p2cecilx
        p2cecil.y = p2cecily
        p2cecil.anims.play('cecilidle', true);
        p2ceciltargetselected = false
        playerabilityselected = false
        p2cecilselected = false
        player2selectabilitybox.visible = false
        team1turn = true
        team2turn = false
        resetai()
        
    }
}
//endcecilholysword
//cecil2sword
    if (p2cecilattacksword == true){
        if (p2cecil.x > p2ceciltargetx + 75){
            p2cecil.anims.play('cecilwalk', true);
            p2cecil.x -= 25
            
        }
         if (p2cecil.y < p2ceciltargety + 25){
            p2cecil.y += 5
        }
        if (p2cecil.y > p2ceciltargety + 25){
            p2cecil.y -= 5
        }
        
        if (p2cecil.x < p2ceciltargetx + 76){
        p2cecilswordtime -= 1;
        p2cecilsword.x = p2cecil.x - 7
        p2cecilsword.y = p2cecil.y - 25
        playerbattleattack()
        p2cecil.anims.play('cecilattacksword', true);
        if (p2cecilswordsoundplayed == false){
            playerattacking = true 
            game.sound.add( "cecilswordsound" ).play() 
            //playerbattleattackhere
            p2cecilswordsoundplayed = true
        }
        p2cecilsword.anims.play('cecilsword', true);
        p2cecilsword.visible = true
    }

if (p2cecilswordtime < 1){
        p2cecilattacksword = false
        p2cecilsword.visible = false
        p2cecil.x = p2cecilx
        p2cecil.y = p2cecily
        p2cecil.anims.play('cecilidle', true);
        p2ceciltargetselected = false
        playerabilityselected = false
        p2cecilselected = false
        player2selectabilitybox.visible = false
        team1turn = true
        team2turn = false
        resetai()
        
    }
    }

}
}
