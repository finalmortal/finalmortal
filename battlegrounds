p1kunglaodidle = false
if (p1kunglao.x < p1kunglaotargetx) {
p1kunglaohat.visible = true
p1kunglao.anims.play('kunglaohatthrow', true);
p1kunglaohat.anims.play('kunglaohatthrowgr', true);
p1kunglaohat.x += 45
}
if (p1kunglaohat.x > p1kunglaotargetx - 25) {
playerattacking = true
playerbattleattack()
p1kunglaoidle = true
playerabilityselected = false
p1kunglaoselected = false
player1selectabilitybox.visible = false
p1kunglaohat.visible = false
p1kunglaohat.x = p1kunglaox
p1kunglaohat.y = p1kunglaoy
team2turn = true
team1turn = false
p1kunglaotargetselected = false
p1kunglaoattackhatthrow = false
}
}
}

if (p1kunglaoattackpalmkick == true){
if (p1kunglaotargetselected == true){
p1kunglaoidle = false
p1kunglaopalmkicktime -= 0.2;
if (p1kunglao.x < p1kunglaotargetx) {
p1kunglao.anims.play('kunglaorun', true);
p1kunglao.x += 25 
if (p1kunglao.y > p1kunglaotargety){
    p1kunglao.y -= 7;
}
if (p1kunglao.y < p1kunglaotargety){
    p1kunglao.y += 7;
}
}
if (p1kunglao.x > p1kunglaotargetx - 10) {
p1kunglao.anims.play('kunglaopalmkick', true);
}
