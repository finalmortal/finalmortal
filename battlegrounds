//rosattacks
if (p1rosaselected == true){
    currentplayerhealth = p1rosahealthrating
    currentplayerattackrating = p1rosaattackrating
    currentplayerdefenserating = p1rosadefenserating
    currentplayermagicrating = p1rosamagicrating
    currentplayerspeedrating = p1rosaspeedrating
    currentplayersupportrating = p1rosasupportrating

    if (p1rosasupportcure == true){
        currenttargethealthrating +=   p1rosasupportcurehealing * currentplayerchargepower + currentplayersupportrating 
        p1rosaactionpoints -= 49
    }
    if (p1rosasupportheal == true){
        currenttargethealthrating +=   p1rosasupporthealhealing * currentplayerchargepower + currentplayersupportrating 
        p1rosaactionpoints -= 24
    }
    if (p1rosasupportfast == true){
        p1rosafastaura = true
        p1rosaactionpoints -= 50
    }
    if (p1rosasupportshell == true){
        p1rosaactionpoints -= 74
    }
    if (p1rosaattackbow == true){
        currenttargethealthrating -=   p1rosaattackbowdamage - currentplayerattackrating 
        p1rosaactionpoints += 25
    }
    currentattacktarget()
}
//
if (p2rosaselected == true){
    currentplayerhealth = p2rosahealthrating
    currentplayerattackrating = p2rosaattackrating
    currentplayerdefenserating = p2rosadefenserating
    currentplayermagicrating = p2rosamagicrating
    currentplayerspeedrating = p2rosaspeedrating
    currentplayersupportrating = p2rosasupportrating

    if (p2rosasupportcure == true){
        currenttargethealthrating +=   p2rosasupportcurehealing * currentplayerchargepower + currentplayersupportrating 
        p2rosaactionpoints -= 49

    }
    if (p2rosasupportheal == true){
        currenttargethealthrating +=   p2rosasupporthealhealing * currentplayerchargepower + currentplayersupportrating 
        p2rosaactionpoints -= 24
    }
    if (p2rosasupportfast == true){
        p2rosaactionpoints -= 24
    }
    if (p2rosasupportshell == true){
        p2rosaactionpoints -= 74
    }
    if (p2rosaattackbow == true){
        currenttargethealthrating -=   p2rosaattackbowdamage - currentplayerattackrating
        p2rosaactionpoints += 25
    }

   currentattacktarget()
}

if (p1mogselected == true){
    currentplayerhealthrating = p1moghealthrating
    currentplayerattackrating = p1mogattackrating
    currentplayerdefenserating = p1mogdefenserating
    currentplayermagicrating = p1mogmagicrating
    currentplayerspeedrating = p1mogspeedrating
    currentplayersupportrating = p1mogsupportrating

    if (p1mogsupportmoon == true){
        p1mogmoonaura == true
        p1mogactionpoints -= 49
    }
    if (p1mogsupportpower == true){
        p1mogpoweraura == true
        p1mogactionpoints -= 49
    }
    if (p1mogsupportdance == true){
        p1mogdanceaura == true
        p1mogactionpoints -= 49
    }
    if (p1mogsupportdream == true){
        p1mogdreamaura == true
        p1mogactionpoints -= 49
    }
    if (p1mogsupportbull == true){
        p1mogbullaura == true
        p1mogactionpoints -= 49
    }
    currentattacktarget()
}
//
if (p2mogselected == true){
    currentplayerhealthrating = p2moghealthrating
    currentplayerattackrating = p2mogattackrating
    currentplayerdefenserating = p2mogdefenserating
    currentplayermagicrating = p2mogmagicrating
    currentplayerspeedrating = p2mogspeedrating
    currentplayersupportrating = p2mogsupportrating

    if (p2mogsupportmoon == true){
        p2mogactionpoints -= 49
        p2mogmoonaura == true
    }
    if (p2mogsupportpower == true){
        p2mogpoweraura == true
        p2mogactionpoints -= 49
    }
    if (p2mogsupportdance == true){
        p2mogdanceaura == true
        p2mogactionpoints -= 49
    }
    if (p2mogsupportdream == true){
        p2mogdreamaura == true
        p2mogactionpoints -= 49
    }
    if (p2mogsupportbull == true){
        p2mogbullaura == true
        p2mogactionpoints -= 49
    }
   currentattacktarget()
}

///
}
currentplayerchargepower = 1
playerattacking = false
notenoughaction()
resettarget()
rounds()
}
//endplayerbattleattack


