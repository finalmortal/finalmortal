//updatetargets
function updatetargets(){
if (loadcecil == true){
p1ceciltargetx = p1cecilselectedtargetx
p1ceciltargety = p1cecilselectedtargety
p2ceciltargetx = p2cecilselectedtargetx
p2ceciltargety = p2cecilselectedtargety
}
if (loadkain == true){
p1kaintargetx = p1kainselectedtargetx
p1kaintargety = p1kainselectedtargety
