//begincreate battleground************************************************************
//createbg

create: function ()
{
        
        
    youlosescreen = this.physics.add.sprite(25, 0, 'youlosescreen').setOrigin(0, 0);
    youlosescreen.visible = false
    youwinscreen = this.physics.add.sprite(25, 0, 'youwinscreen').setOrigin(0, 0);
    youwinscreen.visible = false
    p1winscreen = this.physics.add.sprite(25, 0, 'p1winscreen').setOrigin(0, 0);
    p1winscreen.visible = false
    p2winscreen = this.physics.add.sprite(25, 0, 'p2winscreen').setOrigin(0, 0);
    p2winscreen.visible = false
    youlosescreen.setDepth(7);
    youwinscreen.setDepth(7);
    p1winscreen.setDepth(7);
    p2winscreen.setDepth(7);
    
    

  
    


        
        if (ancienttemplebattleground == true){
        backgroundclouds = true
        ancienttemplebackground = this.physics.add.sprite(0, 0, 'ancienttemplebackground').setOrigin(0, 0); 
        mountainclouds2 = this.add.sprite(900, -150, 'mountainclouds2').setOrigin(0, 0); 
        mountainclouds1 = this.add.sprite(0, -100, 'mountainclouds1').setOrigin(0, 0); 
        ancienttemplefrontpiece = this.physics.add.sprite(0, 0, 'ancienttemplefrontpiece').setOrigin(0, 0);
        ancienttemplehead = this.physics.add.sprite(592, 0, 'ancienttemplehead').setOrigin(0, 0);
        }

        if (dumpgroundbattleground == true){
        backgroundclouds = true
        mountainclouds2 = this.add.sprite(900, -150, 'mountainclouds2').setOrigin(0, 0); 
        mountainclouds1 = this.add.sprite(0, -100, 'mountainclouds1').setOrigin(0, 0); 
        dumpgroundfrontpiece = this.physics.add.sprite(0, 0, 'dumpgroundfrontpiece').setOrigin(0, 0); 
        dumpgroundfrontwall = this.physics.add.sprite(0, 0, 'dumpgroundfrontwall').setOrigin(0, 0); 
        buzzingflies = this.add.sprite(400, 10, 'buzzingflies').setOrigin(0, 0); 
        }

        if (waterfallbattleground == true){
        backgroundclouds = true
        mountainclouds2 = this.add.sprite(900, -150, 'mountainclouds2').setOrigin(0, 0); 
        mountainclouds1 = this.add.sprite(0, -100, 'mountainclouds1').setOrigin(0, 0); 
        waterfallfrontpiece = this.physics.add.sprite(0, 0, 'waterfallfrontpiece').setOrigin(0, 0);
        falls = this.add.sprite(625, -58, 'falls').setOrigin(0, 0);
        }
        if (stellarcitybattleground == true){
        backgroundclouds = true
        ancienttemplebackground = this.physics.add.sprite(0, 0, 'ancienttemplebackground').setOrigin(0, 0);
        mountainclouds2 = this.add.sprite(900, -150, 'mountainclouds2').setOrigin(0, 0); 
        mountainclouds1 = this.add.sprite(0, -100, 'mountainclouds1').setOrigin(0, 0);
        stellarcityfrontpiece = this.physics.add.sprite(0, 0, 'stellarcityfrontpiece').setOrigin(0, 0);
        arcade1 = this.physics.add.sprite(806, 145, 'arcade1').setOrigin(0, 0);
        arcade2 = this.physics.add.sprite(1082, 144, 'arcade2').setOrigin(0, 0);
        smokeface = this.physics.add.sprite(535, 175, 'smokeface').setOrigin(0, 0);
        fingertop = this.physics.add.sprite(466, 75, 'fingertop').setOrigin(0, 0);
        fingerarrow = this.physics.add.sprite(600, 160, 'fingerarrow').setOrigin(0, 0);
        thefinger = this.physics.add.sprite(729, 75, 'thefinger').setOrigin(0, 0);
        trianglerotate = this.physics.add.sprite(120, 152, 'trianglerotate').setOrigin(0, 0);
        stellarshoptv = this.physics.add.sprite(48, 183, 'stellarshoptv').setOrigin(0, 0);
        }

        if (beachbattleground == true){
        backgroundclouds = true
        ancienttemplebackground = this.physics.add.sprite(0, 0, 'ancienttemplebackground').setOrigin(0, 0); 
        mountainclouds2 = this.add.sprite(900, -350, 'mountainclouds2').setOrigin(0, 0); 
        mountainclouds1 = this.add.sprite(0, -300, 'mountainclouds1').setOrigin(0, 0);
        beachfrontpiece = this.physics.add.sprite(0, 0, 'beachfrontpiece').setOrigin(0, 0); 
        }
        if (graveyardbattleground == true){
        backgroundclouds = true
        ancienttemplebackground = this.physics.add.sprite(0, 0, 'ancienttemplebackground').setOrigin(0, 0);
        mountainclouds2 = this.add.sprite(900, -150, 'mountainclouds2').setOrigin(0, 0); 
        mountainclouds1 = this.add.sprite(0, -100, 'mountainclouds1').setOrigin(0, 0);
        graveyardfrontpiece = this.physics.add.sprite(0, 0, 'graveyardfrontpiece').setOrigin(0, 0); 
        }

        if (oakloungebattleground == true){
        colorsmear = this.physics.add.sprite(20, 0, 'colorsmear').setOrigin(0, 0);
        colortrip = this.physics.add.sprite(150, 0, 'colortrip').setOrigin(0, 0);
        outsidebackground = this.physics.add.sprite(460, 0, 'colortrip').setOrigin(0, 0);
        trianglean = this.physics.add.sprite(790, 5, 'trianglean').setOrigin(0, 0);
        oakloungefrontpiece = this.add.tileSprite(0, 0, 1365, 600, 'oakloungefrontpiece').setOrigin(0, 0);
        doorlights = this.physics.add.sprite(355, 10, 'doorlights').setOrigin(0, 0);
        tablelamp = this.physics.add.sprite(1285, 143, 'tablelamp1');
        }

        if (dojobattleground == true){
        dojofrontpiece = this.physics.add.sprite(0, 0, 'dojofrontpiece').setOrigin(0, 0);
        }

        if (launchtowerbattleground == true){
        launchbackground1 = this.physics.add.sprite(0, 0, 'launchbackground1').setOrigin(0, 0);
        launchtowerfrontpiece = this.physics.add.sprite(0, 0, 'launchtowerfrontpiece').setOrigin(0, 0);

        }

        if (airportbattleground == true){
        airportbackground = this.add.sprite(0, 0, 'airportbackground').setOrigin(0, 0);
        airportfrontpiece = this.add.sprite(0, 0, 'airportfrontpiece').setOrigin(0, 0);
        airportplanelight = this.physics.add.sprite(341, 195, 'planelight').setOrigin(0, 0);
        }

        matchstartinggraphic = this.physics.add.sprite(298, 5, 'matchstartinggraphic').setOrigin(0, 0);
        matchstartinggraphic.visible = false
        roundhover = this.physics.add.sprite(450, 200, 'roundhover').setOrigin(0, 0);
        roundhover.visible = false
        roundtxt = this.add.text(625, 325, '1', { font: '111px Arial', fill: '#ffffff' }).setScrollFactor(0);
        roundtxt.visible = false
        team1hphover = this.physics.add.sprite(0, 0, 'team1hphover').setOrigin(0, 0);
        team2hphover = this.physics.add.sprite(1024, 0, 'team2hphover').setOrigin(0, 0);


        



        if (ancienttemplebattleground == true){
        if (musicon == true){
        var templetheme = this.sound.add('templetheme',{ loop: true });
        templetheme.play({
        });
        }
        }

        if (stellarcitybattleground == true){
        if (musicon == true){
        var stellartheme = this.sound.add('stellartheme',{ loop: true });
        stellartheme.play({
        });
        }
        }

        if (dumpgroundbattleground == true){
        if (musicon == true){
        var dumpgroundtheme = this.sound.add('dumpgroundtheme',{ loop: true });
        dumpgroundtheme.play({
        });
        }
        }

        if (waterfallbattleground == true){
        if (musicon == true){
        var waterfalltheme = this.sound.add('waterfalltheme',{ loop: true });
        waterfalltheme.play({
        });
        }
        }

        if (beachbattleground == true){
        if (musicon == true){
        var beachtheme = this.sound.add('beachtheme',{ loop: true });
        beachtheme.play({
        });
        }
        }

        if (graveyardbattleground == true){
        if (musicon == true){
        var graveyardtheme = this.sound.add('graveyardtheme',{ loop: true });
        graveyardtheme.play({
        });
        }
        }

        if (launchtowerbattleground == true){
        if (musicon == true){
        var launchtowertheme = this.sound.add('launchtowertheme',{ loop: true });
        launchtowertheme.play({
        });
        }
        }
        if (dojobattleground == true){
        if (musicon == true){
        var dojotheme = this.sound.add('dojotheme',{ loop: true });
        dojotheme.play({
        });
        }
        }
        if (airportbattleground == true){
        if (musicon == true){
        var airporttheme = this.sound.add('airporttheme',{ loop: true });
        airporttheme.play({
        });
        }
        }
        if (oakloungebattleground == true){
        if (musicon == true){
        var oakloungetheme = this.sound.add('oakloungetheme',{ loop: true });
        oakloungetheme.play({
        });
        }
        }
    //loadcecil     
    if (loadcecil == true){
    if (player1cecil == true){
    setplayerposition()
    p1cecil = this.add.sprite(p1cecilx, p1cecily, 'cecil').setInteractive();
    p1cecil.setOrigin(0, 0);
    p1cecil.setFlipX(true);
    p1cecil.on('pointerdown', function (pointer) {
    p1cecilbuttonclick()
    });
    p1ceciltimer = this.time.addEvent({ delay: 50, callback: p1cecilaction, callbackScope: this, loop: true,startAt: 1 });
    }   
//loadcecil1end
    if (player2cecil == true){
    setplayerposition()
    p2cecil = this.add.sprite(p2cecilx, p2cecily, 'cecil').setInteractive();
    p2cecil.setOrigin(0, 0);
    p2cecil.on('pointerdown', function (pointer) {
    p2cecilbuttonclick()
    });
    p2ceciltimer = this.time.addEvent({ delay: 50, callback: p2cecilaction, callbackScope: this, loop: true,startAt: 1 });
    }
    }
//loadcecil2end    
//loadcecilend

//loadkain     
    if (loadkain == true){
    if (player1kain == true){
    setplayerposition()
    p1kain = this.add.sprite(p1kainx, p1kainy, 'kain').setInteractive();
    p1kain.setOrigin(0, 0);
    p1kain.setFlipX(true);
    p1kain.on('pointerdown', function (pointer) {
    p1kainbuttonclick()   
    });
    p1kaintimer = this.time.addEvent({ delay: 50, callback: p1kainaction, callbackScope: this, loop: true,startAt: 1 });
    }   
    //loadkain1end

    if (player2kain == true){
    setplayerposition()
    p2kain = this.add.sprite(p2kainx, p2kainy, 'kain').setInteractive();
