//removetint
function removetint(){
if(player1cecil == true){
p1cecil.clearTint();
p1cecilswordbutton.clearTint();
p1cecilholybutton.clearTint();
p1cecilholyswordbutton.clearTint();
p1cecilshieldbutton.clearTint();
p1ceciltargetbutton.clearTint();  
}
if(player1kain == true){
p1kain.clearTint();
p1kainjumpbutton.clearTint();
p1kainpowerbutton.clearTint();
p1kainslashbutton.clearTint();
p1kainstrikebutton.clearTint();
}
if(player1kang == true){
p1kang.clearTint();
p1kangfireballbutton.clearTint();
p1kangflykickbutton.clearTint();
p1kangbicyclebutton.clearTint();
p1kangdragonbutton.clearTint();
p1kangprayerbutton.clearTint();
}
if (player1orchid == true){
