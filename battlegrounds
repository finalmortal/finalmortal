 function keyfunctions(){
    if (p1cecilselected == true){
    if (Phaser.Input.Keyboard.JustDown(onekey))
    {
    p1cecilswordkey()
    }
    if (Phaser.Input.Keyboard.JustDown(twokey))
    {
    p1cecilblockkey()
    }

    if (Phaser.Input.Keyboard.JustDown(threekey))
    {
   p1cecilholykey()
    }

    if (Phaser.Input.Keyboard.JustDown(fourkey))
    {
    p1cecilholyswordkey()
    }
   if (Phaser.Input.Keyboard.JustDown(fivekey))
    {
    p1ceciltargetkey()
    }
    }
    

    if (p1kainselected == true){
    if (Phaser.Input.Keyboard.JustDown(onekey))
    {
    p1kainstrikekey()
    }
    if (Phaser.Input.Keyboard.JustDown(twokey))
    {
    p1kainslashkey()
    }

    if (Phaser.Input.Keyboard.JustDown(threekey))
    {
    p1kainjumpkey()
    }
   if (Phaser.Input.Keyboard.JustDown(fourkey))
    {
    p1kainpowerkey()
    }
    }


    if (p1kangselected == true){
    if (Phaser.Input.Keyboard.JustDown(onekey))
    {
    p1kangflykickkey()
    }
    if (Phaser.Input.Keyboard.JustDown(twokey))
    {
    p1kangfireballkey()
    }
    if (Phaser.Input.Keyboard.JustDown(threekey))
    {
    p1kangbicyclekey()
    }
   if (Phaser.Input.Keyboard.JustDown(fourkey))
    {
    p1kangdragonkey()
    }
     if (Phaser.Input.Keyboard.JustDown(fivekey))
    {
    p1kangprayerkey()
    }
    }

    if (p1orchidselected == true){
    if (Phaser.Input.Keyboard.JustDown(onekey))
    {
    p1orchidwheelkey()
    }
    if (Phaser.Input.Keyboard.JustDown(twokey))
    {
    p1orchidfirebakey()
    }
    if (Phaser.Input.Keyboard.JustDown(threekey))
    {
    p1orchidcatkey()
    }
   if (Phaser.Input.Keyboard.JustDown(fourkey))
    {
    p1orchidbatonkey()
    }
     if (Phaser.Input.Keyboard.JustDown(fivekey))
    {
    p1orchidfitkey()
    }
    }

