//playerposition
function setplayerposition() {
    if (player1cecil == true){
    if (player1p1name == 'CECIL') {      
    p1cecilx = team1player1x - 20
    p1cecily = team1player1y - 35
    }
    if (player1p2name == 'CECIL') {      
    p1cecilx = team1player2x - 20
    p1cecily = team1player2y - 35
    }
     if (player1p3name == 'CECIL') {      
