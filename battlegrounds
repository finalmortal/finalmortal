//p1rosaaction
//rosaaction
function p1rosaaction(){
    if (p1rosaidle == true){
        p1rosa.anims.play('rosaidle', true);
    }
    if (p1rosasupportcure == true){
        if (p1rosatargetselected == true){
        p1rosaidle = false
        p1rosacuretime -= 0.2;
        p1rosa.anims.play('rosacasting', true);
        p1rosacurespell.visible = true
        p1rosacurespell.anims.play('rosacurespell', true);
        if (p1rosacuresoundplayed == false){
        game.sound.add( "rosacuresound" ).play()
        p1rosacuresoundplayed = true
    }
        p1rosacurespell.x = p1rosatargetx - 20
        p1rosacurespell.y = p1rosatargety - 10
    if (p1rosacuretime < 1){
       playerattacking = true
       playerbattleattack()
       p1rosaidle = true
       playerabilityselected = false
       p1rosaselected = false
       player1selectabilitybox.visible = false
       p1rosacurespell.visible = false
       p1rosa.x = p1rosax
       p1rosa.y = p1rosay
       team2turn = true
       team1turn = false
       p1rosahealingtarget = false
       p1rosatargetselected = false
       p1rosasupportcure = false
    }
    }
    }
    
if (p1rosasupportheal == true){
        if (p1rosatargetselected == true){
        p1rosaidle = false
        p1rosahealtime -= 0.2;
        p1rosa.anims.play('rosacasting', true);
        p1rosahealspell.visible = true
        p1rosahealspell.anims.play('rosahealspell', true);
        if (p1rosahealsoundplayed == false){
        game.sound.add( "rosahealsound" ).play()
        p1rosahealsoundplayed = true
    }
        p1rosahealspell.x = p1rosatargetx - 20
        p1rosahealspell.y = p1rosatargety - 10

    if (p1rosahealtime < 1){
       playerattacking = true
       playerbattleattack()
       p1rosaidle = true
       playerabilityselected = false
       p1rosaselected = false
       player1selectabilitybox.visible = false
       p1rosahealspell.visible = false
       p1rosa.x = p1rosax
       p1rosa.y = p1rosay
       team2turn = true
       team1turn = false
       p1rosahealingtarget = false
       p1rosatargetselected = false
       p1rosasupportheal = false
    }
    }
    }
 if (p1rosasupportfast == true){
        p1rosaidle = false
        p1rosafasttime -= 0.2;
        p1rosa.anims.play('rosacasting', true);
        p1rosafastspell.visible = true
        p1rosafastspell.anims.play('rosafastspell', true);
        if (p1rosafastsoundplayed == false){
        game.sound.add( "rosafastsound" ).play()
        p1rosafastsoundplayed = true
    }
        p1rosafastspell.x = p1rosatargetx - 20
        p1rosafastspell.y = p1rosatargety - 10
    if (p1rosafasttime < 1){
       playerattacking = true
       playerbattleattack()
       p1rosaidle = true
       playerabilityselected = false
       p1rosaselected = false
       player1selectabilitybox.visible = false
       p1rosafastspell.visible = false
       p1rosa.x = p1rosax
       p1rosa.y = p1rosay
       p1rosafastaura = true
       p1rosaspells()
       team2turn = true
       team1turn = false
       p1rosahealingtarget = true
       p1rosatargetselected = false
       p1rosasupportfast = false
    }
    }

     if (p1rosasupportshell == true){
        p1rosaidle = false
        p1rosashelltime -= 0.2;
        p1rosa.anims.play('rosacasting', true);
        p1rosashellspell.visible = true
        p1rosashellspell.anims.play('rosashellspell', true);
        if (p1rosashellsoundplayed == false){
        game.sound.add( "rosashellsound" ).play()
        p1rosashellsoundplayed = true
    }
        p1rosashellspell.x = p1rosatargetx - 20
        p1rosashellspell.y = p1rosatargety - 10
    if (p1rosashelltime < 1){
       playerattacking = true
       playerbattleattack()
       p1rosaidle = true
       playerabilityselected = false
       p1rosaselected = false
       player1selectabilitybox.visible = false
       p1rosashellspell.visible = false
       p1rosa.x = p1rosax
       p1rosa.y = p1rosay
       p1rosashellaura = true
       p1rosaspells()
       team2turn = true
       team1turn = false
       p1rosahealingtarget = true
       p1rosatargetselected = false
       p1rosasupportshell = false
    }
    }
}
//endrosap2
