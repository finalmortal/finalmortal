//p2spinalaction
function p2spinalaction(){
if (p2spinalidle == true){
p2spinal.anims.play('spinalidle', true);
}
//spinalattackslice
if (p2spinalattackslice == true){
p2spinalslicetime -= 0.1
if (p2spinaltargetselected == true){
p2spinalidle = false
 if (p2spinalslicesoundplayed == false){
        game.sound.add( "spinalslice" ).play()
        p2spinalslicesoundplayed = true
    }
if (p2spinal.x > p2spinaltargetx + 51) {
p2spinal.anims.play('spinalrun', true);
p2spinal.x -= 25 
if (p2spinal.y < p2spinaltargety){
    p2spinal.y += 7;
}
if (p2spinal.y > p2spinaltargety){
    p2spinal.y -= 7;
}
}
if (p2spinal.x < p2spinaltargetx + 52) {
p2spinal.anims.play('spinalslice', true);
p2spinal.x = p2spinaltargetx - 10
p2spinal.y = p2spinaltargety - 55

}
if (p2spinalslicetime < 1){
playerattacking = true
playerbattleattack()
p2spinalidle = true
playerabilityselected = false
p2spinalselected = false
player2selectabilitybox.visible = false
p2spinaltargetselected = false
p2spinal.x = p2spinalx
p2spinal.y = p2spinaly
team1turn = true
team2turn = false
p2spinalslicesoundplayed = false
p2spinalattackslice = false

}
}
}
//endspinalslice

//spinalattackslash
if (p2spinalattackslash == true){
p2spinalslashtime -= 0.1
if (p2spinaltargetselected == true){
p2spinalidle = false
if (p2spinalslashsoundplayed == false){
        game.sound.add( "spinalslash" ).play()
        p2spinalslashsoundplayed = true
    }
if (p2spinal.x > p2spinaltargetx + 51) {
p2spinal.anims.play('spinalrun', true);
p2spinal.x -= 25 
if (p2spinal.y < p2spinaltargety){
    p2spinal.y += 7;
}
if (p2spinal.y > p2spinaltargety){
    p2spinal.y -= 7;
}
}
if (p2spinal.x < p2spinaltargetx + 52) {
p2spinal.anims.play('spinalslash', true);
p2spinal.x = p2spinaltargetx - 10
p2spinal.y = p2spinaltargety - 55
}
if (p2spinalslashtime < 1){
playerattacking = true
playerbattleattack()
p2spinalidle = true
playerabilityselected = false
p2spinalselected = false
player2selectabilitybox.visible = false
p2spinaltargetselected = false
p2spinal.x = p2spinalx
p2spinal.y = p2spinaly
team1turn = true
team2turn = false
p2spinalslashsoundplayed = false
p2spinalattackslash = false

}
}
}
//endspinalslash
//spinalattackbash
if (p2spinalattackbash == true){
p2spinalbashtime -= 0.1
if (p2spinaltargetselected == true){
p2spinalidle = false
if (p2spinal.x > p2spinaltargetx + 51) {
p2spinal.anims.play('spinalrun', true);
p2spinal.x -= 25 
if (p2spinal.y < p2spinaltargety){
    p2spinal.y += 7;
}
if (p2spinal.y > p2spinaltargety){
    p2spinal.y -= 7;
}
}
if (p2spinal.x < p2spinaltargetx + 52) {
p2spinal.anims.play('spinalbash', true);
p2spinal.x = p2spinaltargetx - 10
p2spinal.y = p2spinaltargety - 55
}
if (p2spinalbashtime < 1){
playerattacking = true
playerbattleattack()
p2spinalidle = true
playerabilityselected = false
p2spinalselected = false
player2selectabilitybox.visible = false
p2spinaltargetselected = false
p2spinal.x = p2spinalx
p2spinal.y = p2spinaly
team1turn = true
team2turn = false
p2spinalattackbash = false

}
}
}
//endspinalbash
//spinalattackspike
if (p2spinalattackspike == true){
p2spinalspiketime -= 0.1
if (p2spinaltargetselected == true){
p2spinalidle = false
if (p2spinal.x > p2spinaltargetx + 51) {
p2spinal.anims.play('spinalrun', true);
p2spinal.x -= 25 
if (p2spinal.y < p2spinaltargety){
    p2spinal.y += 7;
}
if (p2spinal.y > p2spinaltargety){
    p2spinal.y -= 7;
}
}
if (p2spinal.x < p2spinaltargetx + 52) {
p2spinal.anims.play('spinalspike', true);
p2spinal.x = p2spinaltargetx - 10
p2spinal.y = p2spinaltargety - 55
}
if (p2spinalspiketime < 1){
playerattacking = true
playerbattleattack()
p2spinalidle = true
playerabilityselected = false
p2spinalselected = false
player2selectabilitybox.visible = false
p2spinaltargetselected = false
p2spinal.x = p2spinalx
p2spinal.y = p2spinaly
team1turn = true
team2turn = false
p2spinalattackspike = false

}
}
}
//endspinalspike
}
//endspinal2action 

//p1terraaction
//terraaction
function p1terraaction(){
if (p1terraidle == true){
 p1terra.anims.play('terraidle', true);  
}
    if (p1terraattackfire == true){
    if (p1terratargetselected == true){
     p1terraidle = false
    p1terracastnumber -= 0.1;
    p1terra.anims.play('terracasting', true);
    p1terraspells.anims.play('terrafirespell', true);
    p1terraspells.visible = true
    }
    if (p1terracastnumber < 1){
        playerattacking = true
        playerbattleattack()
        p1terraspells.visible = false
        p1terraidle = true
        playerabilityselected = false
        player1selectabilitybox.visible = false
        team2turn = true
        team1turn = false
        p1terraselected = false
        p1terratargetselected = false
        p1terraattackfire = false
    }
    }

    if (p1terraattackice == true){
    if (p1terratargetselected == true){
        p1terraidle = false
    p1terracastnumber -= 0.1
    p1terra.anims.play('terracasting', true);
    p1terraspells.anims.play('terraicespell', true);
    p1terraspells.visible = true
    }
    if (p1terracastnumber < 1){
        playerattacking = true
        playerbattleattack()
        p1terraspells.visible = false
        p1terraidle = true
        playerabilityselected = false
        player1selectabilitybox.visible = false
        team2turn = true
        team1turn = false
        p1terraselected = false
        p1terratargetselected = false
        p1terraattackice = false

    }
    }

    if (p1terraattackwind == true){
    if (p1terratargetselected == true){
    p1terraidle = false
    p1terracastnumber -= 0.1
    p1terra.anims.play('terracasting', true);
    p1terraspells.anims.play('terrawindspell', true);
    p1terraspells.visible = true
    }
    if (p1terracastnumber < 1){
        playerattacking = true
        playerbattleattack()
        p1terraattackwind = false
        p1terraspells.visible = false
        p1terraidle = true
        playerabilityselected = false
        player1selectabilitybox.visible = false
        team2turn = true
        team1turn = false
        p1terraselected = false
        p1terratargetselected = false
        p1terraattackwind = false
    }
    }

    if (p1terraattackearth == true){
    if (p1terratargetselected == true){
        p1terraidle = false
    p1terracastnumber -= 0.1
    p1terra.anims.play('terracasting', true);
    p1terraspells.anims.play('terraearthspell', true);
    p1terraspells.visible = true
    }
    if (p1terracastnumber < 1){
        playerattacking = true
        playerbattleattack()
        p1terraattackearth= false
        p1terraspells.visible = false
        p1terraidle = true
        playerabilityselected = false
        player1selectabilitybox.visible = false
        team2turn = true
        team1turn = false
        p1terraselected = false
        p1terratargetselected = false
        p1terraattackearth = false
    }
    }
}
//p2terraaction
//terraaction
function p2terraaction(){
if (p2terraidle == true){
 p2terra.anims.play('terraidle', true);  
}
    if (p2terraattackfire == true){
    if (p2terratargetselected == true){
     p2terraidle = false
    p2terracastnumber -= 0.1;
    p2terra.anims.play('terracasting', true);
    p2terraspells.anims.play('terrafirespell', true);
    p2terraspells.visible = true
    }
    if (p2terracastnumber < 1){
        playerattacking = true
        playerbattleattack()
        p2terraspells.visible = false
        p2terraidle = true
        playerabilityselected = false
        player2selectabilitybox.visible = false
        team1turn = true
        team2turn = false
        p2terraselected = false
        p2terratargetselected = false
        p2terraattackfire = false
    }
    }

    if (p2terraattackice == true){
    if (p2terratargetselected == true){
        p2terraidle = false
    p2terracastnumber -= 0.1
    p2terra.anims.play('terracasting', true);
    p2terraspells.anims.play('terraicespell', true);
    p2terraspells.visible = true
    }
    if (p2terracastnumber < 1){
        playerattacking = true
        playerbattleattack()
        p2terraspells.visible = false
        p2terraidle = true
        playerabilityselected = false
        player2selectabilitybox.visible = false
        team1turn = true
        team2turn = false
        p2terraselected = false
        p2terratargetselected = false
        p2terraattackice = false

    }
    }

    if (p2terraattackwind == true){
    if (p2terratargetselected == true){
    p2terraidle = false
    p2terracastnumber -= 0.1
    p2terra.anims.play('terracasting', true);
    p2terraspells.anims.play('terrawindspell', true);
    p2terraspells.visible = true
    }
    if (p2terracastnumber < 1){
        playerattacking = true
        playerbattleattack()
        p2terraattackwind = false
        p2terraspells.visible = false
        p2terraidle = true
        playerabilityselected = false
        player2selectabilitybox.visible = false
        team1turn = true
        team2turn = false
        p2terraselected = false
        p2terratargetselected = false
        p2terraattackwind = false
    }
    }

    if (p2terraattackearth == true){
    if (p2terratargetselected == true){
        p2terraidle = false
    p2terracastnumber -= 0.1
    p2terra.anims.play('terracasting', true);
    p2terraspells.anims.play('terraearthspell', true);
    p2terraspells.visible = true
    }
    if (p2terracastnumber < 1){
        playerattacking = true
        playerbattleattack()
        p2terraattackearth= false
        p2terraspells.visible = false
        p2terraidle = true
        playerabilityselected = false
        player2selectabilitybox.visible = false
        team1turn = true
        team2turn = false
        p2terraselected = false
        p2terratargetselected = false
        p2terraattackearth = false
    }
    }
}

//kanoaction
//p1kanoaction
function p1kanoaction(){
if (p1kanoidle == true){
 p1kano.anims.play('kanoidle', true);  
}
if (p1kanoattackblades == true){
p1kanoidle = false
if (p1kanotargetselected == true){
if (p1kano.x < p1kanotargetx) {
p1kano.anims.play('kanoblades', true); 
p1kanoblades.visible = true
p1kanoblades.anims.play('kanobladesgr', true);
p1kanoblades.x += 45 
if (p1kanobladessoundplayed == false){
        game.sound.add( "kanoblade" ).play()
        p1kanobladessoundplayed = true
    }

if (p1kanobladesvoiceplayed == false){
        game.sound.add( "kanobladevoice" ).play()
        p1kanobladesvoiceplayed = true
    }

if (p1kanoblades.y < p1kanotargety){
    p1kanoblades.y += 7;
}
if (p1kanoblades.y > p1kanotargety){
    p1kanoblades.y -= 7;
}
if (p1kanoblades.x > p1kanotargetx - 25) {
playerattacking = true
playerbattleattack()
p1kanoidle = true
playerabilityselected = false
p1kanoselected = false
player1selectabilitybox.visible = false
p1kanotargetselected = false
p1kanoblades.x = p1kanox
p1kanoblades.y = p1kanoy
team2turn = true
team1turn = false
p1kanoattackblades = false

}
}
}
}

if (p1kanoattackcut == true){
if (p1kanotargetselected == true){
p1kanocuttime -= 0.2;
p1kanoidle = false
if (p1kano.x < p1kanotargetx) {
p1kano.anims.play('kanorun', true);
p1kano.x += 25
if (p1kanocutvoiceplayed == false){
        game.sound.add( "kanocutvoice" ).play()
        p1kanocutvoiceplayed = true
    }


}
if (p1kano.y > p1kanotargety){
    p1kano.y -= 7;
}
if (p1kano.y < p1kanotargety){
    p1kano.y += 7;
}
if (p1kano.x > p1kanotargetx - 1) {
p1kano.anims.play('kanocut', true);

}

if (p1kanocuttime < 0) {
playerattacking = true
playerbattleattack()
p1kanoidle = true
playerabilityselected = false
p1kanoselected = false
player1selectabilitybox.visible = false
p1kano.x = p1kanox
p1kano.y = p1kanoy
team2turn = true
team1turn = false
p1kanotargetselected = false
p1kanoattackcut = false
}
}
}
if (p1kanoattacklaser == true){
if (p1kanotargetselected == true){
p1kanolasertime -= 0.2;
p1kanoidle = false
if (p1kano.x < p1kanotargetx - 100) {
p1kano.anims.play('kanorun', true);
p1kano.x += 25 
if (p1kano.x > p1kanotargetx - 100) {
p1kano.anims.play('kanolaser', true);
p1kanolaser.x = p1kano.x + 40
p1kanolaser.y = p1kano.y + 25
p1kanolasersmoke.x = p1kano.x + 115
p1kanolasersmoke.y = p1kano.y + 10
p1kanolaser.visible = true
p1kanolasersmoke.visible = true
p1kanolaser.anims.play('kanolasergr', true);
p1kanolasersmoke.anims.play('kanolasersmokegr', true);
}
}
if (p1kanolasertime < 1){
playerattacking = true
playerbattleattack()
p1kanoidle = true
playerabilityselected = false
p1kanoselected = false
player1selectabilitybox.visible = false
p1kanolaser.visible = false
p1kanolasersmoke.visible = false
p1kano.x = p1kanox
p1kano.y = p1kanoy
team2turn = true
team1turn = false
p1kanotargetselected = false
p1kanoattacklaser = false   
}
}
}

if (p1kanoattackhell == true){
if (p1kanotargetselected == true){
p1kanohelltime -= 0.2;
p1kanoidle = false
if (p1kano.x < p1kanotargetx - 90) {
p1kano.anims.play('kanorun', true);
p1kano.x += 25
if (p1kanohellvoiceplayed == false){
        game.sound.add( "kanohellvoice" ).play()
        p1kanohellvoiceplayed = true
    }
if (p1kano.x > p1kanotargetx - 90) {
p1kano.anims.play('kanoidle', true);
p1kanohellhead.x = p1kano.x + 9
p1kanohellhead.y = p1kano.y - 6 
p1kanohellhead.anims.play('kanohell', true);
p1kanohellheadfire.x = p1kano.x + 14
p1kanohellheadfire.y = p1kano.y - 6
p1kanohellhead.setDepth(5);
p1kanohellheadfire.setDepth(5);
p1kanohellhead.visible = true
p1kanohellheadfire.visible = true
p1kanohellheadfire.anims.play('kanohellheadbreathgr', true);
}
}
if (p1kanohelltime < 1){
playerattacking = true
playerbattleattack()
p1kanoidle = true
playerabilityselected = false
p1kanoselected = false
player1selectabilitybox.visible = false
p1kanohellhead.visible = false
p1kanohellheadfire.visible = false
p1kano.x = p1kanox
p1kano.y = p1kanoy
team2turn = true
team1turn = false
p1kanotargetselected = false
p1kanoattackhell= false   
}
}
}

if (p1kanoattackkick == true){
if (p1kanotargetselected == true){
p1kanokicktime -= 0.2;
p1kanoidle = false
if (p1kano.x < p1kanotargetx) {
p1kano.anims.play('kanorun', true);
p1kano.x += 25 
if (p1kanokickvoiceplayed == false){
        game.sound.add( "kanokickvoice" ).play()
        p1kanokickvoiceplayed = true
    }
if (p1kano.x > p1kanotargetx - 1) {
p1kano.anims.play('kanokick', true);
}
}
if (p1kanokicktime < 1){
playerattacking = true
playerbattleattack()
p1kanoidle = true
playerabilityselected = false
p1kanoselected = false
player1selectabilitybox.visible = false
p1kano.x = p1kanox
p1kano.y = p1kanoy
team2turn = true
team1turn = false
p1kanotargetselected = false
p1kanoattackkick= false   
}
}
}
}

//kanoaction
//p2kanoaction
function p2kanoaction(){
if (p2kanoidle == true){
 p2kano.anims.play('kanoidle', true);  
}
if (p2kanoattackblades == true){
p2kanoidle = false
if (p2kanotargetselected == true){
if (p2kano.x > p2kanotargetx) {
p2kano.anims.play('kanoblades', true); 
p2kanoblades.visible = true
p2kanoblades.anims.play('kanobladesgr', true);
p2kanoblades.x -= 45 
if (p2kanobladessoundplayed == false){
        game.sound.add( "kanoblade" ).play()
        p2kanobladessoundplayed = true
    }

if (p2kanobladesvoiceplayed == false){
        game.sound.add( "kanobladevoice" ).play()
        p2kanobladesvoiceplayed = true
    }

if (p2kanoblades.y < p2kanotargety){
    p2kanoblades.y += 7;
}
if (p2kanoblades.y > p2kanotargety){
    p2kanoblades.y -= 7;
}
if (p2kanoblades.x < p2kanotargetx - 25) {
playerattacking = true
playerbattleattack()
p2kanoidle = true
playerabilityselected = false
p2kanoselected = false
player2selectabilitybox.visible = false
p2kanotargetselected = false
p2kanoblades.x = p2kanox
p2kanoblades.y = p2kanoy
team1turn = true
team2turn = false
p2kanoattackblades = false

}
}
}
}

if (p2kanoattackcut == true){
if (p2kanotargetselected == true){
p2kanocuttime -= 0.2;
p2kanoidle = false
if (p2kano.x > p2kanotargetx) {
p2kano.anims.play('kanorun', true);
p2kano.x -= 25 
if (p2kanocutvoiceplayed == false){
        game.sound.add( "kanocutvoice" ).play()
        p2kanocutvoiceplayed = true
    }


}
if (p2kano.y > p2kanotargety){
    p2kano.y -= 7;
}
if (p2kano.y < p2kanotargety){
    p2kano.y += 7;
}
if (p2kano.x < p2kanotargetx + 30) {
p2kano.anims.play('kanocut', true);
}

if (p2kanocuttime < 0) {
playerattacking = true
playerbattleattack()
p2kanoidle = true
playerabilityselected = false
p2kanoselected = false
player2selectabilitybox.visible = false
p2kano.x = p2kanox
p2kano.y = p2kanoy
team1turn = true
team2turn = false
p2kanotargetselected = false
p2kanoattackcut = false
}
}
}
if (p2kanoattacklaser == true){
if (p2kanotargetselected == true){
p2kanolasertime -= 0.2;
p2kanoidle = false
if (p2kano.x > p2kanotargetx + 119) {
p2kano.anims.play('kanorun', true);
p2kano.x -= 25 
if (p2kano.x < p2kanotargetx + 120) {
p2kano.anims.play('kanolaser', true);
p2kanolaser.x = p2kano.x - 65
p2kanolaser.y = p2kano.y + 25
p2kanolasersmoke.x = p2kano.x - 85
p2kanolasersmoke.y = p2kano.y + 10
p2kanolaser.visible = true
p2kanolasersmoke.visible = true
p2kanolaser.anims.play('kanolasergr', true);
p2kanolasersmoke.anims.play('kanolasersmokegr', true);
}
}
if (p2kanolasertime < 1){
playerattacking = true
playerbattleattack()
p2kanoidle = true
playerabilityselected = false
p2kanoselected = false
player2selectabilitybox.visible = false
p2kanolaser.visible = false
p2kanolasersmoke.visible = false
p2kano.x = p2kanox
p2kano.y = p2kanoy
team1turn = true
team2turn = false
p2kanotargetselected = false
p2kanoattacklaser = false   
}
}
}

if (p2kanoattackhell == true){
if (p2kanotargetselected == true){
p2kanohelltime -= 0.2;
p2kanoidle = false
if (p2kano.x > p2kanotargetx + 109) {
p2kano.anims.play('kanorun', true);
p2kano.x -= 25 
if (p2kanohellvoiceplayed == false){
        game.sound.add( "kanohellvoice" ).play()
        p2kanohellvoiceplayed = true
    }
if (p2kano.x < p2kanotargetx + 110) {
p2kano.anims.play('kanoidle', true);
p2kanohellhead.x = p2kano.x + 25
p2kanohellhead.y = p2kano.y - 6 
p2kanohellhead.anims.play('kanohell', true);
p2kanohellheadfire.x = p2kano.x - 55
p2kanohellheadfire.y = p2kano.y - 6
p2kanohellhead.setDepth(5);
p2kanohellheadfire.setDepth(5);
p2kanohellhead.visible = true
p2kanohellheadfire.visible = true
p2kanohellheadfire.anims.play('kanohellheadbreathgr', true);
}
}
if (p2kanohelltime < 1){
playerattacking = true
playerbattleattack()
p2kanoidle = true
playerabilityselected = false
p2kanoselected = false
player2selectabilitybox.visible = false
p2kanohellhead.visible = false
p2kanohellheadfire.visible = false
p2kano.x = p2kanox
p2kano.y = p2kanoy
team1turn = true
team2turn = false
p2kanotargetselected = false
p2kanoattackhell= false   
}
}
}

if (p2kanoattackkick == true){
if (p2kanotargetselected == true){
p2kanokicktime -= 0.2;
p2kanoidle = false
if (p2kano.x > p2kanotargetx + 50) {

p2kano.anims.play('kanorun', true);
p2kano.x -= 25 
if (p2kanokickvoiceplayed == false){
        game.sound.add( "kanokickvoice" ).play()
        p2kanokickvoiceplayed = true
    }
if (p2kano.x < p2kanotargetx + 51) {
p2kano.anims.play('kanokick', true);

}
}
if (p2kanokicktime < 1){
playerattacking = true
playerbattleattack()
p2kanoidle = true
playerabilityselected = false
p2kanoselected = false
player2selectabilitybox.visible = false
p2kano.x = p2kanox
p2kano.y = p2kanoy
team1turn = true
team2turn = false
p2kanotargetselected = false
p2kanoattackkick= false   
}
}
}
}
//endp2kanoaction

//p1jagoaction
//jagoaction
function p1jagoaction(){
if (p1jagoidle == true){
p1jago.anims.play('jagoidle', true);  
}

if (p1jagoattacksword == true){
if (p1jagotargetselected == true){
p1jagoidle = false
p1jagoswordtime -= 0.2;
if (p1jago.x < p1jagotargetx) {
p1jago.anims.play('jagowalk', true);
p1jago.x += 25
if (p1jago.x > p1jagotargetx - 1) {
p1jago.anims.play('jagosword', true);
}
}

if (p1jagoswordtime < 1){
playerattacking = true
playerbattleattack()
p1jagoidle = true
playerabilityselected = false
p1jagoselected = false
player1selectabilitybox.visible = false
p1jago.x = p1jagox
p1jago.y = p1jagoy
team2turn = true
team1turn = false
p1jagotargetselected = false
p1jagoattacksword = false  
}
}
}
if (p1jagoattackplasma == true){
if (p1jagotargetselected == true){
p1jagoidle = false
if (p1jago.x < p1jagotargetx) {
p1jago.anims.play('jagoplasma', true);
p1jagoplasma.visible = true
p1jagoplasma.anims.play('jagoplasmagr', true);
p1jagoplasma.x += 45;
if (p1jagoplasma.x > p1jagotargetx + 25) {
playerattacking = true
playerbattleattack()
p1jagoidle = true
playerabilityselected = false
p1jagoselected = false
player1selectabilitybox.visible = false
p1jagoplasma.x = p1jagox
p1jagoplasma.y = p1jagoy
p1jagoplasma.visible = false
team2turn = true
team1turn = false
p1jagotargetselected = false
p1jagoattackplasma = false
}
}
}
}
if (p1jagoattackrkick == true){
if (p1jagotargetselected == true){
p1jagoidle = false
p1jagorkicktime -= 0.2;
if (p1jago.x < p1jagotargetx) {
p1jago.anims.play('jagowalk', true);
p1jago.x += 25
}
if (p1jago.x > p1jagotargetx - 1) {
p1jago.anims.play('jagorkick', true);
}
if (p1jagorkicktime < 1){
playerattacking = true
playerbattleattack()
p1jagoidle = true
playerabilityselected = false
p1jagoselected = false
player1selectabilitybox.visible = false
p1jago.x = p1jagox
p1jago.y = p1jagoy
team2turn = true
team1turn = false
p1jagotargetselected = false
p1jagoattackrkick = false
}
}
}

if (p1jagoattackfocus == true){
p1jagoidle = false
p1jagofocustime -= 0.2;
if (p1jago.x <  + 499 ) {
p1jago.anims.play('jagowalk', true);
p1jago.x += 25
}
if (p1jago.x >  + 499) {
p1jago.anims.play('jagofocus', true);
}
if (p1jagofocustime < 1){
playerattacking = true
playerbattleattack()
p1jagoidle = true
playerabilityselected = false
p1jagoselected = false
player1selectabilitybox.visible = false
p1jago.x = p1jagox
p1jago.y = p1jagoy
team2turn = true
team1turn = false
p1jagotargetselected = false
p1jagoattackfocus = false
}
}


if (p1jagoattackstab == true){
if (p1jagotargetselected == true){
p1jagoidle = false
p1jagostabtime -= 0.2;
if (p1jago.x < p1jagotargetx -100) {
p1jago.anims.play('jagowalk', true);
p1jago.x += 25
}
if (p1jago.x > p1jagotargetx - 101) {
p1jago.anims.play('jagostab', true);
}
if (p1jagostabtime < 1){
playerattacking = true
playerbattleattack()
p1jagoidle = true
playerabilityselected = false
p1jagoselected = false
player1selectabilitybox.visible = false
p1jago.x = p1jagox
p1jago.y = p1jagoy
team2turn = true
team1turn = false
p1jagotargetselected = false
p1jagoattackstab = false

}
}
}
}
//endp1jagoaction

//p2jagoaction
function p2jagoaction(){
