}
if (p2jagoattacksword == true){
if (p2jagotargetselected == true){
p2jagoidle = false
p2jagoswordtime -= 0.2;
if (p2jago.x > p2jagotargetx) {
p2jago.anims.play('jagowalk', true);
p2jago.x -= 25
if (p2jago.x < p2jagotargetx + 1) {
p2jago.anims.play('jagosword', true);
}
}

if (p2jagoswordtime < 1){
playerattacking = true
