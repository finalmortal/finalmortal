//aipicktarget
function aipicktarget(){
 if (aitargetchosen == false){
 chooseaitarget = Math.floor(Math.random() * 6);
 aitargetchosen = true
 }

    if (chooseaitarget == 1 ){
     if (p2cecilselectingtarget == true){
        aitargets()
        p2ceciltargetselected = true
        setenemyposition()
        removetint()
    }
    }

    if (chooseaitarget == 2 ){
     if (p2cecilselectingtarget == true){
        aitargets()
        p2ceciltargetselected = true
        setenemyposition()
