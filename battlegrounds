p1glacius.x += 25 
if (p1glacius.y > p1glaciustargety){
    p1glacius.y -= 7;
}
if (p1glacius.y < p1glaciustargety){
    p1glacius.y += 7;
}
}
if (p1glacius.x > p1glaciustargetx - 110) {
p1glacius.anims.play('glaciusicespike', true);
}
