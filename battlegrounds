}
//spinalattackslice
if (p1spinalattackslice == true){
p1spinalslicetime -= 0.1
if (p1spinaltargetselected == true){
    if (p1spinalslicesoundplayed == false){
        game.sound.add( "spinalslice" ).play()
        p1spinalslicesoundplayed = true
    }
p1spinalidle = false
if (p1spinal.x < p1spinaltargetx - 51) {
p1spinal.anims.play('spinalrun', true);
p1spinal.x += 25 
if (p1spinal.y < p1spinaltargety){
    p1spinal.y += 7;
}
if (p1spinal.y > p1spinaltargety){
    p1spinal.y -= 7;
}
}
if (p1spinal.x > p1spinaltargetx - 50) {
    //swordslashhere
p1spinal.anims.play('spinalslice', true);
p1spinal.x -= 15
p1spinal.y -= 35
}
if (p1spinalslicetime < 1){
playerattacking = true
playerbattleattack()
p1spinalidle = true
playerabilityselected = false
p1spinalselected = false
player1selectabilitybox.visible = false
p1spinaltargetselected = false
p1spinal.x = p1spinalx
p1spinal.y = p1spinaly
team2turn = true
team1turn = false
p1spinalslicesoundplayed = false
p1spinalattackslice = false

}
}
}
//endspinalslice

//spinalattackslash
