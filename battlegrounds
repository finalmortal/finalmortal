//p1cyraxaction
function p1cyraxaction(){
    if (p1cyraxidle == true){
        p1cyrax.anims.play('cyraxidle', true);
    }
  if (p1cyraxattacknet == true){
if (p1cyraxtargetselected == true){
p1cyraxidle = false
if (p1cyrax.x < p1cyraxtargetx) {
p1cyraxnet.visible = true
p1cyrax.anims.play('cyraxbomb', true);
p1cyraxnet.anims.play('cyraxnet', true);
if (p1cyraxnetsoundplayed == false){
        game.sound.add( 'cyraxnetsound' ).play()
        p1cyraxnetsoundplayed = true
    }

p1cyraxnet.x += 35
if (p1cyraxnet.y > p1cyraxtargety){
    p1cyraxnet.y -= 7;
}
if (p1cyraxnet.y < p1cyraxtargety){
    p1cyraxnet.y += 7;
}
}

if (p1cyraxnet.x > p1cyraxtargetx -1 ) {
playerattacking = true
playerbattleattack()
p1cyraxidle = true
playerabilityselected = false
p1cyraxselected = false
p1cyraxnet.visible = false
p1cyraxnet.x = p1cyrax.x
p1cyraxnet.y = p1cyrax.y 
player1selectabilitybox.visible = false
p1cyrax.x = p1cyraxx
p1cyrax.y = p1cyraxy
team2turn = true
team1turn = false
p1cyraxtargetselected = false
p1cyraxattacknet = false
}
}
}

if (p1cyraxattackchopper == true){
if (p1cyraxtargetselected == true){
p1cyraxidle = false
p1cyraxchoppertime -= 0.2;
if (p1cyrax.x < p1cyraxtargetx) {
p1cyrax.anims.play('cyraxchopper', true);
p1cyrax.x += 45 
if (p1cyrax.y > p1cyraxtargety){
    p1cyrax.y -= 7;
}
if (p1cyrax.y < p1cyraxtargety){
    p1cyrax.y += 7;
}
}
if (p1cyrax.x > p1cyraxtargetx ) {
p1cyrax.anims.play('cyraxchopper', true);
p1cyrax.x = p1cyraxtargetx
p1cyrax.setDepth(7);
}
if (p1cyraxchoppertime < 1){
playerattacking = true
playerbattleattack()
p1cyraxidle = true
playerabilityselected = false
p1cyraxselected = false
player1selectabilitybox.visible = false
p1cyrax.x = p1cyraxx
p1cyrax.y = p1cyraxy
team2turn = true
team1turn = false
setdepth()
p1cyraxtargetselected = false
p1cyraxattackchopper = false
}
}
}

if (p1cyraxattackbomb == true){
if (p1cyraxtargetselected == true){
p1cyraxidle = false
if (p1cyrax.x <  600) {
p1cyrax.anims.play('cyraxwalk', true);
p1cyrax.x += 25 
p1cyraxbombgr.x = p1cyrax.x

}
if (p1cyrax.x >  601) {
p1cyrax.anims.play('cyraxbomb', true);
p1cyraxbombgr.visible = true
p1cyraxbombgr.anims.play('cyraxbombgr', true);
if (p1cyraxbombgr.x < p1cyraxtargetx + 20){
p1cyraxbombgr.x += 25
p1cyraxbombexplodetime = 5
if (p1cyraxbombgr.y > p1cyraxtargety + 65){
    p1cyraxbombgr.y -= 7;
}
if (p1cyraxbombgr.y < p1cyraxtargety + 65){
    p1cyraxbombgr.y += 7;
}
}
if (p1cyraxbombgr.x > p1cyraxtargetx - 1){
p1cyraxbombexplodetime -= 0.4
}
if (p1cyraxbombexplodetime < 1){
p1cyraxbombexplodegr.x = p1cyraxbombgr.x - 25
p1cyraxbombexplodegr.y = p1cyraxbombgr.y - 50
p1cyraxbombexplodegr.visible = true
p1cyraxbombexplodegr.anims.play('cyraxbombexplodegr', true);
p1cyraxbombtime -= 0.5
}
}

if (p1cyraxbombtime < 1 ){
playerattacking = true
playerbattleattack()
p1cyraxidle = true
playerabilityselected = false
p1cyraxselected = false
player1selectabilitybox.visible = false
p1cyraxbombgr.visible = false
p1cyraxbombexplodegr.visible = false
p1cyraxbombgr.x = p1cyraxx
p1cyraxbombgr.y = p1cyraxy
p1cyrax.x = p1cyraxx
p1cyrax.y = p1cyraxy
team2turn = true
team1turn = false
p1cyraxtargetselected = false
p1cyraxattackbomb = false
}
}
}

if (p1cyraxattackshark == true){
if (p1cyraxtargetselected == true){
p1cyraxidle = false
p1cyraxsharktime -= 0.1;
if (p1cyrax.x < p1cyraxtargetx) {
p1cyrax.anims.play('cyraxshark', true);
if (p1cyraxsharksoundplayed == false){
        game.sound.add( "cyraxsharksound" ).play()
        p1cyraxsharksoundplayed = true
    }
p1cyrax.x += 35 
p1cyrax.setDepth(7);
if (p1cyrax.y > p1cyraxtargety){
    p1cyrax.y -= 7;
}
if (p1cyrax.y < p1cyraxtargety){
    p1cyrax.y += 7;
}
}
if (p1cyrax.x > p1cyraxtargetx ) {
p1cyrax.anims.play('cyraxshark', true);
p1cyrax.x = p1cyraxtargetx

}
if (p1cyraxsharktime < 1){
playerattacking = true
playerbattleattack()
p1cyraxidle = true
playerabilityselected = false
p1cyraxselected = false
player1selectabilitybox.visible = false
p1cyrax.x = p1cyraxx
p1cyrax.y = p1cyraxy
team2turn = true
team1turn = false
setdepth()
p1cyraxtargetselected = false
p1cyraxattackshark = false
}
}
}

if (p1cyraxattackpowerup == true){
p1cyraxidle = false
p1cyraxpoweruptime -= 0.1;
if (p1cyrax.x <  600) {
p1cyrax.anims.play('cyraxwalk', true);
p1cyrax.x += 25 
}
if (p1cyrax.x >  601) {
if (p1cyraxpowerupvoiceplayed == false){
        game.sound.add( "cyraxpowerupvoice" ).play()
        p1cyraxpowerupvoiceplayed = true
    }
  

p1cyrax.anims.play('cyraxpowerup', true);
}
if (p1cyraxpoweruptime < 1 ){
