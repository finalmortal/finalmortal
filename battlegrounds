p1spinalselected = false
player1selectabilitybox.visible = false
p1spinaltargetselected = false
p1spinal.x = p1spinalx
p1spinal.y = p1spinaly
team2turn = true
team1turn = false
p1spinalslashsoundplayed = true
p1spinalattackslash = false

}
}
}
//endspinalslash
//spinalattackbash
if (p1spinalattackbash == true){
p1spinalbashtime -= 0.1
if (p1spinaltargetselected == true){
