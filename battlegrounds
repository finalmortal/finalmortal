//
}

//p2mogspells
function p2mogspells(){
//p2mogmoon
if (p2mogmoonaura == true){
if (p2mogmoonauratime < 1){
p2mogmoonstatdown = true
}
}
if (p2mogmoonaura == true){
if (p2mogmoonstatup == false){
p2rosamaxhealthrating += p2mogmoonstat
p2rosahealthrating += p2mogmoonstat
p2mogmaxhealthrating += p2mogmoonstat
p2moghealthrating += p2mogmoonstat
p2sindelmaxhealthrating += p2mogmoonstat
p2sindelhealthrating += p2mogmoonstat
p2sonjamaxhealthrating += p2mogmoonstat
p2sonjahealthrating += p2mogmoonstat
p2raidenmaxhealthrating += p2mogmoonstat
p2raidenhealthrating += p2mogmoonstat    
p2sektormaxhealthrating += p2mogmoonstat
p2sektorhealthrating += p2mogmoonstat
p2glaciusmaxhealthrating += p2mogmoonstat
p2glaciushealthrating += p2mogmoonstat
p2kunglaomaxhealthrating += p2mogmoonstat
p2kunglaohealthrating += p2mogmoonstat
p2combomaxhealthrating += p2mogmoonstat
p2combohealthrating += p2mogmoonstat
p2kanomaxhealthrating += p2mogmoonstat
p2kanohealthrating += p2mogmoonstat
p2terramaxhealthrating += p2mogmoonstat
p2terrahealthrating += p2mogmoonstat
p2spinalmaxhealthrating += p2mogmoonstat
p2spinalhealthrating += p2mogmoonstat
p2kangmaxhealthrating += p2mogmoonstat
p2kanghealthrating += p2mogmoonstat
p2kainmaxhealthrating += p2mogmoonstat
p2kainhealthrating += p2mogmoonstat
p2cecilmaxhealthrating += p2mogmoonstat
p2cecilhealthrating += p2mogmoonstat
p2orchidmaxhealthrating += p2mogmoonstat
p2orchidhealthrating += p2mogmoonstat
p2cyraxmaxhealthrating += p2mogmoonstat
p2cyraxhealthrating += p2mogmoonstat
p2golbezmaxhealthrating += p2mogmoonstat
p2golbezhealthrating += p2mogmoonstat
p2jagomaxhealthrating += p2mogmoonstat
p2jagohealthrating += p2mogmoonstat
p2cyanmaxhealthrating += p2mogmoonstat
p2cyanhealthrating += p2mogmoonstat
p2mogmoonstatup = true
}
if (p2mogmoonstatdown == true){
p2rosamaxhealthrating -= p2mogmoonstat
p2rosahealthrating -= p2mogmoonstat
p2mogmaxhealthrating -= p2mogmoonstat
p2moghealthrating -= p2mogmoonstat
p2sindelmaxhealthrating -= p2mogmoonstat
p2sindelhealthrating -= p2mogmoonstat
p2sonjamaxhealthrating -= p2mogmoonstat
p2sonjahealthrating -= p2mogmoonstat
p2raidenmaxhealthrating -= p2mogmoonstat
p2raidenhealthrating -= p2mogmoonstat    
p2sektormaxhealthrating -= p2mogmoonstat
p2sektorhealthrating -= p2mogmoonstat
p2glaciusmaxhealthrating -= p2mogmoonstat
p2glaciushealthrating -= p2mogmoonstat
p2kunglaomaxhealthrating -= p2mogmoonstat
p2kunglaohealthrating -= p2mogmoonstat
p2combomaxhealthrating -= p2mogmoonstat
p2combohealthrating -= p2mogmoonstat
p2kanomaxhealthrating -= p2mogmoonstat
p2kanohealthrating -= p2mogmoonstat
p2terramaxhealthrating -= p2mogmoonstat
p2terrahealthrating -= p2mogmoonstat
p2spinalmaxhealthrating -= p2mogmoonstat
p2spinalhealthrating -= p2mogmoonstat
p2kangmaxhealthrating -= p2mogmoonstat
p2kanghealthrating -= p2mogmoonstat
p2kainmaxhealthrating -= p2mogmoonstat
p2kainhealthrating -= p2mogmoonstat
p2cecilmaxhealthrating -= p2mogmoonstat
p2cecilhealthrating -= p2mogmoonstat
p2orchidmaxhealthrating -= p2mogmoonstat
p2orchidhealthrating -= p2mogmoonstat
