//p1comboaction
function p1comboaction(){
if (p1comboidle == true){
p1combo.anims.play('comboidle', true);
}

if (p1comboattackpunch == true){
if (p1combotargetselected == true){
p1comboidle = false
p1combopunchtime -= 0.2
if (p1combo.x < p1combotargetx - 100) {
p1combo.anims.play('comborun', true);
if (p1combopunchvoiceplayed == false){
        game.sound.add( "combopunchvoice" ).play()
        p1combopunchvoiceplayed = true
    }
p1combo.x += 25
if (p1combo.y > p1combotargety){
    p1combo.y -= 7;
}
if (p1combo.y < p1combotargety){
    p1combo.y += 7;
}
}
if (p1combo.x > p1combotargetx - 101) {
p1combo.anims.play('combopunch', true);
}
if (p1combopunchtime < 1){
playerattacking = true
playerbattleattack()
p1comboidle = true
playerabilityselected = false
p1comboselected = false
player1selectabilitybox.visible = false
p1combo.x = p1combox
p1combo.y = p1comboy
team2turn = true
team1turn = false
p1combotargetselected = false
p1comboattackpunch = false

}
}
}

if (p1comboattackjumpkick == true){
if (p1combotargetselected == true){
p1comboidle = false
p1combojumpkicktime -= 0.2
if (p1combo.x < p1combotargetx) {
p1combo.anims.play('comborun', true);
if (p1combojumpkickvoiceplayed == false){
        game.sound.add( "combojumpkickvoice" ).play()
        p1combojumpkickvoiceplayed = true
    }
p1combo.x += 25
}
if (p1combo.x > p1combotargetx - 1) {
p1combo.anims.play('combojumpkick', true);
}
if (p1combojumpkicktime < 1){
playerattacking = true
playerbattleattack()
p1comboidle = true
playerabilityselected = false
p1comboselected = false
player1selectabilitybox.visible = false
p1combo.x = p1combox
p1combo.y = p1comboy
team2turn = true
team1turn = false
p1combotargetselected = false
p1comboattackjumpkick = false
}
}
}
if (p1comboattackgun == true){
if (p1combotargetselected == true){
p1comboidle = false
p1comboguntime -= 0.2
if (p1combo.x < p1combotargetx - 500) {
p1combo.anims.play('comborun', true);
p1combo.x += 25 
}
if (p1combo.x > p1combotargetx - 501) {
if (p1combogunvoiceplayed == false){
        game.sound.add( "combogunvoice" ).play()
        p1combogunvoiceplayed = true
    }
    if (p1combogunsoundplayed == false){
        game.sound.add( "combogunsound" ).play()
        p1combogunsoundplayed = true
    }
p1combo.anims.play('combogun', true);
}
if (p1comboguntime < 1){
playerattacking = true
playerbattleattack()
p1comboidle = true
playerabilityselected = false
p1comboselected = false
player1selectabilitybox.visible = false
p1combo.x = p1combox
p1combo.y = p1comboy
team2turn = true
team1turn = false
p1combotargetselected = false
p1comboattackgun = false

}
}
}
if (p1comboattackslam == true){
if (p1combotargetselected == true){
p1comboidle = false
p1comboslamtime -= 0.2;
if (p1combo.x < p1combotargetx -50) {
p1combo.anims.play('comborun', true);
if (p1comboslamvoiceplayed == false){
        game.sound.add( "comboslamvoice" ).play()
        p1comboslamvoiceplayed = true
    }
p1combo.x += 25
}
if (p1combo.x > p1combotargetx - 51) {
p1combo.anims.play('comboslam', true);
}
if (p1comboslamtime < 0){
playerattacking = true
playerbattleattack()
p1comboidle = true
playerabilityselected = false
p1comboselected = false
player1selectabilitybox.visible = false
p1combo.x = p1combox
p1combo.y = p1comboy
team2turn = true
team1turn = false
p1combotargetselected = false
p1comboattackslam= false
}
}
}
if (p1comboattackmuscle == true){
p1comboidle = false
p1combomuscletime -= 0.2;
if (p1combo.x <  600) {
p1combo.anims.play('comborun', true);
p1combo.x += 25 
}
if (p1combo.x >  601) {
    if (p1combomusclevoiceplayed == false){
        game.sound.add( "combomusclevoice" ).play()
        p1combomusclevoiceplayed = true
    }
p1combo.anims.play('combomuscle', true);
}
if (p1combomuscletime < 1 ){
playerattacking = true
playerbattleattack()
p1comboidle = true
playerabilityselected = false
p1comboselected = false
player1selectabilitybox.visible = false
p1combo.x = p1combox
p1combo.y = p1comboy
team2turn = true
team1turn = false
p1combotargetselected = false
p1comboattackmuscle = false
}
}
//
}
//endp1combo
//p2comboaction
function p2comboaction(){
if (p2comboidle == true){
p2combo.anims.play('comboidle', true);
}

if (p2comboattackpunch == true){
if (p2combotargetselected == true){
p2comboidle = false
p2combopunchtime -= 0.2
if (p2combo.x > p2combotargetx + 140) {
p2combo.anims.play('comborun', true);
if (p2combopunchvoiceplayed == false){
        game.sound.add( "combopunchvoice" ).play()
        p2combopunchvoiceplayed = true
    }

p2combo.x -= 25
if (p2combo.y > p2combotargety){
    p2combo.y -= 7;
}
if (p2combo.y < p2combotargety){
    p2combo.y += 7;
}
}
if (p2combo.x < p2combotargetx - 141) {
p2combo.anims.play('combopunch', true);
}
if (p2combopunchtime < 1){
playerattacking = true
playerbattleattack()
p2comboidle = true
playerabilityselected = false
p2comboselected = false
player2selectabilitybox.visible = false
p2combo.x = p2combox
p2combo.y = p2comboy
team1turn = true
team2turn = false
p2combotargetselected = false
p2comboattackpunch = false

}
}
}

if (p2comboattackjumpkick == true){
if (p2combotargetselected == true){
p2comboidle = false
p2combojumpkicktime -= 0.2
if (p2combo.x > p2combotargetx) {
p2combo.anims.play('comborun', true);
if (p2combojumpkickvoiceplayed == false){
        game.sound.add( "combojumpkickvoice" ).play()
        p2combojumpkickvoiceplayed = true
    }
p2combo.x -= 25
}
if (p2combo.x < p2combotargetx + 1) {
p2combo.anims.play('combojumpkick', true);
}
if (p2combojumpkicktime < 1){
playerattacking = true
playerbattleattack()
p2comboidle = true
playerabilityselected = false
p2comboselected = false
