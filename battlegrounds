//startbuttonclicks
//cecilbuttonclick
function p1cecilbuttonclick(){

    if (team1turn == true){
    if (p1rosahealingtarget == true){
    cecilistarget = true
    setsupporttarget()
    playertargetselected()
    setenemyposition()
    removetint()
    }

    else {
    deselect()
    team1noplayerselected = false
    p1cecilselected = true
    playerbuttons()
    }
    }
        
    if (team2turn == true){
    cecilistarget = true
    settarget()
    playertargetselected()
    setenemyposition()
    removetint()
    }
    
 
}

