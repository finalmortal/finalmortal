     p1golbezhealthrating = currenttargethealthrating
    }
     if (currentattacktargetname == 'p1sektor'){
     p1sektorhealthrating = currenttargethealthrating
    }
   if (currentattacktargetname == 'p1cyrax'){
     p1cyraxhealthrating = currenttargethealthrating
    }
    if (currentattacktargetname == 'p1sindel'){
     p1sindelhealthrating = currenttargethealthrating
    }
    if (currentattacktargetname == 'p1raiden'){
     p1raidenhealthrating = currenttargethealthrating
    }
    if (currentattacktargetname == 'p1cyan'){
     p1cyanhealthrating = currenttargethealthrating
    }
    if (currentattacktargetname == 'p1rosa'){
     p1rosahealthrating = currenttargethealthrating
    }
     if (currentattacktargetname == 'p1mog'){
     p1moghealthrating = currenttargethealthrating
    }


//
    if (currentattacktargetname == 'p2cecil'){
     p2cecilhealthrating = currenttargethealthrating
    }
 
    if (currentattacktargetname == 'p2kain'){
     p2kainhealthrating = currenttargethealthrating
    }

    if (currentattacktargetname == 'p2kang'){
     p2kanghealthrating = currenttargethealthrating
