function hideaibuttons(){
if (aiteam == true){
if (team2turn == false){
p2cecilswordbutton.visible = false
p2cecilholybutton.visible = false
p2cecilholyswordbutton.visible = false
p2cecilshieldbutton.visible = false
p2ceciltargetbutton.visible = false  
p2kainjumpbutton.visible = false
p2kainpowerbutton.visible = false
p2kainslashbutton.visible = false
p2kainstrikebutton.visible = false
p2kangfireballbutton.visible = false
p2kangflykickbutton.visible = false
p2kangbicyclebutton.visible = false
p2kangdragonbutton.visible = false
p2kangprayerbutton.visible = false
p2orchidwheelbutton.visible = false
p2orchidcatbutton.visible = false
p2orchidbatonbutton.visible = false
p2orchidfirebabutton.visible = false
p2orchidfitbutton.visible = false
p2spinalslicebutton.visible = false
p2spinalslashbutton.visible = false
p2spinalbashbutton.visible = false
p2spinalspikebutton.visible = false
p2spinalblockbutton.visible = false
p2terrafirebutton.visible = false
p2terraicebutton.visible = false
p2terrawindbutton.visible = false
p2terraearthbutton.visible = false
p2terracallbutton.visible = false

}  
}
}

function hideplayerbuttons(){
if(p1cecilselected == false){
p1cecilswordbutton.visible = false
p1cecilholybutton.visible = false
p1cecilholyswordbutton.visible = false
p1cecilshieldbutton.visible = false
p1ceciltargetbutton.visible = false
}
if(p2cecilselected == false){
p2cecilswordbutton.visible = false
p2cecilholybutton.visible = false
p2cecilholyswordbutton.visible = false
p2cecilshieldbutton.visible = false
p2ceciltargetbutton.visible = false
}

if(p1kainselected == false){
p1kainjumpbutton.visible = false
p1kainpowerbutton.visible = false
p1kainslashbutton.visible = false
p1kainstrikebutton.visible = false
}
if(p2kainselected == false){
p2kainjumpbutton.visible = false
p2kainpowerbutton.visible = false
p2kainslashbutton.visible = false
p2kainstrikebutton.visible = false
}

if(p1kangselected == false){
p1kangfireballbutton.visible = false
p1kangflykickbutton.visible = false
p1kangbicyclebutton.visible = false
p1kangdragonbutton.visible = false
p1kangprayerbutton.visible = false
}
if(p2kangselected == false){
p2kangfireballbutton.visible = false
p2kangflykickbutton.visible = false
p2kangbicyclebutton.visible = false
p2kangdragonbutton.visible = false
p2kangprayerbutton.visible = false
}

if(p1orchidselected == false){
p1orchidfirebabutton.visible = false
p1orchidwheelbutton.visible = false
p1orchidcatbutton.visible = false
p1orchidbatonbutton.visible = false
p1orchidfitbutton.visible = false
}

if(p2orchidselected == false){
p2orchidfirebabutton.visible = false
p2orchidwheelbutton.visible = false
p2orchidcatbutton.visible = false
p2orchidbatonbutton.visible = false
p2orchidfitbutton.visible = false
}

