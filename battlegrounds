  if (p2p4chargerating == 5) {
  p2p4chargebox1.visible = true
  p2p4chargebox2.visible = true
  p2p4chargebox3.visible = true
  p2p4chargebox4.visible = true
  p2p4chargebox5.visible = true
  }
  if (p2p5chargerating == 0) {
  p2p5chargebox1.visible = false
  p2p5chargebox2.visible = false
  p2p5chargebox3.visible = false
  p2p5chargebox4.visible = false
  p2p5chargebox5.visible = false
  }
  if (p2p5chargerating == 1) {
  p2p5chargebox1.visible = true
  p2p5chargebox2.visible = false
  p2p5chargebox3.visible = false
  p2p5chargebox4.visible = false
  p2p5chargebox5.visible = false
  }
  if (p2p5chargerating == 2) {
  p2p5chargebox1.visible = true
  p2p5chargebox2.visible = true
  p2p5chargebox3.visible = false
  p2p5chargebox4.visible = false
  p2p5chargebox5.visible = false
  }
  if (p2p5chargerating == 3) {
  p2p5chargebox1.visible = true
  p2p5chargebox2.visible = true
  p2p5chargebox3.visible = true
  p2p5chargebox4.visible = false
  p2p5chargebox5.visible = false
  }
  if (p2p5chargerating == 4) {
  p2p5chargebox1.visible = true
  p2p5chargebox2.visible = true
  p2p5chargebox3.visible = true
  p2p5chargebox4.visible = true
  p2p5chargebox5.visible = false 
  }
  if (p2p5chargerating == 5) {
  p2p5chargebox1.visible = true
  p2p5chargebox2.visible = true
  p2p5chargebox3.visible = true
  p2p5chargebox4.visible = true
  p2p5chargebox5.visible = true
  }
}
//flipcoin
function flipacoin(){
    if (coinflipstart == true){
        coinflipcount -= 0.1
        if (coinflipdone == false){
        coinflipnumber = Math.floor(Math.random() * 11);
        flipcoingraphic.visible = true
        flipcoingraphic.anims.play('flipcoin', true);
    if (coinflipnumber < 5){
        team1turn = true
        team1noplayerselected = true
    }
    else if (coinflipnumber == 5){
        team1turn = true
        team1noplayerselected = true
    }
    else if (coinflipnumber > 5){
        team2turn = true
        team2noplayerselected = true
    }
        coinflipdone = true
    }
    }
    if (coinflipcount < 1){
    flipcoingraphic.visible = false
    coinflipstart = false
    }
}
//endcoinflip

function focuson(){
if (player1focus == true){
    player1focuson.setDepth(6);
    player1focuson.visible = true
    if (p1p1chargebuttondown == true){
        player1focuson.x = p1p1chargebox1.x + 40
        player1focuson.y = p1p1chargebox1.y + 25
    }
    if (p1p2chargebuttondown == true){
        player1focuson.x = p1p2chargebox1.x + 40
        player1focuson.y = p1p2chargebox1.y + 25
    }
    if (p1p3chargebuttondown == true){
        player1focuson.x = p1p3chargebox1.x + 40
        player1focuson.y = p1p3chargebox1.y + 25

    }
    if (p1p4chargebuttondown == true){
        player1focuson.x = p1p4chargebox1.x + 40
        player1focuson.y = p1p4chargebox1.y + 25
    }
    if (p1p5chargebuttondown == true){
        player1focuson.x = p1p5chargebox1.x + 40
        player1focuson.y = p1p5chargebox1.y + 25
    }
}

if (player2focus == true){
    player2focuson.setDepth(6);
    player2focuson.visible = true
    if (p2p1chargebuttondown == true){
        player2focuson.x = p2p1chargebox1.x + 40
        player2focuson.y = p2p1chargebox1.y + 25
    }
    if (p2p2chargebuttondown == true){
        player2focuson.x = p2p2chargebox1.x + 40
        player2focuson.y = p2p2chargebox1.y + 25
    }
    if (p2p3chargebuttondown == true){
        player2focuson.x = p2p3chargebox1.x + 40
        player2focuson.y = p2p3chargebox1.y + 25

    }
    if (p2p4chargebuttondown == true){
        player2focuson.x = p2p4chargebox1.x + 40
        player2focuson.y = p2p4chargebox1.y + 25
    }
    if (p2p5chargebuttondown == true){
        player2focuson.x = p2p5chargebox1.x + 40
        player2focuson.y = p2p5chargebox1.y + 25
    }
}

}

//ENDFUNCTIONS ***********************************

