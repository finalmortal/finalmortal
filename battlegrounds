p1cecilnameplate.setOrigin(0, 0);
p1cecilnameplate.on('pointerdown', function (pointer) {
    p1cecilbuttonclick()
    });
p1cecilhealthtextx = battlegroundboxt1p1.x + 35
p1cecilhealthtexty = battlegroundboxt1p1.y + 3         
p1cecilhealthtext = this.add.text(p1cecilhealthtextx, p1cecilhealthtexty, '', { font: '25px Arial', fill: '#00ff00' }).setScrollFactor(0);


