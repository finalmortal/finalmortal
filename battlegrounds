function p1cecilswordkey(){
if (p1cecilactionpoints > 24){ 
if (playerabilityselected == false){
p1cecilswordbutton.setTint(0xff0000);
setenemytint()
p1cecilattacksword = true
playerattacking = true
playerabilityselected = true
p1cecilswordtime = 8
p1cecilselectingtarget = true
p1cecilswordsoundplayed = false
}
}
}

