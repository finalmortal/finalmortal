function resetai(){
    aiplayerchosen = false
    aiattackchosen = false
    aitargetchosen = false
    aireset()
  
}

function aireset(){
 
      

}



function aidecide(){
    if (team2turn == true){
    minusnumber = Math.floor(Math.random() * 6);
    if (chooseaiattack == 0){
    chooseaiattack = Math.floor(Math.random() * 4);
}
    
    if (chooseaiplayer == 0){
    chooseaiplayer = Math.floor(Math.random() * 6);
}

    if (chooseaitarget == 0){
    chooseaitarget = Math.floor(Math.random() * 6);
}

    if (chooseaitarget > 6){
    chooseaitarget = Math.floor(Math.random() * 6);
}

    if (chooseaiplayer > 4){
    chooseaiplayer = Math.floor(Math.random() * 6);
}

    if (chooseaiattack > 4){
    chooseaiattack = Math.floor(Math.random() * 4);
}
///
}
}

function aitargets(){ 

     if (chooseaitarget == 1 ){
        if (player1p1name == 'CECIL') {  
        currentattacktargetname = 'p1cecil'
        currentenemyposx = p1cecilx
        currentenemyposy = p1cecily
        }
        if (player1p1name == 'KAIN') {  
        currentattacktargetname = 'p1kain'
        currentenemyposx = p1kainx
        currentenemyposy = p1kainy
        }
        if (player1p1name == 'KANG') {  
        currentattacktargetname = 'p1kang'
        currentenemyposx = p1kangx
        currentenemyposy = p1kangy
        }
        
        if (player1p1name == 'ORCHID') {  
        currentattacktargetname = 'p1orchid'
        currentenemyposx = p1orchidx
        currentenemyposy = p1orchidy
        }

        if (player1p1name == 'SPINAL') {  
        currentattacktargetname = 'p1spinal'
        currentenemyposx = p1spinalx
        currentenemyposy = p1spinaly
        }
        if (player1p1name == 'TERRA') {    
        currentattacktargetname = 'p1terra'
        currentenemyposx = p1terrax
        currentenemyposy = p1terray
        }
        } 
     if (chooseaitarget == 2 ){
        if (player1p2name == 'CECIL') {  
        currentattacktargetname = 'p1cecil'
        currentenemyposx = p1cecilx
        currentenemyposy = p1cecily
        }
        if (player1p2name == 'KAIN') {  
        currentattacktargetname = 'p1kain'
