        p2rosacurespell.visible = true
        p2rosacurespell.anims.play('rosacurespell', true);
        if (p2rosacuresoundplayed == false){
        game.sound.add( "rosacuresound" ).play()
        p2rosacuresoundplayed = true
    }
        p2rosacurespell.x = p2rosatargetx - 45
        p2rosacurespell.y = p2rosatargety - 25

    if (p2rosacuretime < 1){
       playerattacking = true
       playerbattleattack()
       p2rosaidle = true
       playerabilityselected = false
       p2rosaselected = false
       player2selectabilitybox.visible = false
       p2rosacurespell.visible = false
       p2rosa.x = p2rosax
       p2rosa.y = p2rosay
       team1turn = true
       team2turn = false
       p2rosahealingtarget = false
       p2rosatargetselected = false
       p2rosasupportcure = false
    }
    }
    }
    

 if (p2rosasupportheal == true){
        if (p2rosatargetselected == true){
        p2rosaidle = false
        p2rosahealtime -= 0.2;
        p2rosa.anims.play('rosacasting', true);
        p2rosahealspell.visible = true
