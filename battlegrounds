if (p1orchid.x < p1orchidtargetx - 99) {
p1orchid.anims.play('orchidwalk', true);
p1orchid.x += 25 
if (p1orchid.y < p1orchidtargety){
    p1orchid.y += 7;
}
if (p1orchid.y > p1orchidtargety){
    p1orchid.y -= 7;
}
if (p1orchid.x > p1orchidtargetx - 100) {
p1orchid.anims.play('orchidwheel', true);

}
}
if (p1orchidwheeltime < 1){
playerattacking = true
playerbattleattack()
