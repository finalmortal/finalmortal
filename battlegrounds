playerabilityselected = false
p2sonjaselected = false
p2sonjakiss.visible = false
p2sonjakiss.x = p2sonja.x
p2sonjakiss.y = p2sonja.y 
player2selectabilitybox.visible = false
p2sonja.x = p2sonjax
p2sonja.y = p2sonjay
team1turn = true
team2turn = false
p2sonjatargetselected = false
p2sonjaattackkiss = false
}
}
}

if (p2sonjaattackdancer == true){
p2sonjaidle = false
p2sonjadancertime -= 0.2;
if (p2sonja.x >  600) {
p2sonja.anims.play('sonjarun', true);
p2sonja.x -= 25 
}
if (p2sonja.x <  601) {
p2sonja.anims.play('sonjadancer', true);
}
