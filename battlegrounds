//ENDCREATE battleground*********************************************
//endcreatebg

//BEGINUPDATE battleground*******************************************
//updatebattleground
//update
update: function (time, delta)
{
updatetargets()
hideaibuttons()
flipacoin()

if (backgroundclouds == true){
mountainclouds2.x -= 0.1;
mountainclouds1.x += 0.1;
}

if (launchtowerbattleground == true){
    
    if (launchbackground1.y < -800){
    launchbackgroundup = true
    }
    if (launchbackground1.y > -1){
    launchbackgroundup = false
    }
    if (launchbackgroundup == true){
    launchbackground1.y += 0.2;
    }
    if (launchbackgroundup == false){
    launchbackground1.y -= 0.2;
    }

}

if (gameround == 2){
   if (round2voiceplayed == false){
        game.sound.add( "round2voice" ).play()
        round2voiceplayed = true
    } 
}
if (gameround == 3){
   if (round3voiceplayed == false){
        game.sound.add( "round3voice" ).play()
        round3voiceplayed = true
    } 
}
if (gameround == 4){
   if (round2voiceplayed == false){
        game.sound.add( "round4voice" ).play()
        round2voiceplayed = true
    } 
}
if (gameround == 5){
   if (round3voiceplayed == false){
        game.sound.add( "round5voice" ).play()
        round3voiceplayed = true
    } 
}
if (gameround == 6){
   if (round2voiceplayed == false){
        game.sound.add( "round6voice" ).play()
        round2voiceplayed = true
    } 
}
if (gameround == 7){
   if (round3voiceplayed == false){
        game.sound.add( "round7voice" ).play()
        round3voiceplayed = true
    } 
}
if (gameround == 8){
   if (round2voiceplayed == false){
        game.sound.add( "round8voice" ).play()
        round2voiceplayed = true
    } 
}
if (gameround == 9){
   if (round3voiceplayed == false){
        game.sound.add( "round9voice" ).play()
        round3voiceplayed = true
    } 
}


if (matchbegin == true){
if (matchbegintime > 0){
 matchbegintime -= 0.3;

  if (round1voiceplayed == false){
        game.sound.add( "round1voice" ).play()
        round1voiceplayed = true
    }

}
if (matchbegintime < 2){
    roundstart = true
    matchbegin = false
    matchstartinggraphic.visible = false

if (fightvoiceplayed == false){
        game.sound.add( "fightvoice" ).play()
        fightvoiceplayed = true
    }
    coinflipstart = true
    
}
}

if (roundstart == true){
gameroundnumber -= 0.1
roundhover.visible = true
roundtxt.visible = true

if (gameroundnumber < 1){
roundhover.visible = false
roundtxt.visible = false
roundstart = false
}
}

//ai
if (aiteam == true){
if (team2turn == true){
if (aiplayerchosen == false){
chooseaiplayer = Math.floor(Math.random() * 6);
aiplayerchosen = true
}
if (p2cecilactive == true){
if (player2p1name == 'CECIL') {     
if (chooseaiplayer == 1){
    p2cecilselected = true
    team2noplayerselected = false
    player2selectabilitybox.visible = true
    p2cecilswordbutton.visible = true
    p2cecilholybutton.visible = true 
    p2cecilholyswordbutton.visible = true
    p2cecilshieldbutton.visible = true
    p2ceciltargetbutton.visible = true
    if (aiattackchosen == false){
    chooseaiattack = Math.floor(Math.random() * 4);
    aiattackchosen = true
    }
if(chooseaiattack == 1){
    p2cecilattacksword = true
    if (playerabilityselected == false){
            p2cecilswordbutton.setTint(0xff0000);
            setenemytint()
            p2cecilattacksword = true
            playerattacking = true
            playerabilityselected = true
            p2cecilswordtime = 8
            p2cecilselectingtarget = true
            p2cecilswordsoundplayed = false
            aipicktarget()
            }

}
if(chooseaiattack == 2){
    p2cecilattackholysword = true
    if (playerabilityselected == false){
            p2cecilholyswordbutton.setTint(0xff0000);
            setenemytint()
            p2cecilattackholysword = true
            playerattacking = true
            playerabilityselected = true
            p2cecilholyswordtime = 8
            p2cecilselectingtarget = true
            p2cecilholyswordsoundplayed = false
            aipicktarget()
            }

}
if(chooseaiattack == 3){
    p2cecilattacktarget = true
    if (playerabilityselected == false){
            p2ceciltargetbutton.setTint(0xff0000);
            setenemytint()
            p2cecilattacktarget = true
            playerattacking = true
            playerabilityselected = true
            p2ceciltargettime = 8
            p2cecilselectingtarget = true
            p2ceciltargetsoundplayed = false
            aipicktarget()
            }

}
} 
}
}
if (p2kainactive == true){
if (player2p2name == 'KAIN') {     
if (chooseaiplayer == 2){
    p2kainselected = true
    team2noplayerselected = false
    player2selectabilitybox.visible = true
    p2kainslashbutton.visible = true
    p2kainjumpbutton.visible = true
    p2kainstrikebutton.visible = true
    p2kainpowerbutton.visible = true
    if (aiattackchosen == false){
    chooseaiattack = Math.floor(Math.random() * 4);
    aiattackchosen = true
    }
if(chooseaiattack == 1){
    p2kainattackstrike = true
    if (playerabilityselected == false){
            p2kainstrikebutton.setTint(0xff0000);
            setenemytint()
            p2kainattackstrike = true
            p2kainstriketime = 8
            p2kainselectingtarget = true
            playerabilityselected = true
            p2kainstrikesoundplayed = false
            aipicktarget()
            }

}
if(chooseaiattack == 2){
    p2kainattackslash = true
    if (playerabilityselected == false){
            p2kainslashbutton.setTint(0xff0000);
            setenemytint()
            p2kainattackslash = true
            p2kainslashtime = 8
            p2kainselectingtarget = true
            playerabilityselected = true
            p2kainslashsoundplayed = false
            aipicktarget()
            }

}
if(chooseaiattack == 3){
    p2kainattackjump = true
    if (playerabilityselected == false){
            p2kainjumpbutton.setTint(0xff0000);
            setenemytint()
            p2kainattackjump = true
            p2kainjumptime = 75
            p2kainselectingtarget = true
            playerabilityselected = true
            aipicktarget()
            }

}
} 
}
}


if (p2kangactive == true){
if (player2p3name == 'KANG') {     
if (chooseaiplayer == 3){
    p2kangselected = true
    team2noplayerselected = false
    player2selectabilitybox.visible = true
    p2kangfireballbutton.visible = true
    p2kangflykickbutton.visible = true 
    p2kangbicyclebutton.visible = true
    p2kangdragonbutton.visible = true
    p2kangprayerbutton.visible = true
    if (aiattackchosen == false){
    chooseaiattack = Math.floor(Math.random() * 4);
    aiattackchosen = true
    }
if(chooseaiattack == 1){
    if (playerabilityselected == false){
            p2kangflykickbutton.setTint(0xff0000);
            setenemytint()
            p2kangattackflykick = true
            p2kangflykicktime = 8
            p2kangselectingtarget = true
            playerabilityselected = true
            p2kangflykicksoundplayed = false
            aipicktarget()
            }

}
if(chooseaiattack == 2){
      if (playerabilityselected == false){
            p2kangfireballbutton.setTint(0xff0000);
            setenemytint()
            p2kangattackfireball = true
            p2kangfireballtime = 8
            p2kangselectingtarget = true
            playerabilityselected = true
            p2kangfireballsoundplayed = false
            aipicktarget()
            }

}
if(chooseaiattack == 3){
    p2kangattackbicycle = true
    if (playerabilityselected == false){
            setenemytint()
            p2kangattackbicycle = true
            p2kangbicycletime = 8
            p2kangselectingtarget = true
            playerabilityselected = true
            aipicktarget()
            }

}
} 
}
}

if (p2orchidactive == true){
if (player2p4name == 'ORCHID') {     
if (chooseaiplayer == 4){
    p2orchidselected = true
    team2noplayerselected = false
    player2selectabilitybox.visible = true
    p2orchidwheelbutton.visible = true
    p2orchidfirebabutton.visible = true 
    p2orchidcatbutton.visible = true
    p2orchidbatonbutton.visible = true
    p2orchidfitbutton.visible = true
    if (aiattackchosen == false){
    chooseaiattack = Math.floor(Math.random() * 4);
    aiattackchosen = true
    }
if(chooseaiattack == 1){
    if (playerabilityselected == false){
            p2orchidwheelbutton.setTint(0xff0000);
            setenemytint()
            p2orchidattackwheel = true
            p2orchidwheeltime = 16
            p2orchidselectingtarget = true
            playerabilityselected = true
            p2orchidwheelsoundplayed = false
            aipicktarget()
            }

}
if(chooseaiattack == 2){
      if (playerabilityselected == false){
            p2orchidfirebabutton.setTint(0xff0000);
            setenemytint()
            p2orchidattackfireba = true
            p2orchidfirebatime = 8
            p2orchidselectingtarget = true
            playerabilityselected = true
            p2orchidfirebasoundplayed = false
            aipicktarget()
            }

}
if(chooseaiattack == 3){
     if (playerabilityselected == false){
         p2orchidcatbutton.setTint(0xff0000);
            setenemytint()
            p2orchidattackcat = true
            p2orchidcattime = 8
            p2orchidselectingtarget = true
            playerabilityselected = true
            aipicktarget()
            }

}
if(chooseaiattack == 4){
     if (playerabilityselected == false){ 
            p2orchidbatonbutton.setTint(0xff0000);
            setenemytint()
            p2orchidattackbaton = true
            p2orchidbatontime = 15
            p2orchidselectingtarget = true
            playerabilityselected = true
            aipicktarget()
            }

}
} 
}
}

if (p2spinalactive == true){
if (player2p5name == 'SPINAL') {     
if (chooseaiplayer == 5){
    p2spinalselected = true
    team2noplayerselected = false
    player2selectabilitybox.visible = true
    p2spinalslicebutton.visible = true
    p2spinalslashbutton.visible = true 
    p2spinalbashbutton.visible = true
    p2spinalspikebutton.visible = true
    p2spinalblockbutton.visible = true
    if (aiattackchosen == false){
    chooseaiattack = Math.floor(Math.random() * 4);
    aiattackchosen = true
    }
if(chooseaiattack == 1){
    if (playerabilityselected == false){
            p2spinalslicebutton.setTint(0xff0000);
            setenemytint()
            p2spinalattackslice = true
            p2spinalslicetime = 16
            p2spinalselectingtarget = true
            playerabilityselected = true
            p2spinalslicesoundplayed = false
            aipicktarget()
            }

}
if(chooseaiattack == 2){
      if (playerabilityselected == false){
            p2spinalslashbutton.setTint(0xff0000);
            setenemytint()
            p2spinalattackslash = true
            p2spinalslashtime = 16
            p2spinalselectingtarget = true
            playerabilityselected = true
            p2spinalslashsoundplayed = false
            aipicktarget()
            }

}
if(chooseaiattack == 3){
     if (playerabilityselected == false){
            p2spinalbashbutton.setTint(0xff0000);
            setenemytint()
            p2spinalattackbash = true
            p2spinalbashtime = 16
            p2spinalselectingtarget = true
            playerabilityselected = true
            aipicktarget()
            }

}
if(chooseaiattack == 4){
     if (playerabilityselected == false){
           p2spinalspikebutton.setTint(0xff0000);
            setenemytint()
            p2spinalattackspike = true
            p2spinalspiketime = 16
            p2spinalselectingtarget = true
            playerabilityselected = true
            aipicktarget()
            }

}
} 
}
}
}
}

if (ancienttemplebattleground == true){
ancienttemplehead.anims.play('ancienttempleheadplay', true);
ancienttemplefrontwall.setDepth(6);
}

if (dumpgroundbattleground == true){
buzzingflies.anims.play('buzzingfliesplay', true);
}
if (waterfallbattleground == true){
falls.anims.play('fallsplay', true);
}

if (stellarcitybattleground == true){
stellarshoptv.anims.play('stellarshoptvplay', true);
fingerarrow.anims.play('fingerarrowplay', true);
trianglerotate.anims.play('trianglerotateplay', true);
arcade1.anims.play('arcade1play', true);
arcade2.anims.play('arcade2play', true);
smokeface.anims.play('smokefaceplay', true);
fingertop.anims.play('fingertopplay', true);
thefinger.anims.play('thefingerplay', true);
}

if (oakloungebattleground == true){
tablelamp.anims.play('tablelampplay', true);
doorlights.anims.play('doorlightsplay', true);
trianglean.anims.play('triangleanplay', true);
colorsmear.anims.play('colorsmearplay', true);
colortrip.anims.play('colortripplay', true);
outsidebackground.anims.play('outsidebackgroundplay', true);
}

if (airportbattleground == true){
airportplanelight.anims.play('planelight', true);
}    


//*******************
if (matchbegin == true){
    matchstartinggraphic.visible = true
}

if (matchbegin == false){
    matchstartinggraphic.visible = false
}
//ENDUPDATE*****************
//////////////////////////////
//ENDUPDATE***************
//BEGIN UI
}
});
//ui
var battlegroundui = new Phaser.Class({

    Extends: Phaser.Scene,

    initialize:

    function battlegroundui ()
    {
        Phaser.Scene.call(this, { key: 'battlegroundui' });
    },

    preload: function ()
    {
      
    },
//BEGINCREATE UI ******************************************************************
create: function ()
{
//backbox
battlegroundbackbox = this.add.sprite(uix, uiy, 'battlegroundbackbox').setOrigin(0, 0);
//team1boxes
battlegroundboxt1p1 = this.add.sprite(21, 609, 'battlegroundbox').setOrigin(0, 0);
battlegroundboxt1p2 = this.add.sprite(150, 609, 'battlegroundbox').setOrigin(0, 0);
battlegroundboxt1p3 = this.add.sprite(279, 609, 'battlegroundbox').setOrigin(0, 0);
battlegroundboxt1p4 = this.add.sprite(408, 609, 'battlegroundbox').setOrigin(0, 0);
battlegroundboxt1p5 = this.add.sprite(537, 609, 'battlegroundbox').setOrigin(0, 0);
//team2boxes
battlegroundboxt2p1 = this.add.sprite(700, 609, 'battlegroundbox').setOrigin(0, 0);
battlegroundboxt2p2 = this.add.sprite(829, 609, 'battlegroundbox').setOrigin(0, 0);
battlegroundboxt2p3 = this.add.sprite(958, 609, 'battlegroundbox').setOrigin(0, 0);
battlegroundboxt2p4 = this.add.sprite(1087, 609, 'battlegroundbox').setOrigin(0, 0);
battlegroundboxt2p5 = this.add.sprite(1216, 609, 'battlegroundbox').setOrigin(0, 0);
player1selectabilitybox = this.add.sprite(149, 533, 'player1selectabilitybox').setOrigin(0, 0);
player1selectabilitybox.visible = false
player1selecttargetbox = this.add.sprite(27, 6, 'player1selecttargetbox').setOrigin(0, 0);
player1selecttargetbox.visible = false
player2selectabilitybox = this.add.sprite(857, 533, 'player2selectabilitybox').setOrigin(0, 0);
player2selectabilitybox.visible = false
player2selecttargetbox = this.add.sprite(810, 6, 'player2selecttargetbox').setOrigin(0, 0);
player2selecttargetbox.visible = false
blueteamselectplayerbox = this.add.sprite(4, 543, 'blueteamselectplayerbox').setOrigin(0, 0);
blueteamselectplayerbox.visible = true
redteamselectplayerbox = this.add.sprite(685, 543, 'redteamselectplayerbox').setOrigin(0, 0);
redteamselectplayerbox.visible = false

player1focuson = this.add.sprite(p1focusx, p1focusy, 'focusongraphic').setOrigin(0, 0);
player1focuson.visible = false
player2focuson = this.add.sprite(p2focusx, p2focusy, 'focusongraphic').setOrigin(0, 0);
player2focuson.visible = false


this.input.setPollAlways();
        this.input.on('gameobjectover', function (pointer, gameObject) {
        gameObject.setTint(0xff0000);
    });
    
        this.input.on('gameobjectout', function (pointer, gameObject) {
        gameObject.clearTint();
    });      

    flipcoingraphic = this.add.sprite(660, 300, 'flipcoin').setOrigin(0, 0);
    flipcoingraphic.visible = false
    

    cursors = this.input.keyboard.createCursorKeys();
    onekey = this.input.keyboard.addKey(Phaser.Input.Keyboard.KeyCodes.ONE);   
    twokey = this.input.keyboard.addKey(Phaser.Input.Keyboard.KeyCodes.TWO);   
    threekey = this.input.keyboard.addKey(Phaser.Input.Keyboard.KeyCodes.THREE);
    fourkey = this.input.keyboard.addKey(Phaser.Input.Keyboard.KeyCodes.FOUR);
    fivekey = this.input.keyboard.addKey(Phaser.Input.Keyboard.KeyCodes.FIVE);
    cursors.left.isDown = false
    cursors.right.isDown = false
    cursors.up.isDown = false
    cursors.down.isDown = false
    statustxt = this.add.text(10, 10, '', { font: '17px Arial', fill: '#00ff00' }).setScrollFactor(0);
    team1hptxt = this.add.text(65, -2, '', { font: '45px Arial', fill: '#ffffff' }).setScrollFactor(0);
    team2hptxt = this.add.text(1200, -2, '', { font: '45px Arial', fill: '#ffffff' }).setScrollFactor(0);
//loadcecil***************************************
  if (loadcecil == true){
