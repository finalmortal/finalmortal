function currentattacktarget(){
    if (currentattacktargetname == 'p1cecil'){
     p1cecilhealthrating = currenttargethealthrating
    }
 
    if (currentattacktargetname == 'p1kain'){
     p1kainhealthrating = currenttargethealthrating
    }

    if (currentattacktargetname == 'p1kang'){
     p1kanghealthrating = currenttargethealthrating
    }

    if (currentattacktargetname == 'p1orchid'){
     p1orchidhealthrating = currenttargethealthrating
    }

    if (currentattacktargetname == 'p1spinal'){
     p1spinalhealthrating = currenttargethealthrating
    }
    if (currentattacktargetname == 'p1terra'){
     p1terrahealthrating = currenttargethealthrating
    }
     if (currentattacktargetname == 'p1kano'){
     p1kanohealthrating = currenttargethealthrating
    }
