p2combo.x = p2combox
p2combo.y = p2comboy
team1turn = true
team2turn = false
p2combotargetselected = false
p2comboattackjumpkick = false
}
}
}
if (p2comboattackgun == true){
if (p2combotargetselected == true){
p2comboidle = false
p2comboguntime -= 0.2
if (p2combo.x > p2combotargetx + 500) {
p2combo.anims.play('comborun', true);
p2combo.x -= 25 
}
if (p2combo.x < p2combotargetx + 501) {
if (p2combogunvoiceplayed == false){
        game.sound.add( "combogunvoice" ).play()
        p2combogunvoiceplayed = true
    }
if (p2combogunsoundplayed == false){
        game.sound.add( "combogunsound" ).play()
        p2combogunsoundplayed = true
    }
   
p2combo.anims.play('combogun', true);
}
if (p2comboguntime < 1){
playerattacking = true
playerbattleattack()
p2comboidle = true
playerabilityselected = false
p2comboselected = false
player2selectabilitybox.visible = false
p2combo.x = p2combox
p2combo.y = p2comboy
team1turn = true
team2turn = false
p2combotargetselected = false
p2comboattackgun = false

}
}
}
if (p2comboattackslam == true){
if (p2combotargetselected == true){
p2comboidle = false
p2comboslamtime -= 0.2;
if (p2combo.x > p2combotargetx + 50) {
p2combo.anims.play('comborun', true);
if (p2comboslamvoiceplayed == false){
        game.sound.add( "comboslamvoice" ).play()
        p2comboslamvoiceplayed = true
    }

p2combo.x -= 25
}
if (p2combo.x < p2combotargetx + 51) {
p2combo.anims.play('comboslam', true);
}
if (p2comboslamtime < 0){
playerattacking = true
playerbattleattack()
p2comboidle = true
playerabilityselected = false
p2comboselected = false
player2selectabilitybox.visible = false
p2combo.x = p2combox
p2combo.y = p2comboy
team1turn = true
team2turn = false
p2combotargetselected = false
p2comboattackslam= false
}
}
}
if (p2comboattackmuscle == true){
p2comboidle = false
p2combomuscletime -= 0.2;
if (p2combo.x >  600) {
p2combo.anims.play('comborun', true);
p2combo.x -= 25 
}
if (p2combo.x <  601) {
p2combo.anims.play('combomuscle', true);
if (p2combomusclevoiceplayed == false){
        game.sound.add( "combomusclevoice" ).play()
        p2combomusclevoiceplayed = true
    }
}
if (p2combomuscletime < 1 ){
playerattacking = true
playerbattleattack()
p2comboidle = true
playerabilityselected = false
p2comboselected = false
player2selectabilitybox.visible = false
p2combo.x = p2combox
p2combo.y = p2comboy
team1turn = true
team2turn = false
p2combotargetselected = false
p2comboattackmuscle = false
}
}
//
}
