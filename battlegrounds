//kain1action
function p1kainaction(){
if (p1kaintargetselected == true){
if (p1kainattackstrike == true){
        if (p1kain.x < p1kaintargetx - 75){
            p1kain.anims.play('kainwalk', true);
            p1kain.x += 25
            
        }
         if (p1kain.y > p1kaintargety + 25){
            p1kain.y -= 5
        }
        if (p1kain.y < p1kaintargety + 25){
            p1kain.y += 5
        }
        
        if (p1kain.x > p1kaintargetx - 76){
        p1kainstriketime -= 1;
        p1kainstrike.x = p1kain.x 
        p1kainstrike.y = p1kain.y + 25
        p1kain.anims.play('kainattacksword', true);
        if (p1kainstrikesoundplayed == false){
             
            game.sound.add( "kainstrikesound" ).play() 
            
            p1kainstrikesoundplayed = true
        }
        p1kainstrike.anims.play('kainstrike', true);
        p1kainstrike.visible = true
    }

    if (p1kainstriketime < 1){
        playerattacking = true
        playerbattleattack()
        p1kainstrike.visible = false
        p1kain.x = p1kainx
        p1kain.y = p1kainy
        p1kain.anims.play('kainidle', true);
        p1kaintargetselected = false
        playerabilityselected = false
        p1kainselected = false
        player1selectabilitybox.visible = false
        team2turn = true
        team1turn = false
        p1kainattackstrike = false
        
       
    }

}
    if (p1kainattackslash == true){
    if (p1kain.x < p1kaintargetx - 75){
            p1kain.anims.play('kainwalk', true);
            p1kain.x += 25
            
        }
         if (p1kain.y > p1kaintargety + 25){
            p1kain.y -= 5
        }
        if (p1kain.y < p1kaintargety + 25){
            p1kain.y += 5
        }

        if (p1kain.x > p1kaintargetx - 76){
        p1kainslashtime -= 1;
        p1kainslash.x = p1kain.x 
        p1kainslash.y = p1kain.y + 25
        p1kain.anims.play('kainattacksword', true);
        p1kainslash.anims.play('kainslash', true);
        if (p1kainslashsoundplayed == false){
        
        game.sound.add( "kainslashsound" ).play() 
        
        p1kainslashsoundplayed = true
    }
        p1kainslash.visible = true
    }

    if (p1kainslashtime < 1){
        playerattacking = true
        playerbattleattack()
        p1kainslash.visible = false
        p1kain.x = p1kainx
        p1kain.y = p1kainy
        p1kain.anims.play('kainidle', true);
        p1kaintargetselected = false
        playerabilityselected = false
        p1kainselected = false
        player1selectabilitybox.visible = false
        team2turn = true
        team1turn = false
        p1kainattackslash = false

        
        
}
}
//p1kainjump
if (p1kainattackjump == true){
if (p1kaintargetselected == true){
    p1kainjumptime -= 1;
    if (p1kainjumptime > 21){
        
    if (p1kain.y > -200){
        p1kain.y -= 35
    }

    if (p1kain.x < p1kaintargetx){
        p1kain.x += 35
    }
    }

    if (p1kainjumptime < 20){
    if (p1kain.y < p1kaintargety - 100 ){
        p1kain.y += 35
        p1kainjumpweapon.anims.play('kainjumpweapon', true);
        p1kain.anims.play('kainjump', true);
        p1kainjumpweapon.visible = true
        p1kainjumpweapon.x = p1kain.x + 25
        p1kainjumpweapon.y = p1kain.y - 20
    if (p1kain.x > p1kaintargetx - 20){
        p1kain.x -= 20;
    }
    }  
    
    if (p1kain.y > p1kaintargety - 101)
      p1kainjumpimpact.visible = true
      p1kainjumpimpact.anims.play('kainjumpimpact', true);
      p1kainjumpimpact.x = p1kaintargetx
      p1kainjumpimpact.y = p1kaintargety - 120
      if (p1kainjumpdownsoundplayed == false){
      game.sound.add( "kainjumpdown" ).play() 
      p1kainjumpdownsoundplayed = true
  }
    }

    if (p1kainjumptime < 1){
        playerattacking = true
        playerbattleattack()
        p1kainattackjump = false
        p1kainjumpweapon.visible = false
        p1kainjumpimpact.visible = false
        p1kain.x = p1kainx
        p1kain.y = p1kainy
        p1kain.anims.play('kainidle', true);
        p1kaintargetselected = false
        playerabilityselected = false
        p1kainselected = false
        player1selectabilitybox.visible = false
        team2turn = true
        team1turn = false
        p1kainattackjump = false
       
        
    }

}
}

    
}
}

//p2kainaction
function p2kainaction(){
if (p2kaintargetselected == true){
if (p2kainattackstrike == true){
if (p2kain.x > p2kaintargetx + 109){
            p2kain.anims.play('kainwalk', true);
            p2kain.x -= 25
            
        }
         if (p2kain.y < p2kaintargety + 25){
            p2kain.y += 2
        }
        if (p2kain.y > p2kaintargety + 25){
            p2kain.y -= 3
        }
        
        if (p2kain.x < p2kaintargetx + 110){
        p2kainstriketime -= 1;
        p2kainstrike.x = p2kain.x - 45
        p2kainstrike.y = p2kain.y + 25
        p2kain.anims.play('kainattacksword', true);
        if (p2kainstrikesoundplayed == false){
            game.sound.add( "kainstrikesound" ).play() 
            p2kainstrikesoundplayed = true
        }
        p2kainstrike.anims.play('kainstrike', true);
        p2kainstrike.visible = true
    }

    if (p2kainstriketime < 1){
        playerattacking = true
        playerbattleattack()
        p2kainstrike.visible = false
        p2kain.x = p2kainx
        p2kain.y = p2kainy
        p2kain.anims.play('kainidle', true);
        p2kaintargetselected = false
        playerabilityselected = false
        p2kainselected = false
        player2selectabilitybox.visible = false
        team1turn = true
        team2turn = false
        p2kainattackstrike = false
        
       
    }

}
    if (p2kainattackslash == true){
    if (p2kain.x > p2kaintargetx + 125){
            p2kain.anims.play('kainwalk', true);
            p2kain.x -= 25
            
        }
         if (p2kain.y < p2kaintargety + 25){
            p2kain.y += 2
        }
        if (p2kain.y > p2kaintargety + 25){
            p2kain.y -= 3
        }

        if (p2kain.x < p2kaintargetx + 124){
        p2kainslashtime -= 1;
        p2kainslash.x = p2kain.x -45
        p2kainslash.y = p2kain.y + 25
        p2kain.anims.play('kainattacksword', true);
        p2kainslash.anims.play('kainslash', true);
        if (p2kainslashsoundplayed == false){
        
        game.sound.add( "kainslashsound" ).play() 
        
        p2kainslashsoundplayed = true
    }
        p2kainslash.visible = true
    }

    if (p2kainslashtime < 1){
        playerattacking = true
        playerbattleattack()
        p2kainslash.visible = false
        p2kain.x = p2kainx
        p2kain.y = p2kainy
        p2kain.anims.play('kainidle', true);
        p2kaintargetselected = false
        playerabilityselected = false
        p2kainselected = false
        player2selectabilitybox.visible = false
        team1turn = true
        team2turn = false
        p2kainattackslash = false
        
        
}
}
//p2kainjump
if (p2kainattackjump == true){
     
    p2kainjumptime -= 1;
    if (p2kainjumptime > 21){
        
    if (p2kain.y > -200){
        p2kain.y -= 35
    }

    if (p2kain.x > p2kaintargetx){
        p2kain.x -= 35
    }
    }

    if (p2kainjumptime < 20){
    if (p2kain.y < p2kaintargety - 100 ){
        p2kain.y += 35
        p2kainjumpweapon.anims.play('kainjumpweapon', true);
        p2kain.anims.play('kainjump', true);
        p2kainjumpweapon.visible = true
        p2kainjumpweapon.x = p2kain.x + 25
        p2kainjumpweapon.y = p2kain.y - 20
    if (p2kain.x > p2kaintargetx + 20){
        p2kain.x -= 20;
    }
    }  
    
    if (p2kain.y > p2kaintargety - 101)
      p2kainjumpimpact.visible = true
      p2kainjumpimpact.anims.play('kainjumpimpact', true);
      p2kainjumpimpact.x = p2kaintargetx
      p2kainjumpimpact.y = p2kaintargety - 120
      if (p2kainjumpdownsoundplayed == false){
     
      game.sound.add( "kainjumpdown" ).play() 
      
      p2kainjumpdownsoundplayed = true
  }
    }

    if (p2kainjumptime < 1){
        playerattacking = true
        playerbattleattack()
        p2kainjumpweapon.visible = false
        p2kainjumpimpact.visible = false
        p2kain.x = p2kainx
        p2kain.y = p2kainy
        p2kain.anims.play('kainidle', true);
        p2kaintargetselected = false
        playerabilityselected = false
        p2kainselected = false
        player2selectabilitybox.visible = false
        team1turn = true
        team2turn = false
        p2kainattackjump = false
       
        
    }

}
}
}


//p1kang1action
function p1kangaction(){
//kangidle    
if (p1kangidle == true){
p1kang.anims.play('kangidle', true);
}
//kangidle
//p1kangflykick
if (p1kangattackflykick == true){
if (p1kangtargetselected == true){
p1kangidle = false
if (p1kang.x < p1kangtargetx) {
if (p1kangflykicksoundplayed == false){ 
game.sound.add( "kangflykick" ).play()
p1kangflykicksoundplayed = true
}    
p1kang.anims.play('kangflykick', true);
p1kang.x += 50
if (p1kang.y < p1kangtargety - 35 ){
    p1kang.y += 10;
}
if (p1kang.y > p1kangtargety - 35 ){
    p1kang.y -= 10;
}

if (p1kang.x > p1kangtargetx - 25) {
playerattacking = true
playerbattleattack()
p1kangattackflykick = false
p1kangidle = true
p1kang.x = p1kangx
p1kang.y = p1kangy
p1kangtargetselected = false
playerabilityselected = false
p1kangselected = false
player1selectabilitybox.visible = false
team2turn = true
team1turn = false
}
}
}
}
//endkangflykick
//p1kangfireball
if (p1kangattackfireball == true){
if (p1kangtargetselected == true){
p1kangidle = false
if (p1kang.x < p1kangtargetx) {
if (p1kangfireballsoundplayed == false){
p1kang.anims.play('kangfireball', true);
game.sound.add( "kangfireball" ).play()
p1kangfireballsoundplayed = true
}
p1kangfireball.visible = true
p1kangfireball.anims.play('kangfireballgr', true);
p1kangfireball.x += 45 
if (p1kangfireball.y < p1kangtargety){
    p1kangfireball.y += 7;
}
if (p1kangfireball.y > p1kangtargety){
    p1kangfireball.y -= 7;
}

if (p1kangfireball.x > p1kangtargetx - 25) {
playerattacking = true
playerbattleattack()
p1kangidle = true
p1kangfireballsoundplayed = false
p1kangfireball.visible = false
p1kangfireball.x = p1kang.x
p1kangfireball.y = p1kang.y
p1kangtargetselected = false
playerabilityselected = false
p1kangselected = false
player1selectabilitybox.visible = false
team2turn = true
team1turn = false
p1kangattackfireball = false
}
}
}
}
//endp1kangfireball
//p1kangbicycle
if (p1kangattackbicycle == true){
if (p1kangtargetselected == true){
p1kangidle = false

if (p1kang.x < p1kangtargetx - 6) {
if (p1kang.x < p1kangtargetx -301){
p1kang.anims.play('kangrun', true);
}
p1kang.x += 15 
if (p1kang.y < p1kangtargety){
    p1kang.y += 7;
}
if (p1kang.y > p1kangtargety){
    p1kang.y -= 7;
}
if (p1kang.x > p1kangtargetx - 300) {
if (p1kangbicyclesoundplayed == false){ 
game.sound.add( "kangbicycle" ).play()
p1kangbicyclesoundplayed = true
}    
p1kang.anims.play('kangbicycle', true);
}

if (p1kang.x > p1kangtargetx -5){
playerattacking = true
playerbattleattack()
p1kang.x = p1kangx
p1kang.y = p1kangy
p1kangidle = true
p1kangtargetselected = false
playerabilityselected = false
p1kangselected = false
player1selectabilitybox.visible = false
team2turn = true
team1turn = false
p1kangattackbicycle = false
}
}
}
}
//endp1kangbicycle
//p1kangdragon
if (p1kangattackdragon == true){
if (p1kangtargetselected == true){
p1kangidle = false
p1kangdragontime -= 1;
if (p1kang.x < p1kangtargetx - 6) {
if (p1kang.x < p1kangtargetx - 175 ){
p1kang.anims.play('kangrun', true);
p1kang.x += 15 
}

if (p1kang.y < p1kangtargety - 50 ){
    p1kang.y += 4;
}
if (p1kang.y > p1kangtargety - 50 ){
    p1kang.y -= 4;
}
if (p1kang.x > p1kangtargetx - 176) {
p1kang.anims.play('kangdragon', true);
p1kang.x += 2;
p1kang.y -= 2;
}

if (p1kangdragontime < 2){
    
}
}
}
if (p1kangdragontime < 3){
playerattacking = true
playerbattleattack()
p1kangidle = true
playerabilityselected = false
p1kangselected = false
player1selectabilitybox.visible = false
p1kangtargetselected = false
p1kang.x = p1kangx
p1kang.y = p1kangy
team2turn = true
team1turn = false
p1kangattackdragon = false
}
}
//endp1kangdragon

//p1kangprayer
if (p1kangattackprayer == true){
p1kangprayertime -= 1;
if (p1kang.x < 600){
p1kang.anims.play('kangrun', true);
}
if (p1kang.x < 600 ) {
p1kangidle = false
p1kang.x += 25 
if (p1kang.x > 601) {
p1kangprayerboltstrike.x = p1kang.x
p1kangprayerboltstrike.visible = true
p1kang.anims.play('kangprayer', true);
p1kangprayerboltstrike.anims.play('kangprayerbolt', true);

}
}
if (p1kangprayertime < 1){
p1kangtargetselected = false
playerattacking = true
p1kangprayerboltstrike.visible = false
p1kang.x = p1kangx
p1kang.y = p1kangy
p1kangidle = true
p1kangtargetselected = false
playerabilityselected = false
p1kangselected = false
player1selectabilitybox.visible = false
p1kangattackprayer = false
team2turn = true
team1turn = false

}
}
//endp1kangprayer
}
//endkang1action
//kang2action
//p2kang1action
function p2kangaction(){
//kangidle    
if (p2kangidle == true){
p2kang.anims.play('kangidle', true);
}
//kangidle
//p2kangflykick
if (p2kangattackflykick == true){
if (p2kangtargetselected == true){
p2kangidle = false
if (p2kang.x > p2kangtargetx) {
if (p2kangflykicksoundplayed == false){ 
game.sound.add( "kangflykick" ).play()
p2kangflykicksoundplayed = true
}
p2kang.anims.play('kangflykick', true);
p2kang.x -= 50
if (p2kang.y < p2kangtargety){
    p2kang.y += 7;
}
if (p2kang.y > p2kangtargety){
    p2kang.y -= 7;
}

if (p2kang.x < p2kangtargetx + 25) {
playerattacking = true
playerbattleattack()
p2kangattackflykick = false
p2kangidle = true
p2kang.x = p2kangx
p2kang.y = p2kangy
p2kangtargetselected = false
playerabilityselected = false
p2kangselected = false
player2selectabilitybox.visible = false
team1turn = true
team2turn = false
}
}
}
}
//endkangflykick
//p2kangfireball
if (p2kangattackfireball == true){
if (p2kangtargetselected == true){
p2kangidle = false

if (p2kang.x > p2kangtargetx) {
if (p2kangfireballsoundplayed == false){
p2kang.anims.play('kangfireball', true);
game.sound.add( "kangfireball" ).play()
p2kangfireballsoundplayed = true
}
p2kangfireball.visible = true
p2kangfireball.anims.play('kangfireballgr', true);
p2kangfireball.x -= 45 
if (p2kangfireball.y < p2kangtargety){
    p2kangfireball.y += 7;
}
if (p2kangfireball.y > p2kangtargety){
    p2kangfireball.y -= 7;
}

if (p2kangfireball.x < p2kangtargetx + 25) {
playerattacking = true
playerbattleattack()
p2kangidle = true
p2kangfireballsoundplayed = false
p2kangfireball.visible = false
p2kangfireball.x = p2kang.x
p2kangfireball.y = p2kang.y
p2kangtargetselected = false
playerabilityselected = false
p2kangselected = false
player2selectabilitybox.visible = false
team1turn = true
team2turn = false
p2kangattackfireball = false
}
}
}
}
//endp2kangfireball
//p2kangbicycle
if (p2kangattackbicycle == true){
if (p2kangtargetselected == true){
p2kangidle = false

if (p2kang.x > p2kangtargetx + 6) {
if (p2kang.x > p2kangtargetx +301){
p2kang.anims.play('kangrun', true);
}
p2kang.x -= 15 
if (p2kang.y < p2kangtargety){
    p2kang.y += 7;
}
if (p2kang.y > p2kangtargety){
    p2kang.y -= 7;
}
if (p2kang.x < p2kangtargetx + 300) {
if (p2kangbicyclesoundplayed == false){ 
game.sound.add( "kangbicycle" ).play()
p2kangbicyclesoundplayed = true
}
p2kang.anims.play('kangbicycle', true);
}
if (p2kang.x < p2kangtargetx +20){
playerattacking = true
playerbattleattack()
p2kang.x = p2kangx
p2kang.y = p2kangy
p2kangidle = true
p2kangtargetselected = false
playerabilityselected = false
p2kangselected = false
player2selectabilitybox.visible = false
team1turn = true
team2turn = false
p2kangattackbicycle = false
}
}
}
}
//endp2kangbicycle
//p2kangdragon
if (p2kangattackdragon == true){
if (p2kangtargetselected == true){
p2kangidle = false
p2kangdragontime -= 1;
if (p2kang.x > p2kangtargetx + 6) {
if (p2kang.x > p2kangtargetx ){
p2kang.anims.play('kangrun', true);
}
p2kang.x -= 15 
if (p2kang.y < p2kangtargety){
    p2kang.y += 7;
}
if (p2kang.y > p2kangtargety){
    p2kang.y -= 7;
}
if (p2kang.x < p2kangtargetx + 25) {
p2kang.anims.play('kangdragon', true);
p2kang.x -= 11;
p2kang.y -= 13;
}

if (p2kangdragontime < 1){
    p2kangdragontime = 1
}
}
}
if (p2kangdragontime < 1){
playerattacking = true
playerbattleattack()
p2kangidle = true
playerabilityselected = false
p2kangselected = false
player2selectabilitybox.visible = false
p2kangtargetselected = false
p2kang.x = p2kangx
p2kang.y = p2kangy
team1turn = true
team2turn = false
p2kangattackdragon = false
}
}
//endp2kangdragon

//p2kangprayer
if (p2kangattackprayer == true){
p2kangprayertime -= 1;
if (p2kang.x > 600){
p2kang.anims.play('kangrun', true);
}
if (p2kang.x > 600 ) {
p2kangidle = false
p2kang.x -= 25 
if (p2kang.x < 601 ) {
p2kangprayerboltstrike.x = p2kang.x
p2kangprayerboltstrike.visible = true
p2kang.anims.play('kangprayer', true);
p2kangprayerboltstrike.anims.play('kangprayerbolt', true);

}
}
if (p2kangprayertime < 1){
p2kangtargetselected = false
playerattacking = true
p2kangprayerboltstrike.visible = false
p2kang.x = p2kangx
p2kang.y = p2kangy
p2kangidle = true
p2kangtargetselected = false
playerabilityselected = false
p2kangselected = false
player2selectabilitybox.visible = false
p2kangattackprayer = false
team1turn = true
team2turn = false

}
}
//endp2kangprayer
