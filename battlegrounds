bufftimer = this.time.addEvent({ delay: 100, callback: bufftimer, callbackScope: this, loop: true,startAt: 1 });

function bufftimer(){
     if (p1cecilholyaura == true){
         p1cecilholyparticles.visible = true
        p1cecilholyauratime -= 1
        if (p1cecilholyaurastatup == false){
        p1cecildefenserating += 100;
        p1cecilholyaurastatup = true
    } 
}

    if (p1cecilholyauratime < 1){
        if (p1cecilholyaurastatup == true){
        p1cecildefenserating -= 100;
        p1cecilholyparticles.visible = false
        p1cecilholyaurastatup = false
        p1cecilholyaura = false
        p1cecilholyauratime = 500
    }
}

     if (p2cecilholyaura == true){
         p2cecilholyparticles.visible = true
        p2cecilholyauratime -= 1
        if (p2cecilholyaurastatup == false){
        p2cecildefenserating += 100;
        p2cecilholyaurastatup = true
    } 
}

    if (p2cecilholyauratime < 1){
        if (p2cecilholyaurastatup == true){
        p2cecildefenserating -= 100;
        p2cecilholyparticles.visible = false
        p2cecilholyaurastatup = false
        p2cecilholyaura = false
        p2cecilholyauratime = 500
    }
}
}
