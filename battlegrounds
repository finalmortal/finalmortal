if (p2sindelflybreathgr.x > p2sindeltargetx + 25){
    p2sindelflybreathgr.x -= 10;
}
if (p2sindelflybreathgr.y < p2sindeltargety + 25){
    p2sindelflybreathgr.y += 25;
}
if (p2sindelflybreathgr.y > p2sindeltargety + 24){
    p2sindelflybreathgr.visible = false
}
