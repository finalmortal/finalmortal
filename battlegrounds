p1sektorselected = false
player1selectabilitybox.visible = false
p1sektor.x = p1sektorx
p1sektor.y = p1sektory
team2turn = true
team1turn = false
p1sektortargetselected = false
p1sektorattackhikick = false
}
}
}


}
//endp1sektoraction
//p2sektoraction
function p2sektoraction(){
if (p2sektoridle == true){
p2sektor.anims.play('sektoridle', true);
}
if (p2sektorattackuppercut == true){
if (p2sektortargetselected == true){
p2sektoridle = false
p2sektoruppercuttime -= 0.1;
if (p2sektor.x > p2sektortargetx + 500) {
    if (p2sektoruppercutvoiceplayed == false){
        game.sound.add( "sektoruppercutvoice" ).play()
        p2sektoruppercutvoiceplayed = true
    }

p2sektor.anims.play('sektorrun', true);
p2sektor.x -= 25 
if (p2sektor.y > p2sektortargety){
    p2sektor.y -= 7;
}
if (p2sektor.y < p2sektortargety){
    p2sektor.y += 7;
}
}

if (p2sektoruppercutup == false){
if (p2sektor.x < p2sektortargetx + 501) {
p2sektor.y -= 25
p2sektor.anims.play('sektoruppercut1', true);
}
}

if (p2sektor.y > 700 ) {
p2sektoruppercutup = true
}

if (p2sektoruppercutup == true){
p2sektor.anims.play('sektoruppercut2', true);
p2sektor.x = p2sektortargetx - 25
p2sektor.y += 40
}

if (p2sektoruppercuttime < 1){
playerattacking = true
playerbattleattack()
p2sektoridle = true
playerabilityselected = false
p2sektorselected = false
player2selectabilitybox.visible = false
p2sektor.x = p2sektorx
p2sektor.y = p2sektory
team1turn = true
team2turn = false
p2sektortargetselected = false
p2sektoruppercutup = false
p2sektorattackuppercut = false
}
}
}
if (p2sektorattackrocket == true){
if (p2sektortargetselected == true){
p2sektordidle = false
if (p2sektor.x > p2sektortargetx) {
p2sektorrocket.visible = true
p2sektor.anims.play('sektorrocket', true);
if (p2sektorrocketsoundplayed == false){
        game.sound.add( "sektorrocketsound" ).play()
        p2sektorrocketsoundplayed = true
    }

if (p2sektorrocketvoiceplayed == false){
        game.sound.add( "sektorrocketvoice" ).play()
        p2sektorrocketvoiceplayed = true
    }

p2sektorrocket.anims.play('sektorrocketgr', true);
p2sektorrocket.x -= 45
if (p2sektorrocket.y > p2sektortargety){
    p2sektorrocket.y -= 7;
}
if (p2sektorrocket.y < p2sektortargety){
    p2sektorrocket.y += 7;
}
}
if (p2sektorrocket.x < p2sektortargetx + 45) {
playerattacking = true
playerbattleattack()
p2sektoridle = true
playerabilityselected = false
p2sektorselected = false
player2selectabilitybox.visible = false
p2sektorrocket.visible = false
p2sektorrocket.x = p2sektorx
p2sektorrocket.y = p2sektory
team1turn = true
team2turn = false
p2sektortargetselected = false
p2sektorattackrocket = false
}
}
}

if (p2sektorattackflamethrow == true){
if (p2sektortargetselected == true){
p2sektoridle = false
p2sektorflamethrowtime -= 0.1;
if (p2sektor.x > p2sektortargetx + 91) {
if (p2sektorflamethrowvoiceplayed == false){
        game.sound.add( "sektorflamethrowvoice" ).play()
        p2sektorflamethrowvoiceplayed = true
    }

