//startp1sektoraction
//p1sektoraction
function p1sektoraction(){
if (p1sektoridle == true){
p1sektor.anims.play('sektoridle', true);
}
if (p1sektorattackuppercut == true){
if (p1sektortargetselected == true){
p1sektoridle = false
p1sektoruppercuttime -= 0.1;
if (p1sektor.x < p1sektortargetx - 500) {
p1sektor.anims.play('sektorrun', true);
if (p1sektoruppercutvoiceplayed == false){
        game.sound.add( "sektoruppercutvoice" ).play()
        p1sektoruppercutvoiceplayed = true
    }
p1sektor.x += 25 
if (p1sektor.y > p1sektortargety){
    p1sektor.y -= 7;
}
if (p1sektor.y < p1sektortargety){
    p1sektor.y += 7;
}
}

if (p1sektoruppercutup == false){
if (p1sektor.x > p1sektortargetx - 500) {
p1sektor.y += 25
p1sektor.anims.play('sektoruppercut1', true);
}
}

if (p1sektor.y > 700 ) {
p1sektoruppercutup = true
}

if (p1sektoruppercutup == true){
p1sektor.anims.play('sektoruppercut2', true);
p1sektor.x = p1sektortargetx
p1sektor.y -= 40
}

if (p1sektoruppercuttime < 1){
playerattacking = true
playerbattleattack()
p1sektoridle = true
playerabilityselected = false
p1sektorselected = false
player1selectabilitybox.visible = false
p1sektor.x = p1sektorx
p1sektor.y = p1sektory
team2turn = true
team1turn = false
p1sektortargetselected = false
p1sektoruppercutup = false
p1sektorattackuppercut = false
}
}
}
if (p1sektorattackrocket == true){
if (p1sektortargetselected == true){
p1sektordidle = false
if (p1sektor.x < p1sektortargetx) {
p1sektorrocket.visible = true
p1sektor.anims.play('sektorrocket', true);
p1sektorrocket.anims.play('sektorrocketgr', true);
if (p1sektorrocketsoundplayed == false){
        game.sound.add( "sektorrocketsound" ).play()
        p1sektorrocketsoundplayed = true
    }

if (p1sektorrocketvoiceplayed == false){
        game.sound.add( "sektorrocketvoice" ).play()
        p1sektorrocketvoiceplayed = true
    }
p1sektorrocket.x += 45
if (p1sektorrocket.y > p1sektortargety){
    p1sektorrocket.y -= 7;
}
if (p1sektorrocket.y < p1sektortargety){
    p1sektorrocket.y += 7;
}
}
if (p1sektorrocket.x > p1sektortargetx - 25) {
playerattacking = true
playerbattleattack()
p1sektoridle = true
playerabilityselected = false
p1sektorselected = false
player1selectabilitybox.visible = false
p1sektorrocket.visible = false
p1sektorrocket.x = p1sektorx
p1sektorrocket.y = p1sektory
team2turn = true
team1turn = false
p1sektortargetselected = false
p1sektorattackrocket = false
}
}
}

if (p1sektorattackflamethrow == true){
if (p1sektortargetselected == true){
p1sektoridle = false
p1sektorflamethrowtime -= 0.1;
if (p1sektor.x < p1sektortargetx - 91) {
p1sektor.anims.play('sektorwalk', true);
p1sektor.x += 25
if (p1sektorflamethrowvoiceplayed == false){
        game.sound.add( "sektorflamethrowvoice" ).play()
        p1sektorflamethrowvoiceplayed = true
    }

if (p1sektor.y > p1sektortargety){
    p1sektor.y -= 7;
}
if (p1sektor.y < p1sektortargety){
    p1sektor.y += 7;
}
}
if (p1sektor.x > p1sektortargetx - 89) {
p1sektor.anims.play('sektorflamethrow', true);
p1sektorflamethrow.visible = true
p1sektorflamethrow.x = p1sektor.x + 45
p1sektorflamethrow.y = p1sektor.y + 25
p1sektorflamethrow.anims.play('sektorflamethrowgr', true);
}
if (p1sektorflamethrowtime < 1){
playerattacking = true
playerbattleattack()
p1sektoridle = true
playerabilityselected = false
p1sektorselected = false
player1selectabilitybox.visible = false
p1sektor.x = p1sektorx
p1sektor.y = p1sektory
team2turn = true
team1turn = false
p1sektortargetselected = false
p1sektorflamethrow.visible = false
p1sektorattackflamethrow = false
}
}
}

if (p1sektorattackhammer == true){
if (p1sektortargetselected == true){
p1sektoridle = false
p1sektorhammertime -= 0.1;
if (p1sektor.x < p1sektortargetx - 81) {
p1sektor.anims.play('sektorrun', true);
if (p1sektorhammervoiceplayed == false){
        game.sound.add( "sektorhammervoice" ).play()
        p1sektorhammervoiceplayed = true
    }
p1sektor.x += 25 
if (p1sektor.y > p1sektortargety){
    p1sektor.y -= 7;
}
if (p1sektor.y < p1sektortargety){
    p1sektor.y += 7;
}
}
if (p1sektor.x > p1sektortargetx - 81) {
p1sektor.anims.play('sektorhammer', true);
}
if (p1sektorhammertime < 1){
playerattacking = true
playerbattleattack()
p1sektoridle = true
playerabilityselected = false
p1sektorselected = false
player1selectabilitybox.visible = false
p1sektor.x = p1sektorx
p1sektor.y = p1sektory
team2turn = true
team1turn = false
p1sektortargetselected = false
p1sektorattackhammer = false
}
}
}

if (p1sektorattackhikick == true){
if (p1sektortargetselected == true){
p1sektoridle = false
p1sektorhikicktime -= 0.1;
if (p1sektor.x < p1sektortargetx - 81) {
p1sektor.anims.play('sektorrun', true);
if (p1sektorhikickvoiceplayed == false){
        game.sound.add( "sektorkickvoice" ).play()
        p1sektorhikickvoiceplayed = true
    }
p1sektor.x += 25 
if (p1sektor.y > p1sektortargety){
    p1sektor.y -= 7;
}
if (p1sektor.y < p1sektortargety){
    p1sektor.y += 7;
}
}
if (p1sektor.x > p1sektortargetx - 81) {
