p2sektor.x -= 25 
if (p2sektor.y > p2sektortargety){
    p2sektor.y -= 7;
}
if (p2sektor.y < p2sektortargety){
    p2sektor.y += 7;
}
}
if (p2sektor.x < p2sektortargetx + 90) {
p2sektor.anims.play('sektorflamethrow', true);
p2sektorflamethrow.visible = true
p2sektorflamethrow.x = p2sektor.x - 60
p2sektorflamethrow.y = p2sektor.y + 25
p2sektorflamethrow.anims.play('sektorflamethrowgr', true);
}
