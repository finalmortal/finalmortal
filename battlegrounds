    if (Phaser.Input.Keyboard.JustDown(twokey))
    {
    p1sindelscreamkey()
    }
    if (Phaser.Input.Keyboard.JustDown(threekey))
    {
    p1sindelflybreathkey()
    }
   if (Phaser.Input.Keyboard.JustDown(fourkey))
    {
    p1sindelhairwhipkey()
    }
     if (Phaser.Input.Keyboard.JustDown(fivekey))
    {
    p1sindelpuntupkey()
    }
    }

if (p1raidenselected == true){
    if (Phaser.Input.Keyboard.JustDown(onekey))
    {
    p1raidenthunderboltkey()
    }
    if (Phaser.Input.Keyboard.JustDown(twokey))
    {
    p1raidenflykey()
    }
    if (Phaser.Input.Keyboard.JustDown(threekey))
    {
    p1raidenstormkey()
    }
   if (Phaser.Input.Keyboard.JustDown(fourkey))
    {
    p1raidenpowerstrikekey()
    }
     if (Phaser.Input.Keyboard.JustDown(fivekey))
    {
    p1raidenkickkey()
    }
    }

if (p1cyanselected == true){
    if (Phaser.Input.Keyboard.JustDown(onekey))
    {
    p1cyanswordslashkey()
    }
    if (Phaser.Input.Keyboard.JustDown(twokey))
    {
    p1cyanswordswingkey()
    }
    if (Phaser.Input.Keyboard.JustDown(threekey))
    {
    p1cyanmagicswordkey()
    }
   if (Phaser.Input.Keyboard.JustDown(fourkey))
    {
    p1cyancommandkey()
    }
     if (Phaser.Input.Keyboard.JustDown(fivekey))
    {
    p1cyanbattleplankey()
    }
    }

if (p1rosaselected == true){
    if (Phaser.Input.Keyboard.JustDown(onekey))
    {
    p1rosahealkey()
    }
    if (Phaser.Input.Keyboard.JustDown(twokey))
    {
    p1rosacurekey()
    }
    if (Phaser.Input.Keyboard.JustDown(threekey))
    {
    p1rosafastkey()
    }
   if (Phaser.Input.Keyboard.JustDown(fourkey))
    {
    p1rosashellkey()
    }
     if (Phaser.Input.Keyboard.JustDown(fivekey))
    {
    p1rosabowkey()
    }
    }

if (p1mogselected == true){
    
    if (Phaser.Input.Keyboard.JustDown(onekey))
    {
    p1mogmoonkey()
    }
    if (Phaser.Input.Keyboard.JustDown(twokey))
    {
    p1mogpowerkey()
    }
    if (Phaser.Input.Keyboard.JustDown(threekey))
    {
    p1mogdreamkey()
    }
   if (Phaser.Input.Keyboard.JustDown(fourkey))
    {
    p1mogdancekey()
    }
     if (Phaser.Input.Keyboard.JustDown(fivekey))
    {
    p1mogbullkey()
    }
    }
    

//
}
