       p2rosatargetselected = false
       p2rosasupportheal = false
    }
    }
    }
 if (p2rosasupportfast == true){
        if (p2rosatargetselected == true){
        p2rosaidle = false
        p2rosafasttime -= 0.2;
        p2rosa.anims.play('rosacasting', true);
        p2rosafastspell.visible = true
        p2rosafastspell.anims.play('rosafastspell', true);
        if (p2rosafastsoundplayed == false){
        game.sound.add( "rosafastsound" ).play()
        p2rosafastsoundplayed = true
    }
        p2rosafastspell.x = p2rosatargetx - 20
        p2rosafastspell.y = p2rosatargety - 10

    if (p2rosafasttime < 1){
       playerattacking = true
       playerbattleattack()
       p2rosaidle = true
       playerabilityselected = false
       p2rosaselected = false
       player2selectabilitybox.visible = false
       p2rosafastspell.visible = false
       p2rosa.x = p2rosax
       p2rosa.y = p2rosay
       team1turn = true
       team2turn = false
       p2rosafasttarget = false
       p2rosatargetselected = false
       p2rosasupportfast = false
    }
    }
    }

     if (p2rosasupportshell == true){
        if (p2rosatargetselected == true){
        p2rosaidle = false
        p2rosashelltime -= 0.2;
        p2rosa.anims.play('rosacasting', true);
        p2rosashellspell.visible = true
        p2rosashellspell.anims.play('rosashellspell', true);
        if (p2rosashellsoundplayed == false){
        game.sound.add( "rosashellsound" ).play()
        p2rosashellsoundplayed = true
    }
        p2rosashellspell.x = p2rosatargetx - 20
        p2rosashellspell.y = p2rosatargety - 10

    if (p2rosashelltime < 1){
       playerattacking = true
       playerbattleattack()
       p2rosaidle = true
       playerabilityselected = false
       p2rosaselected = false
       player2selectabilitybox.visible = false
       p2rosashellspell.visible = false
       p2rosa.x = p2rosax
       p2rosa.y = p2rosay
       team1turn = true
       team2turn = false
       p2rosashelltarget = false
       p2rosatargetselected = false
       p2rosasupportshell = false
    }
    }
    }
}
//endrosap2

//p1mogaction
//mogaction
function p1mogaction(){
    if (p1mogidle == true){
        p1mog.anims.play('mogidle', true);
    }
        if (p1mogsupportmoon == true){
        p1mogidle = false
        p1mogmoontime -= 0.2;
        p1mog.anims.play('mogcasting', true);
        p1mogmoonspell.visible = true
        p1mogmoonspell.anims.play('mogmoonspell', true);
        p1mogmoonspell.x = p1mog.x - 20
        p1mogmoonspell.y = p1mog.y - 250

    if (p1mogmoontime < 1){
       playerattacking = true
       playerbattleattack()
       p1mogidle = true
       playerabilityselected = false
       p1mogselected = false
       player1selectabilitybox.visible = false
       p1mogmoonspell.visible = false
       p1mog.x = p1mogx
       p1mog.y = p1mogy
       p1mogmoonaura = true
       p1mogspells()
       team2turn = true
       team1turn = false
       p1moghealingtarget = false
       p1mogtargetselected = false
       team2noplayerselected = true
       p1mogsupportmoon = false
    }
    }
    

 if (p1mogsupportpower == true){
        p1mogidle = false
        p1mogpowertime -= 0.2;
        p1mog.anims.play('mogcasting', true);
        p1mogpowerspell.visible = true
        p1mogpowerspell.anims.play('mogpowerspell', true);
        if (p1mogpowersoundplayed == false){
        game.sound.add( "mogpowersound" ).play()
        p1mogpowersoundplayed = true
    }
        p1mogpowerspell.x = p1mog.x - 20
        p1mogpowerspell.y = p1mog.y - 10

    if (p1mogpowertime < 1){
       playerattacking = true
       playerbattleattack()
       p1mogidle = true
       playerabilityselected = false
       p1mogselected = false
       player1selectabilitybox.visible = false
       p1mogpowerspell.visible = false
       p1mog.x = p1mogx
       p1mog.y = p1mogy
       p1mogpoweraura = true
       p1mogspells()
       team2turn = true
       team1turn = false
       p1moghealingtarget = false
       p1mogtargetselected = false
       team2noplayerselected = true
       p1mogsupportpower = false
    }
    }
    
    
 if (p1mogsupportdance == true){
       
        p1mogidle = false
        p1mogdancetime -= 0.2;
        p1mog.anims.play('mogcasting', true);
        p1mogdancespell.visible = true
        p1mogdancespell.anims.play('mogdancespell', true);
        if (p1mogdancesoundplayed == false){
        game.sound.add( "mogdancesound" ).play()
        p1mogdancesoundplayed = true
    }
        p1mogdancespell.x = p1mog.x - 20
        p1mogdancespell.y = p1mog.y - 10

    if (p1mogdancetime < 1){
       playerattacking = true
       playerbattleattack()
       p1mogidle = true
       playerabilityselected = false
       p1mogselected = false
       player1selectabilitybox.visible = false
       p1mogdancespell.visible = false
       p1mog.x = p1mogx
       p1mog.y = p1mogy
       p1mogdanceaura = true
       p1mogspells()
       team2turn = true
       team1turn = false
       p1mogdancetarget = false
       p1mogtargetselected = false
       team2noplayerselected = true
       p1mogsupportdance = false
    }
   
    }

     if (p1mogsupportdream == true){
        p1mogidle = false
        p1mogdreamtime -= 0.2;
        p1mog.anims.play('mogcasting', true);
        p1mogdreamspell.visible = true
        p1mogdreamspell.anims.play('mogdreamspell', true);
        p1mogdreamspell.x = p1mog.x - 20
        p1mogdreamspell.y = p1mog.y - 10

    if (p1mogdreamtime < 1){
       playerattacking = true
       playerbattleattack()
       p1mogidle = true
       playerabilityselected = false
       p1mogselected = false
       player1selectabilitybox.visible = false
       p1mogdreamspell.visible = false
       p1mog.x = p1mogx
       p1mog.y = p1mogy
       p1mogdreamaura = true
       p1mogspells()
       team2turn = true
       team1turn = false
       p1mogdreamtarget = false
       p1mogtargetselected = false
       team2noplayerselected = true
       p1mogsupportdream = false
    }
    
    }

     if (p1mogsupportbull == true){
       
        p1mogidle = false
        p1mogbulltime -= 0.2;
        p1mog.anims.play('mogcasting', true);
        p1mogbullspell.visible = true
        p1mogbullspell.anims.play('mogbullspell', true);
        p1mogbullspell.x = p1mog.x - 20
        p1mogbullspell.y = p1mog.y - 10

    if (p1mogbulltime < 1){
       playerattacking = true
       playerbattleattack()
       p1mogidle = true
       playerabilityselected = false
       p1mogselected = false
       player1selectabilitybox.visible = false
       p1mogbullspell.visible = false
       p1mog.x = p1mogx
       p1mog.y = p1mogy
       p1mogbullaura = true
       p1mogspells()
       team2turn = true
       team1turn = false
       p1mogbulltarget = false
       p1mogtargetselected = false
       team2noplayerselected = true
       p1mogsupportbull = false
    }
    
    }

}
//endmogp1


//p2mogaction
//mogaction
function p2mogaction(){
    if (p2mogidle == true){
        p2mog.anims.play('mogidle', true);
    }
        if (p2mogsupportmoon == true){
           
        p2mogidle = false
        p2mogmoontime -= 0.2;
        p2mog.anims.play('mogcasting', true);
        p2mogmoonspell.visible = true
        p2mogmoonspell.anims.play('mogmoonspell', true);
        p2mogmoonspell.x = p2mog.x - 200
        p2mogmoonspell.y = p2mog.y - 250

    if (p2mogmoontime < 1){
       playerattacking = true
       playerbattleattack()
       p2mogidle = true
       playerabilityselected = false
       p2mogselected = false
       player2selectabilitybox.visible = false
       p2mogmoonspell.visible = false
       p2mog.x = p2mogx
       p2mog.y = p2mogy
       p2mogmoonaura = true
       p2mogspells()
       team1turn = true
       team2turn = false
       p2moghealingtarget = false
       p2mogtargetselected = false
       team2noplayerselected = true
       p2mogsupportmoon = false
    }
    }
    

 if (p2mogsupportpower == true){
        p2mogidle = false
        p2mogpowertime -= 0.2;
        p2mog.anims.play('mogcasting', true);
        p2mogpowerspell.visible = true
        p2mogpowerspell.anims.play('mogpowerspell', true);
        if (p2mogpowersoundplayed == false){
        game.sound.add( "mogpowersound" ).play()
        p2mogpowersoundplayed = true
    }


        p2mogpowerspell.x = p2mog.x - 100
        p2mogpowerspell.y = p2mog.y - 10

    if (p2mogpowertime < 1){
       playerattacking = true
       playerbattleattack()
       p2mogidle = true
       playerabilityselected = false
       p2mogselected = false
       player2selectabilitybox.visible = false
       p2mogpowerspell.visible = false
       p2mog.x = p2mogx
       p2mog.y = p2mogy
       p2mogpoweraura = true
       p2mogspells()
       team1turn = true
       team2turn = false
       p2moghealingtarget = false
       p2mogtargetselected = false
       team2noplayerselected = true
       p2mogsupportpower = false
    }
    }


 if (p2mogsupportdance == true){
       
        p2mogidle = false
        p2mogdancetime -= 0.2;
        p2mog.anims.play('mogcasting', true);
        p2mogdancespell.visible = true
        p2mogdancespell.anims.play('mogdancespell', true);
        if (p2mogdancesoundplayed == false){
        game.sound.add( "mogdancesound" ).play()
        p2mogdancesoundplayed = true
    }
        p2mogdancespell.x = p2mog.x - 20
        p2mogdancespell.y = p2mog.y - 10

    if (p2mogdancetime < 1){
       playerattacking = true
       playerbattleattack()
       p2mogidle = true
       playerabilityselected = false
       p2mogselected = false
       player2selectabilitybox.visible = false
       p2mogdancespell.visible = false
       p2mog.x = p2mogx
       p2mog.y = p2mogy
       p2mogdanceaura = true
       p2mogspells()
       team1turn = true
       team2turn = false
       p2mogdancetarget = false
       p2mogtargetselected = false
       team2noplayerselected = true
       p2mogsupportdance = false
    }
   
    }

     if (p2mogsupportdream == true){
        p2mogidle = false
        p2mogdreamtime -= 0.2;
        p2mog.anims.play('mogcasting', true);
        p2mogdreamspell.visible = true
        p2mogdreamspell.anims.play('mogdreamspell', true);
        p2mogdreamspell.x = p2mog.x - 20
        p2mogdreamspell.y = p2mog.y - 10

    if (p2mogdreamtime < 1){
       playerattacking = true
       playerbattleattack()
       p2mogidle = true
       playerabilityselected = false
       p2mogselected = false
       player2selectabilitybox.visible = false
       p2mogdreamspell.visible = false
       p2mog.x = p2mogx
       p2mog.y = p2mogy
       p2mogdreamaura = true
       p2mogspells()
       team1turn = true
       team2turn = false
       p2mogdreamtarget = false
       p2mogtargetselected = false
       team2noplayerselected = true
       p2mogsupportdream = false
    }
    
    }

     if (p2mogsupportbull == true){
       
        p2mogidle = false
        p2mogbulltime -= 0.2;
        p2mog.anims.play('mogcasting', true);
        p2mogbullspell.visible = true
        p2mogbullspell.anims.play('mogbullspell', true);
        p2mogbullspell.x = p2mog.x - 20
        p2mogbullspell.y = p2mog.y - 10

    if (p2mogbulltime < 1){
