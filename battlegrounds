//p1golbezaction
function p1golbezaction(){
if (p1golbezidle == true){
p1golbez.anims.play('golbezidle', true);
}
if (p1golbezattackwand == true){
if (p1golbeztargetselected == true){
p1golbezdidle = false
if (p1golbez.x < p1golbeztargetx) {
p1golbezwand.visible = true
p1golbez.anims.play('golbezcasting', true);
p1golbezwand.anims.play('golbezwandgr', true);
if (p1golbezwandsoundplayed == false){
        game.sound.add( "golbezwandsound" ).play()
        p1golbezwandsoundplayed = true
}
p1golbezwand.x += 45
if (p1golbezwand.y > p1golbeztargety){
    p1golbezwand.y -= 7;
}
if (p1golbezwand.y < p1golbeztargety){
    p1golbezwand.y += 7;
}
}
if (p1golbezwand.x > p1golbeztargetx - 25) {
playerattacking = true
playerbattleattack()
p1golbezidle = true
playerabilityselected = false
p1golbezselected = false
player1selectabilitybox.visible = false
p1golbezwand.visible = false
p1golbezwand.x = p1golbezx
p1golbezwand.y = p1golbezy
team2turn = true
team1turn = false
p1golbeztargetselected = false
p1golbezattackwand = false
}
}
}

if (p1golbezattackshadowbolt == true){
if (p1golbeztargetselected == true){
p1golbezdidle = false
if (p1golbez.x < p1golbeztargetx) {
p1golbezshadowbolt.visible = true
p1golbez.anims.play('golbezcasting', true);
p1golbezshadowbolt.anims.play('golbezshadowbolt', true);
if (p1golbezshadowboltsoundplayed == false){
        game.sound.add( "golbezshadowboltsound" ).play()
        p1golbezshadowboltsoundplayed = true
}
p1golbezshadowbolt.x += 45
if (p1golbezshadowbolt.y > p1golbeztargety){
    p1golbezshadowbolt.y -= 7;
}
if (p1golbezshadowbolt.y < p1golbeztargety){
    p1golbezshadowbolt.y += 7;
}
}
if (p1golbezshadowbolt.x > p1golbeztargetx - 25) {
playerattacking = true
playerbattleattack()
p1golbezidle = true
playerabilityselected = false
p1golbezselected = false
player1selectabilitybox.visible = false
p1golbezshadowbolt.visible = false
p1golbezshadowbolt.x = p1golbezx
p1golbezshadowbolt.y = p1golbezy
team2turn = true
team1turn = false
p1golbeztargetselected = false
p1golbezattackshadowbolt = false
}
}
}

if (p1golbezattackdarkflame == true){
if (p1golbeztargetselected == true){
p1golbezdidle = false
p1golbezdarkflametime -= 0.3;
if (p1golbez.x < p1golbeztargetx) {
p1golbezdarkflame.visible = true
p1golbez.anims.play('golbezcasting', true);
p1golbezdarkflame.anims.play('golbezdarkflame', true);
if (p1golbezdarkflamesoundplayed == false){
        game.sound.add( "golbezdarkflamesound" ).play()
        p1golbezdarkflamesoundplayed = true
}
p1golbezdarkflame.x = p1golbeztargetx - 75
p1golbezdarkflame.y = p1golbeztargety - 100
}
