//endnotenoughaction
function deselect(){
p1cecilselected = false 
p1kainselected = false
p1kangselected = false
p1orchidselected = false
p1spinalselected = false
p1terraselected = false
p1kanoselected = false
p1jagoselected = false
p1comboselected = false
p1sonjaselected = false
p1kunglaoselected = false
p1glaciusselected = false
p1golbezselected = false
p1sektorselected = false
p1cyraxselected = false
p1sindelselected = false
p1raidenselected = false
p1cyanselected = false
p1rosaselected = false
p1mogselected = false
p2cecilselected = false 
p2kainselected = false
p2kangselected = false
p2orchidselected = false
p2spinalselected = false
p2terraselected = false
p2kanoselected = false
p2jagoselected = false
p2comboselected = false
p2sonjaselected = false
p2kunglaoselected = false
p2glaciusselected = false
p2golbezselected = false
p2sektorselected = false
p2cyraxselected = false
p2sindelselected = false
p2raidenselected = false
p2cyanselected = false
p2rosaselected = false
p2mogselected = false
}
