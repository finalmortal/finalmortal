    currentplayerattackrating = p1cyanattackrating
    currentplayerdefenserating = p1cyandefenserating
    currentplayermagicrating = p1cyanmagicrating
    currentplayerspeedrating = p1cyanspeedrating
    currentplayersupportrating = p1cyansupportrating


    if (p1cyanattackswordslash == true){
        currenttargethealthrating -=   p1cyanattackswordslashdamage * currentplayerchargepower - currenttargetdefenserating
        p1cyanactionpoints -= 24
    }
    if (p1cyanattackswordswing == true){
        currenttargethealthrating -=   p1cyanattackswordswingdamage * currentplayerchargepower - currenttargetdefenserating 
        p1cyanactionpoints -= 49
    }
    if (p1cyanattackmagicsword == true){
        currenttargethealthrating -=   p1cyanattackmagicsworddamage * currentplayerchargepower - currenttargetdefenserating 
        p1cyanactionpoints -= 49
    }
    if (p1cyanattackcommand == true){
         p1p1chargerating += 1;
         p1p2chargerating += 1;
         p1p3chargerating += 1;
         p1p4chargerating += 1;
         p1p5chargerating += 1;
         p1cyanactionpoints -= 74
         
    }
    
    if (p1cyanattackbattleplan == true){
         p1p1actionpoints += 10
         p1p2actionpoints += 10
         p1p3actionpoints += 10
         p1p4actionpoints += 10
         p1p5actionpoints += 10
         p2p1actionpoints -= 10
         p2p2actionpoints -= 10
         p2p3actionpoints -= 10
         p2p4actionpoints -= 10
         p2p5actionpoints -= 10
        p1cyanactionpoints -= 74
    }
       
    currentattacktarget()
    
}
