     p2raidenhealthrating = currenttargethealthrating
    }
    if (currentattacktargetname == 'p2cyan'){
     p2cyanhealthrating = currenttargethealthrating
    }
    if (currentattacktargetname == 'p2rosa'){
     p2rosahealthrating = currenttargethealthrating
    }
    if (currentattacktargetname == 'p2mog'){
     p2moghealthrating = currenttargethealthrating
    }

}

function gamewin(){
//teamhealth    
team1health = player1p1health + player1p2health + player1p3health + player1p4health + player1p5health
team2health = player2p1health + player2p2health + player2p3health + player2p4health + player2p5health

    if (team1health < 1){
        team2win = true
         if (aiteam == false){
            p2winscreen.visible = true
        }
        if (aiteam == true){
            youlosescreen.visible = true
        }
        showstatsbutton.visible = true
        mainmenubutton.visible = true
    }
    if (team2health < 1){
        team1win = true
        if (aiteam == false){
            p1winscreen.visible = true
        }
        if (aiteam == true){
            youwinscreen.visible = true
        }
        showstatsbutton.visible = true
        mainmenubutton.visible = true
    }

    
    //dead
    if (p1cecilhealthrating < 1){
    p1cecildead = true
    p1cecilidle = false
    p1cecil.anims.play('cecildead', true);
    }
    if (p1kainhealthrating < 1){
    p1kaindead = true
    p1kainidle = false
    p1kain.anims.play('kaindead', true);
    }
    if (p1kanghealthrating < 1){
    p1kangdead = true
    p1kainidle = false
    p1kang.anims.play('kangdead', true);
    }
    if (p1orchidhealthrating < 1){
