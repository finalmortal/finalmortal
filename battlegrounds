p1p1focusbox = this.physics.add.sprite(battlegroundboxt1p1.x + 15, battlegroundboxt1p1.y +90, 'focusbox').setInteractive();
p1p1focusbox.setOrigin(0, 0);
p1p1focusbox.on('pointerdown', function (pointer) {
p1p1chargebuttondown = true
p1p2chargebuttondown = false
p1p3chargebuttondown = false
p1p4chargebuttondown = false
p1p5chargebuttondown = false  
player1focus = true 

});
