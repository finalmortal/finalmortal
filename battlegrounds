//p1mogspells
function p1mogspells(){
//p1mogmoon
if (p1mogmoonaura == true){
if (p1mogmoonauratime < 1){
p1mogmoonstatdown = true
}
}
if (p1mogmoonaura == true){
if (p1mogmoonstatup == false){
p1rosamaxhealthrating += p1mogmoonstat
p1rosahealthrating += p1mogmoonstat
p1mogmaxhealthrating += p1mogmoonstat
p1moghealthrating += p1mogmoonstat
p1sindelmaxhealthrating += p1mogmoonstat
p1sindelhealthrating += p1mogmoonstat
p1sonjamaxhealthrating += p1mogmoonstat
p1sonjahealthrating += p1mogmoonstat
p1raidenmaxhealthrating += p1mogmoonstat
p1raidenhealthrating += p1mogmoonstat    
p1sektormaxhealthrating += p1mogmoonstat
p1sektorhealthrating += p1mogmoonstat
p1glaciusmaxhealthrating += p1mogmoonstat
p1glaciushealthrating += p1mogmoonstat
p1kunglaomaxhealthrating += p1mogmoonstat
p1kunglaohealthrating += p1mogmoonstat
p1combomaxhealthrating += p1mogmoonstat
p1combohealthrating += p1mogmoonstat
p1kanomaxhealthrating += p1mogmoonstat
