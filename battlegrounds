//removedebuff*******************************
function removedebuffstatus(){
if (targethasdebuff == true){
if (targetdebuffnumber > 1){
targetdebuffnumber -= 1
}
else{
targetdebuffnumber -= 1
targethasdebuff = false
}
}
}
//endremovedebuff******************************
//playerability******************************
function playerabilitytime(){
if (p1rosafastaura == true){
    p1rosafastauratime -= 10
}
if (p1rosashellaura == true){
    p1rosashellauratime -= 10
}
if (p1mogmoonaura == true){
    p1mogmoonauratime -= 10
}
if (p1mogpoweraura == true){
    p1mogpowerauratime -= 10
}
if (p1mogdanceaura == true){
    p1mogdanceauratime -= 10
}
if (p1mogdreamaura == true){
    p1mogdreamauratime -= 10
}
p1rosaspells()
p1mogspells()
//

if (p2rosafastaura == true){
    p2rosafastauratime -= 10
}
if (p2rosashellaura == true){
    p2rosashellauratime -= 10
}
if (p2mogmoonaura == true){
    p2mogmoonauratime -= 10
}
if (p2mogpoweraura == true){
    p2mogpowerauratime -= 10
}
if (p2mogdanceaura == true){
    p2mogdanceauratime -= 10
}
if (p2mogdreamaura == true){
    p2mogdreamauratime -= 10
}
p2rosaspells()
p2mogspells()
//
}
//endplayerability*****************************


//rounds
function rounds(){
roundnumber += 10;

 if (gameround == 2){
    if (round2voiceplayed == false){
    round2voiceplayed = true
    }
}


if (roundnumber > 99){
    gameround += 1
    nextround()
    roundstart = true
    gameroundnumber = 9
    roundnumber = 0

}


playercharges()
playerabilitytime()

}


//endplayerability**********************************
//beginplayercharges
function playercharges(){
if (p1p1chargebuttondown == true){
    p1p1chargerating += 0.5
    
}
if (p1p2chargebuttondown == true){
    p1p2chargerating += 0.5
}
if (p1p3chargebuttondown == true){
    p1p3chargerating += 0.5
}
if (p1p4chargebuttondown == true){
    p1p4chargerating += 0.5
}
if (p1p5chargebuttondown == true){
    p1p5chargerating += 0.5
}

if (p2p1chargebuttondown == true){
    p2p1chargerating += 0.5
}
if (p2p2chargebuttondown == true){
    p2p2chargerating += 0.5
}
if (p2p3chargebuttondown == true){
    p2p3chargerating += 0.5
}
if (p2p4chargebuttondown == true){
    p2p4chargerating += 0.5
}
if (p2p5chargebuttondown == true){
    p2p5chargerating += 0.5
}
}

